{
  final Identifier catalogIdentifier=identifierHelper().fromMetaDataCatalogName(resultSet.getString("TABLE_CAT"));
  final Identifier schemaIdentifier=identifierHelper().fromMetaDataSchemaName(resultSet.getString("TABLE_SCHEM"));
  final Identifier tableIdentifier=identifierHelper().fromMetaDataObjectName(resultSet.getString("TABLE_NAME"));
  final QualifiedTableName tableName=new QualifiedTableName(new Schema.Name(catalogIdentifier,schemaIdentifier),tableIdentifier);
  return new TableInformationImpl(this,tableName,isPhysicalTableType(resultSet.getString("TABLE_TYPE")),resultSet.getString("REMARKS"));
}
