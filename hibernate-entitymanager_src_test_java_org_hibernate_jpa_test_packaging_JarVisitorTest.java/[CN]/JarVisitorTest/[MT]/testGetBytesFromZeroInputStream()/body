{
  try {
    InputStream emptyStream=new BufferedInputStream(new FileInputStream(new File("src/test/resources/org/hibernate/jpa/test/packaging/empty.txt")));
    int length=JarVisitorFactory.getBytesFromInputStream(emptyStream).length;
    assertEquals(length,0);
    emptyStream.close();
  }
 catch (  Exception e) {
    fail(e.getMessage());
  }
}
