{
  if (isDelayed) {
    getSession().getPersistenceContext().replaceDelayedEntityIdentityInsertKeys(delayedEntityKey,generatedId);
  }
  PostInsertEventListener[] postListeners=getSession().getListeners().getPostInsertEventListeners();
  if (postListeners.length > 0) {
    PostInsertEvent postEvent=new PostInsertEvent(getInstance(),generatedId,state,getPersister(),(EventSource)getSession());
    for (int i=0; i < postListeners.length; i++) {
      postListeners[i].onPostInsert(postEvent);
    }
  }
}
