{
  Constructor ctor=null;
  try {
    ctor=clazz.getDeclaredConstructor(COMPONENT_TUP_CTOR_SIG);
    if (!ReflectHelper.isPublic(ctor)) {
      try {
        ctor.setAccessible(true);
      }
 catch (      SecurityException e) {
        ctor=null;
      }
    }
  }
 catch (  NoSuchMethodException ignore) {
  }
  return ctor;
}
