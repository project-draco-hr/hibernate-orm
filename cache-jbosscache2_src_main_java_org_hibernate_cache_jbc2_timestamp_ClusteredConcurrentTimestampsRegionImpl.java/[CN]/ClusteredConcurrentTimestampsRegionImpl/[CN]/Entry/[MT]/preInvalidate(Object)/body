{
  boolean result=false;
  if (newVal instanceof Comparable) {
    if (current == null || ((Comparable)newVal).compareTo(current) > 0) {
      preInval=current=newVal;
      preInvalidated=true;
      result=true;
    }
  }
 else {
    preInval=current=newVal;
    result=true;
  }
  return result;
}
