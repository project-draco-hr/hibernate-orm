{
  super.tearDown();
  if (localCollectionRegion != null)   localCollectionRegion.destroy();
  if (remoteCollectionRegion != null)   remoteCollectionRegion.destroy();
  try {
    localCache.getAdvancedCache().clear(Flag.CACHE_MODE_LOCAL);
  }
 catch (  Exception e) {
    log.error("Problem purging local cache",e);
  }
  try {
    remoteCache.getAdvancedCache().clear(Flag.CACHE_MODE_LOCAL);
  }
 catch (  Exception e) {
    log.error("Problem purging remote cache",e);
  }
  node1Exception=null;
  node2Exception=null;
  node1Failure=null;
  node2Failure=null;
}
