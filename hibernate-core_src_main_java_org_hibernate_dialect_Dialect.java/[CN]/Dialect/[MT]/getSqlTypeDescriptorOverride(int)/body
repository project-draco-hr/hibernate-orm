{
  SqlTypeDescriptor descriptor;
switch (sqlCode) {
case Types.BLOB:
{
      descriptor=useInputStreamToInsertBlob() ? BlobTypeDescriptor.STREAM_BINDING : null;
      break;
    }
case Types.CLOB:
{
    descriptor=useInputStreamToInsertBlob() ? ClobTypeDescriptor.STREAM_BINDING : null;
    break;
  }
default :
{
  descriptor=null;
  break;
}
}
return descriptor;
}
