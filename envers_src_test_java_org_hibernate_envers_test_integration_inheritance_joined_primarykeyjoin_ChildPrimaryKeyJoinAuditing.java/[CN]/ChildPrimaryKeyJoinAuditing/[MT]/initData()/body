{
  EntityManager em=getEntityManager();
  id1=1;
  em.getTransaction().begin();
  ChildPrimaryKeyJoinEntity ce=new ChildPrimaryKeyJoinEntity(id1,"x",1l);
  em.persist(ce);
  em.getTransaction().commit();
  em.getTransaction().begin();
  ce=em.find(ChildPrimaryKeyJoinEntity.class,id1);
  ce.setData("y");
  ce.setNumber(2l);
  em.getTransaction().commit();
}
