{
  this.rootReturnReader=rootReturnReader;
  this.entityReferenceInitializers=entityReferenceInitializers != null ? entityReferenceInitializers : Collections.<EntityReferenceInitializer>emptyList();
  this.arrayReferenceInitializers=arrayReferenceInitializers != null ? arrayReferenceInitializers : Collections.<CollectionReferenceInitializer>emptyList();
  this.collectionReferenceInitializers=collectionReferenceInitializers != null ? collectionReferenceInitializers : Collections.<CollectionReferenceInitializer>emptyList();
}
