{
  if (index < 0) {
    throw new ArrayIndexOutOfBoundsException("negative index");
  }
  Object old=isPutQueueEnabled() ? readElementByIndex(new Integer(index)) : UNKNOWN;
  if (old == UNKNOWN) {
    write();
    return list.remove(index);
  }
 else {
    queueOperation(new Remove(index,old));
    return old;
  }
}
