{
  final String propertyPath=compositionDefinition.getName();
  final CompositeQuerySpaceImpl rhs=new CompositeQuerySpaceImpl(compositeSubPropertyMapping,querySpaceUid,getQuerySpaces(),sessionFactory());
  getQuerySpaces().registerQuerySpace(rhs);
  final JoinImpl join=new JoinImpl(this,propertyPath,rhs,null,compositionDefinition.isNullable());
  internalGetJoins().add(join);
  return join;
}
