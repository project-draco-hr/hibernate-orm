{
  final String propertyPath=compositionDefinition.getName();
  final boolean required=canJoinsBeRequired() && !compositionDefinition.isNullable();
  final CompositeQuerySpaceImpl rhs=new CompositeQuerySpaceImpl(compositeSubPropertyMapping,querySpaceUid,getQuerySpaces(),required,sessionFactory());
  getQuerySpaces().registerQuerySpace(rhs);
  final JoinImpl join=new JoinImpl(this,propertyPath,rhs,null,null,required);
  internalGetJoins().add(join);
  return join;
}
