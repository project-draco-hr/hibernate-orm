{
  if (rowNumber >= 0)   rowNumber++;
  try {
    boolean result=getResultSet().absolute(rowNumber);
    prepareCurrentRow(result);
    return result;
  }
 catch (  SQLException sqle) {
    throw JDBCExceptionHelper.convert(getSession().getFactory().getSQLExceptionConverter(),sqle,"could not advance using absolute()");
  }
}
