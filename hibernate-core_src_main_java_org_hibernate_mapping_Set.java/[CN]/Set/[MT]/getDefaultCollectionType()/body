{
  if (isSorted()) {
    return getMappings().getTypeResolver().getTypeFactory().sortedSet(getRole(),getReferencedPropertyName(),getComparator());
  }
 else   if (hasOrder()) {
    return getMappings().getTypeResolver().getTypeFactory().orderedSet(getRole(),getReferencedPropertyName());
  }
 else {
    return getMappings().getTypeResolver().getTypeFactory().set(getRole(),getReferencedPropertyName());
  }
}
