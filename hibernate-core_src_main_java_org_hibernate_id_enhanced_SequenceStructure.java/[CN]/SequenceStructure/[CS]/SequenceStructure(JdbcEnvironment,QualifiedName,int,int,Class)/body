{
  this.qualifiedSequenceName=qualifiedSequenceName;
  this.sequenceName=jdbcEnvironment.getQualifiedObjectNameFormatter().format(qualifiedSequenceName,jdbcEnvironment.getDialect());
  this.initialValue=initialValue;
  this.incrementSize=incrementSize;
  this.numberType=numberType;
  sql=jdbcEnvironment.getDialect().getSequenceNextValString(sequenceName);
}
