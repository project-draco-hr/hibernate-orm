{
  this.buildingOptions=buildingOptions;
  this.jdbcEnvironment=jdbcEnvironment;
  this.dialect=determineDialect(buildingOptions);
  this.implicitSchema=makeSchema(new Schema.Name(toIdentifier(buildingOptions.getMappingDefaults().getImplicitCatalogName()),toIdentifier(buildingOptions.getMappingDefaults().getImplicitSchemaName())));
}
