{
  if (!this.monotone) {
    return NON_MONOTONE;
  }
  MCoordinate c1=(MCoordinate)this.getCoordinateN(0);
  MCoordinate c2=(MCoordinate)this.getCoordinateN(this.getNumPoints() - 1);
  if (c1.m < c2.m) {
    return INCREASING;
  }
 else   if (c1.m > c2.m) {
    return DECREASING;
  }
 else {
    return CONSTANT;
  }
}
