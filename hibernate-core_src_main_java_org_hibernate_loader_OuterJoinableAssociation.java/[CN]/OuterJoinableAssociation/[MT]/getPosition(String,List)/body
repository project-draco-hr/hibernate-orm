{
  int result=0;
  for (  Object association : associations) {
    final OuterJoinableAssociation oj=(OuterJoinableAssociation)association;
    if (oj.getJoinable().consumesEntityAlias()) {
      if (oj.rhsAlias.equals(lhsAlias)) {
        return result;
      }
      result++;
    }
  }
  return -1;
}
