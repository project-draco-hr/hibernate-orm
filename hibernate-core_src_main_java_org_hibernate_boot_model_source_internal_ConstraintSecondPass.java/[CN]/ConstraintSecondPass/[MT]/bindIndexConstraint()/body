{
  final Index index=table.getOrCreateIndex(constraintSource.name());
  for (  String columnName : constraintSource.columnNames()) {
    final Identifier physicalName=sourceDocument.getMetadataCollector().getDatabase().toIdentifier(sourceDocument.getMetadataCollector().getPhysicalColumnName(table,columnName));
    index.addColumn(table.getColumn(physicalName));
  }
}
