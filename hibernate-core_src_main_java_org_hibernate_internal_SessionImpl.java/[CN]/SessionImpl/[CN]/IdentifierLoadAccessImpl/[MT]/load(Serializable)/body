{
  CacheMode sessionCacheMode=getCacheMode();
  boolean cacheModeChanged=false;
  if (cacheMode != null) {
    if (cacheMode != sessionCacheMode) {
      setCacheMode(cacheMode);
      cacheModeChanged=true;
    }
  }
  try {
    return doLoad(id);
  }
  finally {
    if (cacheModeChanged) {
      setCacheMode(sessionCacheMode);
    }
  }
}
