{
  errorIfClosed();
  final Object result=persistenceContext.getEntity(key);
  if (result == null) {
    final Object newObject=interceptor.getEntity(key.getEntityName(),key.getIdentifier());
    if (newObject != null) {
      lock(newObject,LockMode.NONE);
    }
    return newObject;
  }
 else {
    return result;
  }
}
