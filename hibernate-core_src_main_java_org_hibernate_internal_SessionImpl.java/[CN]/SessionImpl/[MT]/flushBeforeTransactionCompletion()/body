{
  final boolean doFlush=!isClosed() && isTransactionFlushable() && getHibernateFlushMode() != FlushMode.MANUAL;
  try {
    if (doFlush) {
      managedFlush();
    }
  }
 catch (  RuntimeException re) {
    throw exceptionMapper.mapManagedFlushFailure("error during managed flush",re,this);
  }
}
