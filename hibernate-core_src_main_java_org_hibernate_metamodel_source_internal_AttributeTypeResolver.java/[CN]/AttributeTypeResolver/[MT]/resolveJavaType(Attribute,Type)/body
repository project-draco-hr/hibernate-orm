{
  if (!(type instanceof AbstractStandardBasicType) || !attribute.isSingular()) {
    return;
  }
  AbstractAttributeContainer.SingularAttributeImpl singularAttribute=(AbstractAttributeContainer.SingularAttributeImpl)attribute;
  if (!singularAttribute.isTypeResolved()) {
    singularAttribute.resolveType(new BasicType(new JavaType(((AbstractStandardBasicType)type).getJavaTypeDescriptor().getJavaTypeClass())));
  }
}
