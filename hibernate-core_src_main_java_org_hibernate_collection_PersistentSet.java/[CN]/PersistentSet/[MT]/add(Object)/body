{
  Boolean exists=isOperationQueueEnabled() ? readElementExistence(value) : null;
  if (exists == null) {
    initialize(true);
    if (set.add(value)) {
      dirty();
      return true;
    }
 else {
      return false;
    }
  }
 else   if (exists.booleanValue()) {
    return false;
  }
 else {
    queueOperation(new SimpleAdd(value));
    return true;
  }
}
