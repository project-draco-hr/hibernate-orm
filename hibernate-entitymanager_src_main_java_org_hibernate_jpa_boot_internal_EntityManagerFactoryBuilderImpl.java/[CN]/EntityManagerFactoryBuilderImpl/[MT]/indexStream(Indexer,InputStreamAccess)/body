{
  try {
    InputStream stream=streamAccess.accessInputStream();
    try {
      indexer.index(stream);
    }
  finally {
      try {
        stream.close();
      }
 catch (      Exception ignore) {
      }
    }
  }
 catch (  IOException e) {
    throw persistenceException("Unable to index from stream " + streamAccess.getStreamName(),e);
  }
}
