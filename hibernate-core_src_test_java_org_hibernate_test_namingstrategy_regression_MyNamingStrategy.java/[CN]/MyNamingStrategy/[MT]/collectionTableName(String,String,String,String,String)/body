{
  String name=null;
  if ("localized".equals(propertyName)) {
    name=ownerEntity.replaceAll("\\.","_").toUpperCase() + "_LOCALIZED";
  }
  if (name == null) {
    name=super.collectionTableName(ownerEntity,ownerEntityTable,associatedEntity,associatedEntityTable,propertyName);
  }
  return name;
}
