{
switch (sqlCode) {
case Types.BOOLEAN:
    return BitTypeDescriptor.INSTANCE;
case Types.CLOB:
  return HANA_CLOB_STREAM_BINDING;
case Types.NCLOB:
return HANA_NCLOB_STREAM_BINDING;
case Types.TINYINT:
return SmallIntTypeDescriptor.INSTANCE;
default :
return super.getSqlTypeDescriptorOverride(sqlCode);
}
}
