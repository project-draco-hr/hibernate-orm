{
  for (int low=0, high=names.length - 1; low <= high; ) {
    final int middle=low + ((high - low) / 2);
    final int compare=names[middle].compareTo(name);
    if (compare > 0) {
      high=middle - 1;
    }
 else     if (compare < 0) {
      low=middle + 1;
    }
 else {
      return true;
    }
  }
  return false;
}
