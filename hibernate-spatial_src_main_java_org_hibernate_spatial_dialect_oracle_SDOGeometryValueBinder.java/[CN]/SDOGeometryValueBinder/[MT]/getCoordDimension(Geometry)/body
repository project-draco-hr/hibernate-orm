{
  MCoordinate c=MCoordinate.convertCoordinate(geom.getCoordinate());
  int d=0;
  if (c != null) {
    if (!Double.isNaN(c.x)) {
      d++;
    }
    if (!Double.isNaN(c.y)) {
      d++;
    }
    if (!Double.isNaN(c.z)) {
      d++;
    }
    if (!Double.isNaN(c.m)) {
      d++;
    }
  }
  return d;
}
