{
  final SDOGeometry geom=convertJTSGeometry(jtsGeom);
  if (geom != null) {
    try {
      return store(geom,connection);
    }
 catch (    SQLException e) {
      throw new HibernateException("Problem during conversion from JTS to SDOGeometry",e);
    }
catch (    FinderException e) {
      throw new HibernateException("OracleConnection could not be retrieved for creating SDOGeometry STRUCT",e);
    }
  }
 else {
    throw new UnsupportedOperationException("Conversion of " + jtsGeom.getClass().getSimpleName() + " to Oracle STRUCT not supported");
  }
}
