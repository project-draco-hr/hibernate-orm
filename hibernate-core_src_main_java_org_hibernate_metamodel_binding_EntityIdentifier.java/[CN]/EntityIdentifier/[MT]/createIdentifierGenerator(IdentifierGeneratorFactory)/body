{
  if (identifierGenerator == null) {
    Properties props=new Properties();
    if (idGenerator != null) {
      props.putAll(idGenerator.getParameters());
    }
    identifierGenerator=factory.createIdentifierGenerator(idGenerator.getStrategy(),getValueBinding().getHibernateTypeDescriptor().getResolvedTypeMapping(),props);
  }
  return identifierGenerator;
}
