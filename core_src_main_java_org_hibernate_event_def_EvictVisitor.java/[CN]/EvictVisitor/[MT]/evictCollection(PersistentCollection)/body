{
  CollectionEntry ce=(CollectionEntry)getSession().getPersistenceContext().getCollectionEntries().remove(collection);
  if (log.isDebugEnabled())   log.debug("evicting collection: " + MessageHelper.collectionInfoString(ce.getLoadedPersister(),ce.getLoadedKey(),getSession().getFactory()));
  if (ce.getLoadedPersister() != null && ce.getLoadedKey() != null) {
    getSession().getPersistenceContext().getCollectionsByKey().remove(new CollectionKey(ce.getLoadedPersister(),ce.getLoadedKey(),getSession().getEntityMode()));
  }
}
