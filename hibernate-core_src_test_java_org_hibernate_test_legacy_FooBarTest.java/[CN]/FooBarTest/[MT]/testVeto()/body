{
  Session s=openSession();
  s.beginTransaction();
  Vetoer v=new Vetoer();
  s.save(v);
  Serializable id=s.save(v);
  s.getTransaction().commit();
  s.close();
  s=openSession();
  s.beginTransaction();
  s.update(v,id);
  s.update(v,id);
  s.delete(v);
  s.delete(v);
  s.getTransaction().commit();
  s.close();
}
