{
  for (int i=0; i < size; i++) {
    if (names[i].equals(name)) {
      if (i < size) {
        for (int j=i; j < size - 1; j++) {
          names[j]=names[j + 1];
          owners[j]=owners[j + 1];
        }
        names[size - 1]=null;
        owners[size - 1]=null;
        size--;
      }
    }
  }
}
