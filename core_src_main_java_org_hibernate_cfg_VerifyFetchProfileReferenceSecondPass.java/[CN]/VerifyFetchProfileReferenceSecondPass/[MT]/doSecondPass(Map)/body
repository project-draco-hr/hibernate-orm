{
  org.hibernate.mapping.FetchProfile profile=mappings.findOrCreateFetchProfile(fetchProfileName,MetadataSource.ANNOTATIONS);
  if (MetadataSource.ANNOTATIONS != profile.getSource()) {
    return;
  }
  PersistentClass clazz=mappings.getClass(fetch.entity().getName());
  clazz.getProperty(fetch.association());
  profile.addFetch(fetch.entity().getName(),fetch.association(),fetch.mode().toString().toLowerCase());
}
