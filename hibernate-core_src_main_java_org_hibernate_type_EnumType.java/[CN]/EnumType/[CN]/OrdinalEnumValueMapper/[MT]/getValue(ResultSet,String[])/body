{
  final int ordinal=rs.getInt(names[0]);
  if (rs.wasNull()) {
    if (LOG.isTraceEnabled()) {
      LOG.tracev("Returning null as column {0}",names[0]);
    }
    return null;
  }
  final Enum enumValue=fromOrdinal(ordinal);
  if (LOG.isTraceEnabled()) {
    LOG.tracev("Returning '{0}' as column {1}",enumValue,names[0]);
  }
  return enumValue;
}
