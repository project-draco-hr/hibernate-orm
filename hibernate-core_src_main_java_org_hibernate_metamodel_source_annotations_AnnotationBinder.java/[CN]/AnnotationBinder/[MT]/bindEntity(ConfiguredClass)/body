{
  ClassInfo classInfo=entity.getClassInfo();
  AnnotationInstance jpaEntityAnnotation=JandexHelper.getSingleAnnotation(classInfo,JPADotNames.ENTITY);
  AnnotationInstance mappedSuperClassAnnotation=JandexHelper.getSingleAnnotation(classInfo,JPADotNames.MAPPED_SUPER_CLASS);
  AnnotationInstance hibernateEntityAnnotation=JandexHelper.getSingleAnnotation(classInfo,HibernateDotNames.ENTITY);
  log.info("Binding entity from annotated class: {}",classInfo.name());
  EntityBinder entityBinder=new EntityBinder(metadata,classInfo,jpaEntityAnnotation,hibernateEntityAnnotation);
}
