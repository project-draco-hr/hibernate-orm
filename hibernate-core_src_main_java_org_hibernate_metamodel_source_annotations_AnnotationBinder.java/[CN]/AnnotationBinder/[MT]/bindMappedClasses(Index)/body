{
  Set<ConfiguredClassHierarchy> hierarchies=ConfiguredClassHierarchyBuilder.createEntityHierarchies(annotationIndex,metadata.getServiceRegistry());
  for (  ConfiguredClassHierarchy hierarchy : hierarchies) {
    Iterator<ConfiguredClass> iter=hierarchy.iterator();
    while (iter.hasNext()) {
      ConfiguredClass entity=iter.next();
      bindEntity(entity);
    }
  }
}
