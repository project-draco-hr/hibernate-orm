{
  if (childFromClauses != null && !childFromClauses.isEmpty()) {
    Iterator children=childFromClauses.iterator();
    while (children.hasNext()) {
      FromClause child=(FromClause)children.next();
      if (child.findJoinByPathLocal(path) != null) {
        return child;
      }
    }
  }
  return null;
}
