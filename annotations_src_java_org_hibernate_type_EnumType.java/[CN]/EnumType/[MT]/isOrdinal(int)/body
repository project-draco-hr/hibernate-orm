{
switch (paramType) {
case Types.INTEGER:
case Types.NUMERIC:
case Types.SMALLINT:
case Types.TINYINT:
case Types.BIGINT:
case Types.DECIMAL:
case Types.DOUBLE:
case Types.FLOAT:
    return true;
case Types.CHAR:
case Types.LONGVARCHAR:
case Types.VARCHAR:
  return false;
default :
throw new HibernateException("Unable to persist an Enum in a column of SQL Type: " + paramType);
}
}
