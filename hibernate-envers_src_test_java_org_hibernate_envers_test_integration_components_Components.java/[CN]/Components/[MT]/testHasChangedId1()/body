{
  List list=getAuditReader().createQuery().forRevisionsOfEntity(ComponentTestEntity.class,false,false).add(AuditEntity.id().eq(id1)).add(AuditEntity.property("comp1").hasChanged()).getResultList();
  assertEquals(2,list.size());
  assertEquals(TestTools.makeList(1,2),extractRevisionNumbers(list));
  list=getAuditReader().createQuery().forRevisionsOfEntity(ComponentTestEntity.class,false,false).add(AuditEntity.id().eq(id1)).add(AuditEntity.property("comp1").hasNotChanged()).getResultList();
  assertEquals(0,list.size());
}
