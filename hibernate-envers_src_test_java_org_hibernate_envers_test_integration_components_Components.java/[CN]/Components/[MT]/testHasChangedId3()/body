{
  List list=getAuditReader().createQuery().forRevisionsOfEntity(ComponentTestEntity.class,false,true).add(AuditEntity.id().eq(id3)).add(AuditEntity.property("comp1").hasChanged()).getResultList();
  assertEquals(2,list.size());
  assertEquals(TestTools.makeList(1,3),extractRevisionNumbers(list));
  list=getAuditReader().createQuery().forRevisionsOfEntity(ComponentTestEntity.class,false,true).add(AuditEntity.id().eq(id3)).add(AuditEntity.property("comp1").hasNotChanged()).getResultList();
  assertEquals(0,list.size());
}
