{
  List list=getAuditReader().createQuery().forRevisionsOfEntity(ComponentTestEntity.class,false,true).add(AuditEntity.id().eq(id2)).add(AuditEntity.property("comp1").hasChanged()).getResultList();
  assertEquals(3,list.size());
  assertEquals(TestTools.makeList(1,2,4),extractRevisionNumbers(list));
  list=getAuditReader().createQuery().forRevisionsOfEntity(ComponentTestEntity.class,false,true).add(AuditEntity.id().eq(id2)).add(AuditEntity.property("comp1").hasNotChanged()).getResultList();
  assertEquals(0,list.size());
}
