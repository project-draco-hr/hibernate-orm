{
  if (accessType == null) {
    throw new IllegalArgumentException("access type can't be null.");
  }
switch (accessType) {
case FIELD:
    return MockHelper.getTarget(indexBuilder.getServiceRegistry(),classInfo,getFieldName(),MockHelper.TargetType.FIELD);
case PROPERTY:
  return MockHelper.getTarget(indexBuilder.getServiceRegistry(),classInfo,getFieldName(),MockHelper.TargetType.PROPERTY);
default :
throw new HibernateException("can't determin access type [" + accessType + "]");
}
}
