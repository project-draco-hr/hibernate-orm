{
  try {
    final Object componentInstance=dataObject != null ? dataObject : ReflectHelper.getDefaultConstructor(componentClass).newInstance();
    delegate.mapToEntityFromMap(entityInstantiator.getAuditConfiguration(),componentInstance,data,null,entityInstantiator.getAuditReaderImplementor(),revision);
    return componentInstance;
  }
 catch (  Exception e) {
    throw new AuditException(e);
  }
}
