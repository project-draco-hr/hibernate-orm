{
  ExpectedValue expectedValue=new ExpectedValue(SerializableToBlobType.class.getName(),Thing.class.getName(),false,SerializableToBlobType.class,false,false);
  checkHibernateTypeDescriptor(expectedValue,"serializable");
  assertTrue(getTypeDescriptor("serializable").getTypeParameters().get(SerializableToBlobType.CLASS_NAME).equals(Thing.class.getName()));
