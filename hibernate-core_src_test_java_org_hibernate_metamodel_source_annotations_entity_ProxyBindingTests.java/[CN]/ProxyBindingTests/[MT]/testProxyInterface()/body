{
  buildMetadataSources(ProxyInterfaceEntity.class);
  EntityBinding binding=getEntityBinding(ProxyInterfaceEntity.class);
  assertTrue("Wrong laziness",binding.isLazy());
  assertEquals("Wrong proxy interface","org.hibernate.metamodel.source.annotations.entity.ProxyBindingTests$ProxyInterfaceEntity",binding.getProxyInterfaceName());
}
