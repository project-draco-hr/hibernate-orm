{
  List result=new ArrayList();
  PreparedStatement select=c.prepareStatement("SELECT s.id_ as id_, s.name as name, s.address as address, s.count_ as count_, s.date_ as date_, s.pay as pay, s.other as other FROM Simple s");
  ResultSet rs=select.executeQuery();
  while (rs.next()) {
    Simple s=new Simple();
    new Long(rs.getLong("id_"));
    rs.wasNull();
    s.setName(rs.getString("name"));
    rs.wasNull();
    s.setAddress(rs.getString("address"));
    rs.wasNull();
    s.setCount(rs.getInt("count_"));
    rs.wasNull();
    s.setDate(rs.getTimestamp("date_"));
    rs.wasNull();
    s.setPay(new Float(rs.getFloat("pay")));
    rs.wasNull();
    rs.getLong("other");
    rs.wasNull();
    s.setOther(null);
    result.add(s);
  }
  rs.close();
  select.close();
  c.commit();
}
