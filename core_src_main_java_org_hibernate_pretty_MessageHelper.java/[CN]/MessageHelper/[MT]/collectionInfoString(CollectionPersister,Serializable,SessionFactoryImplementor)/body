{
  StringBuffer s=new StringBuffer();
  s.append('[');
  if (persister == null) {
    s.append("<unreferenced>");
  }
 else {
    s.append(persister.getRole());
    s.append('#');
    if (id == null) {
      s.append("<null>");
    }
 else {
      s.append(persister.getOwnerEntityPersister().getIdentifierType().toLoggableString(id,factory));
    }
  }
  s.append(']');
  return s.toString();
}
