{
  Geometry forced=jtsGeom;
  if (forced.isEmpty()) {
    GeometryFactory factory=jtsGeom.getFactory();
    if (factory == null) {
      factory=HBSpatialExtension.getDefaultGeomFactory();
    }
    forced=factory.createGeometryCollection(null);
    forced.setSRID(jtsGeom.getSRID());
  }
  return forced;
}
