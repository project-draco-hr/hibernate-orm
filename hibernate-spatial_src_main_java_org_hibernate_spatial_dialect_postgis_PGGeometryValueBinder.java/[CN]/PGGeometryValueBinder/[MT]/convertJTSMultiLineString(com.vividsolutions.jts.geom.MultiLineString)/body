{
  org.postgis.LineString[] lines=new org.postgis.LineString[string.getNumGeometries()];
  for (int i=0; i < string.getNumGeometries(); i++) {
    lines[i]=new org.postgis.LineString(toPoints(string.getGeometryN(i).getCoordinates()));
  }
  MultiLineString mls=new MultiLineString(lines);
  if (string instanceof MGeometry) {
    mls.haveMeasure=true;
  }
  mls.setSrid(string.getSRID());
  return mls;
}
