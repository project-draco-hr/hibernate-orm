{
  if (implicitNamingStrategy != null) {
    try {
      metadataBuilder.applyImplicitNamingStrategy((ImplicitNamingStrategy)ReflectHelper.classForName(implicitNamingStrategy).newInstance());
    }
 catch (    Exception e) {
      throw new BuildException("Unable to instantiate specified ImplicitNamingStrategy [" + implicitNamingStrategy + "]",e);
    }
  }
  if (physicalNamingStrategy != null) {
    try {
      metadataBuilder.applyPhysicalNamingStrategy((PhysicalNamingStrategy)ReflectHelper.classForName(physicalNamingStrategy).newInstance());
    }
 catch (    Exception e) {
      throw new BuildException("Unable to instantiate specified PhysicalNamingStrategy [" + physicalNamingStrategy + "]",e);
    }
  }
}
