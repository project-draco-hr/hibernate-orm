{
  List list=queryForPropertyHasChanged(StringMapEntity.class,sme1_id,"strings");
  assertEquals(3,list.size());
  assertEquals(TestTools.makeList(1,2,3),extractRevisionNumbers(list));
  list=queryForPropertyHasChanged(StringMapEntity.class,sme2_id,"strings");
  assertEquals(2,list.size());
  assertEquals(TestTools.makeList(1,3),extractRevisionNumbers(list));
  list=queryForPropertyHasNotChanged(StringMapEntity.class,sme1_id,"strings");
  assertEquals(0,list.size());
  list=queryForPropertyHasNotChanged(StringMapEntity.class,sme2_id,"strings");
  assertEquals(0,list.size());
}
