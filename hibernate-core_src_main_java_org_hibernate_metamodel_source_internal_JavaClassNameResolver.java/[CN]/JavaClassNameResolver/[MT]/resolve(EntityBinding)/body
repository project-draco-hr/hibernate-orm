{
  Entity entity=entityBinding.getEntity();
  if (entity == null) {
    throw new MappingException("Cannot resolve Java type names because the domain model has not been bound to EntityBinding.");
  }
  String entityClassName=entity.getPojoEntitySpecifics().getClassName() != null ? entity.getPojoEntitySpecifics().getClassName() : entity.getPojoEntitySpecifics().getProxyInterfaceName();
  if (entityClassName == null) {
    throw new MappingException("No Java class or interface defined for: " + entityBinding.getEntity().getName());
  }
  for (  Attribute attribute : entity.getAttributes()) {
    if (attribute.isSingular()) {
      SingularAttribute singularAttribute=SingularAttribute.class.cast(attribute);
      if (singularAttribute.getSingularAttributeType().getName() == null) {
      }
    }
  }
}
