{
  byte[] bytes=unWrap(val);
  StringBuilder buf=new StringBuilder(2 * bytes.length);
  for (int i=0; i < bytes.length; i++) {
    String hexStr=Integer.toHexString(bytes[i] - Byte.MIN_VALUE);
    if (hexStr.length() == 1)     buf.append('0');
    buf.append(hexStr);
  }
  return buf.toString();
}
