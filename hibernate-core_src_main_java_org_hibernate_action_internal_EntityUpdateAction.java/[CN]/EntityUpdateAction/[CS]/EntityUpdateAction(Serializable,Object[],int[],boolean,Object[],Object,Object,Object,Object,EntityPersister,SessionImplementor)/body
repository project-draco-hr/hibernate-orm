{
  super(session,id,instance,persister);
  this.state=state;
  this.previousState=previousState;
  this.previousVersion=previousVersion;
  this.nextVersion=nextVersion;
  this.dirtyFields=dirtyProperties;
  this.hasDirtyCollection=hasDirtyCollection;
  this.rowId=rowId;
  this.previousNaturalIdValues=determinePreviousNaturalIdValues(persister,previousState,session,id);
  session.getPersistenceContext().getNaturalIdHelper().manageLocalNaturalIdCrossReference(persister,id,state,previousNaturalIdValues,CachedNaturalIdValueSource.UPDATE);
}
