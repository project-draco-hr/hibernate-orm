{
  PostUpdateEventListener[] postListeners=getSession().getListeners().getPostCommitUpdateEventListeners();
  if (postListeners.length > 0) {
    PostUpdateEvent postEvent=new PostUpdateEvent(getInstance(),getId(),state,previousState,dirtyFields,getPersister(),(EventSource)getSession());
    for (int i=0; i < postListeners.length; i++) {
      postListeners[i].onPostUpdate(postEvent);
    }
  }
}
