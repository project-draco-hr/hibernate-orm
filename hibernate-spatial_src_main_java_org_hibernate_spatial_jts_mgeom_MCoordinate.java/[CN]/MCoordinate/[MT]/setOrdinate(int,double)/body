{
switch (ordinateIndex) {
case CoordinateSequence.X:
    this.x=value;
  break;
case CoordinateSequence.Y:
this.y=value;
break;
case CoordinateSequence.Z:
this.z=value;
break;
case CoordinateSequence.M:
this.m=value;
break;
default :
throw new IllegalArgumentException("invalid ordinateIndex");
}
}
