{
  final ReflectionManager reflectionManager=mappings.getReflectionManager();
  InheritanceState superEntityState=InheritanceState.getInheritanceStateOfSuperEntity(clazzToProcess,inheritanceStatePerClass,reflectionManager);
  PersistentClass superEntity=superEntityState != null ? mappings.getClass(superEntityState.getClazz().getName()) : null;
  if (superEntity == null) {
    if (inheritanceState.hasParents()) {
      throw new AssertionFailure("Subclass has to be binded after it's mother class: " + superEntityState.getClazz().getName());
    }
  }
  return superEntity;
}
