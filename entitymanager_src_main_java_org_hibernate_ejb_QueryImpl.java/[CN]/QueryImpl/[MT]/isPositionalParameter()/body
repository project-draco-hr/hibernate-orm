{
  if (isPositional == null) {
    String queryString=query.getQueryString();
    int index=queryString.indexOf('?');
    if (index == -1) {
      isPositional=true;
    }
 else     if (index == queryString.length() - 1) {
      isPositional=false;
    }
 else {
      isPositional=Character.isDigit(queryString.charAt(index + 1));
    }
  }
  return isPositional;
}
