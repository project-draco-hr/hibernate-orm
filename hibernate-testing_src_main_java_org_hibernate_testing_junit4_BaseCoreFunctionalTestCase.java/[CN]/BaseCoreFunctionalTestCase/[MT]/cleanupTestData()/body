{
  Session s=openSession();
  Transaction transaction=s.beginTransaction();
  try {
    s.createQuery("delete from java.lang.Object").executeUpdate();
    transaction.commit();
  }
 catch (  Exception e) {
    if (transaction.getStatus().canRollback()) {
      transaction.rollback();
    }
  }
 finally {
    s.close();
  }
}
