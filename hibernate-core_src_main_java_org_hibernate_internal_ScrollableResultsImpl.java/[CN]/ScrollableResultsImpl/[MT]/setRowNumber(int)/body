{
  if (rowNumber >= 0) {
    rowNumber++;
  }
  try {
    final boolean result=getResultSet().absolute(rowNumber);
    prepareCurrentRow(result);
    return result;
  }
 catch (  SQLException sqle) {
    throw getSession().getFactory().getSQLExceptionHelper().convert(sqle,"could not advance using absolute()");
  }
}
