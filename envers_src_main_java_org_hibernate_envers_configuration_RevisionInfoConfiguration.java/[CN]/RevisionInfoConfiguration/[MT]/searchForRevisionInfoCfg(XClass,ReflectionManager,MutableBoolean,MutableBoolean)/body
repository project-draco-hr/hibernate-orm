{
  XClass superclazz=clazz.getSuperclass();
  if (!"java.lang.Object".equals(superclazz.getName())) {
    searchForRevisionInfoCfg(superclazz,reflectionManager,revisionNumberFound,revisionTimestampFound);
  }
  searchForRevisionInfoCfgInProperties(clazz,reflectionManager,revisionNumberFound,revisionTimestampFound,"field");
  searchForRevisionInfoCfgInProperties(clazz,reflectionManager,revisionNumberFound,revisionTimestampFound,"property");
}
