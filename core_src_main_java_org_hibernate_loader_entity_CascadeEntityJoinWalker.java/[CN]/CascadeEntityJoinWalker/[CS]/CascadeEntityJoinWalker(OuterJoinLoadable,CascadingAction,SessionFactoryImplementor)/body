{
  super(persister,factory,LoadQueryInfluencers.NONE);
  this.cascadeAction=action;
  StringBuffer whereCondition=whereString(getAlias(),persister.getIdentifierColumnNames(),1).append(persister.filterFragment(getAlias(),CollectionHelper.EMPTY_MAP));
  initAll(whereCondition.toString(),"",LockMode.READ);
}
