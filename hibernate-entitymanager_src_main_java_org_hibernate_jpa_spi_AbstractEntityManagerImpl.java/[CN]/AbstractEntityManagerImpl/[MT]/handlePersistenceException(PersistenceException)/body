{
  if (e instanceof NoResultException) {
    return;
  }
  if (e instanceof NonUniqueResultException) {
    return;
  }
  if (e instanceof LockTimeoutException) {
    return;
  }
  if (e instanceof QueryTimeoutException) {
    return;
  }
  try {
    markForRollbackOnly();
  }
 catch (  Exception ne) {
    LOG.unableToMarkForRollbackOnPersistenceException(ne);
  }
}
