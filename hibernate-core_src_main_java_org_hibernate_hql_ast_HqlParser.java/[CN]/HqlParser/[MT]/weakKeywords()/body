{
  int t=LA(1);
switch (t) {
case ORDER:
case GROUP:
    if (LA(2) != LITERAL_by) {
      LT(1).setType(IDENT);
      LOG.debugf("weakKeywords() : new LT(1) token - %s",LT(1));
    }
  break;
default :
if (LA(0) == FROM && t != IDENT && LA(2) == DOT) {
  HqlToken hqlToken=(HqlToken)LT(1);
  if (hqlToken.isPossibleID()) {
    hqlToken.setType(IDENT);
    LOG.debugf("weakKeywords() : new LT(1) token - %s",LT(1));
  }
}
break;
}
}
