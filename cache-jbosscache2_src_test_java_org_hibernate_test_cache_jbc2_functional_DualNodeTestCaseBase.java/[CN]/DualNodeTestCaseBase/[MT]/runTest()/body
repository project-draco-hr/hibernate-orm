{
  try {
    super.runTest();
  }
  finally {
    if (secondNodeSession != null && secondNodeSession.isOpen()) {
      if (secondNodeSession.isConnected()) {
        secondNodeSession.connection().rollback();
      }
      secondNodeSession.close();
      secondNodeSession=null;
      fail("unclosed session");
    }
 else {
      secondNodeSession=null;
    }
  }
}
