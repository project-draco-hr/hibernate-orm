{
  return new EntityLoader(persister.getFactory(),persister,keyColumnNames,keyType,new QueryBuildingParameters(){
    @Override public LoadQueryInfluencers getQueryInfluencers(){
      return influencers;
    }
    @Override public int getBatchSize(){
      return batchSize;
    }
    @Override public LockMode getLockMode(){
      return lockMode;
    }
    @Override public LockOptions getLockOptions(){
      return lockOptions;
    }
  }
);
}
