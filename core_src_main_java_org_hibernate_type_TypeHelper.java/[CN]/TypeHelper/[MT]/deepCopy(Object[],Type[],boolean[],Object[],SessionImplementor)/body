{
  for (int i=0; i < types.length; i++) {
    if (copy[i]) {
      if (values[i] == LazyPropertyInitializer.UNFETCHED_PROPERTY || values[i] == BackrefPropertyAccessor.UNKNOWN) {
        target[i]=values[i];
      }
 else {
        target[i]=types[i].deepCopy(values[i],session.getEntityMode(),session.getFactory());
      }
    }
  }
}
