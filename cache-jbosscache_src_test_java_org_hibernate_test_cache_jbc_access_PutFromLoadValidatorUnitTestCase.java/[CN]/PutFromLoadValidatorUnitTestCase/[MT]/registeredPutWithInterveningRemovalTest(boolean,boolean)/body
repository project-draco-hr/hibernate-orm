{
  PutFromLoadValidator testee=new PutFromLoadValidator(transactional ? tm : null);
  if (transactional) {
    tm.begin();
  }
  testee.registerPendingPut(KEY1);
  if (removeRegion) {
    testee.regionRemoved();
  }
 else {
    testee.keyRemoved(KEY1);
  }
  assertFalse(testee.isPutValid(KEY1));
}
