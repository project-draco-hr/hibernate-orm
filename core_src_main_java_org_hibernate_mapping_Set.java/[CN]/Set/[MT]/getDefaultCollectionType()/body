{
  if (isSorted()) {
    return getMappings().getTypeResolver().getTypeFactory().sortedSet(getRole(),getReferencedPropertyName(),isEmbedded(),getComparator());
  }
 else   if (hasOrder()) {
    return getMappings().getTypeResolver().getTypeFactory().orderedSet(getRole(),getReferencedPropertyName(),isEmbedded());
  }
 else {
    return getMappings().getTypeResolver().getTypeFactory().set(getRole(),getReferencedPropertyName(),isEmbedded());
  }
}
