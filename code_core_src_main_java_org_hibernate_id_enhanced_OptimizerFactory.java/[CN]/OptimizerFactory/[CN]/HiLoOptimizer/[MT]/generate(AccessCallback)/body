{
  if (lastSourceValue < 0) {
    lastSourceValue=callback.getNextValue();
    while (lastSourceValue <= 0) {
      lastSourceValue=callback.getNextValue();
    }
    hiValue=(lastSourceValue * incrementSize) + 1;
    value=hiValue - incrementSize;
  }
 else   if (value >= hiValue) {
    lastSourceValue=callback.getNextValue();
    hiValue=(lastSourceValue * incrementSize) + 1;
  }
  return make(value++);
}
