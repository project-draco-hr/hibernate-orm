{
  callback.addAttributeSource(new SingularAttributeSourceBasicImpl(mappingDocument,callback.getAttributeSourceContainer(),basicAttributeJaxbMapping,logicalTableName,naturalIdMutability));
  processConstraints(mappingDocument,callback,logicalTableName,basicAttributeJaxbMapping.getColumnAttribute(),basicAttributeJaxbMapping.getColumnOrFormula(),basicAttributeJaxbMapping.getIndex(),basicAttributeJaxbMapping.getUniqueKey());
}
