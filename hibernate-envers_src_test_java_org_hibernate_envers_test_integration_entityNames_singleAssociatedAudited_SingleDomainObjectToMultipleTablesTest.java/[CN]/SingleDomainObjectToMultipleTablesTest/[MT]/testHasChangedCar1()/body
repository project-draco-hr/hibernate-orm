{
  List list=getAuditReader().createQuery().forRevisionsOfEntity(Car.class,false,false).add(AuditEntity.id().eq(carId)).add(AuditEntity.property("owner").hasChanged()).add(AuditEntity.property("driver").hasChanged()).getResultList();
  assertEquals(1,list.size());
  assertEquals(TestTools.makeList(1),extractRevisionNumbers(list));
}
