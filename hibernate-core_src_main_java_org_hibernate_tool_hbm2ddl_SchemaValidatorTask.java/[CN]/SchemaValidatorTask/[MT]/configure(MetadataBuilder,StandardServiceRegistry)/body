{
  final StrategySelector strategySelector=serviceRegistry.getService(StrategySelector.class);
  if (implicitNamingStrategy != null) {
    metadataBuilder.with(strategySelector.resolveStrategy(ImplicitNamingStrategy.class,implicitNamingStrategy));
  }
  if (physicalNamingStrategy != null) {
    metadataBuilder.with(strategySelector.resolveStrategy(PhysicalNamingStrategy.class,physicalNamingStrategy));
  }
}
