{
  EntityPersister p=sessionFactory.getEntityPersister(entityName);
  if (p.hasCache()) {
    if (LOG.isDebugEnabled()) {
      LOG.debugf("Evicting second-level cache: %s",MessageHelper.infoString(p,identifier,sessionFactory));
    }
    EntityRegionAccessStrategy cache=p.getCacheAccessStrategy();
    EntityCacheKey key=cache.generateCacheKey(identifier,p,sessionFactory,null);
    cache.evict(key);
  }
}
