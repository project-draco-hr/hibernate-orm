{
  Session s=openSession();
  s.beginTransaction();
  Parent p=new Parent("foo");
  s.persist(p);
  s.getTransaction().commit();
  s.close();
  s=openSession();
  s.beginTransaction();
  p=(Parent)s.load(Parent.class,p.getName());
  s.delete(p);
  s.persist(p);
  s.getTransaction().commit();
  s.close();
}
