{
  if (x == y) {
    return true;
  }
  if (x == null || y == null) {
    return false;
  }
  Object[] xvalues=getPropertyValues(x,entityMode);
  Object[] yvalues=getPropertyValues(y,entityMode);
  for (int i=0; i < propertySpan; i++) {
    if (!propertyTypes[i].isEqual(xvalues[i],yvalues[i],entityMode,factory)) {
      return false;
    }
  }
  return true;
}
