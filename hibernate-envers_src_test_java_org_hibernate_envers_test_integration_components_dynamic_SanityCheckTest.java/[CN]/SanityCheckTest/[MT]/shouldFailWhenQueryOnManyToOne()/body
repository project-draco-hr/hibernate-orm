{
  PlainEntity plainEntity=(PlainEntity)getAuditReader().createQuery().forEntitiesAtRevision(PlainEntity.class,1).add(AuditEntity.relatedId("component_manyToOneEntity").eq(getManyToOneEntity().getId())).getResultList().get(0);
  Assert.assertEquals(getManyToOneEntity(),plainEntity.getComponent().getManyToOneEntity());
}
