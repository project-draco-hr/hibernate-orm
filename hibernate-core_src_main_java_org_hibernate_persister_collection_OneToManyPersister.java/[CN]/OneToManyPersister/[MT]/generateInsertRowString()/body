{
  final Update update=new Update(getDialect()).setTableName(qualifiedTableName).addColumns(keyColumnNames);
  if (hasIndex && !indexContainsFormula) {
    update.addColumns(indexColumnNames);
  }
  if (getFactory().getSessionFactoryOptions().isCommentsEnabled()) {
    update.setComment("create one-to-many row " + getRole());
  }
  return update.addPrimaryKeyColumns(elementColumnNames,elementColumnWriters).toStatementString();
}
