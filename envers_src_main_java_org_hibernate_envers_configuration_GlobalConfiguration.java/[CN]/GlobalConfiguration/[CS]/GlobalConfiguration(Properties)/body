{
  String generateRevisionsForCollectionsStr=properties.getProperty("org.hibernate.envers.revisionOnCollectionChange","true");
  generateRevisionsForCollections=Boolean.parseBoolean(generateRevisionsForCollectionsStr);
  String warnOnUnsupportedTypesStr=properties.getProperty("org.hibernate.envers.warnOnUnsupportedTypes","false");
  warnOnUnsupportedTypes=Boolean.parseBoolean(warnOnUnsupportedTypesStr);
  String ignoreOptimisticLockingPropertyStr=properties.getProperty("org.hibernate.envers.unversionedOptimisticLockingField","false");
  unversionedOptimisticLockingField=Boolean.parseBoolean(ignoreOptimisticLockingPropertyStr);
  correlatedSubqueryOperator="org.hibernate.dialect.HSQLDialect".equals(properties.getProperty("hibernate.dialect")) ? "in" : "=";
}
