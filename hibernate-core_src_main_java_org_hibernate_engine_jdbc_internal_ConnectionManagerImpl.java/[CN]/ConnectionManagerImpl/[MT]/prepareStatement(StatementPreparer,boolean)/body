{
  if (forceExecuteBatch) {
    executeBatch();
  }
  try {
    PreparedStatement ps=preparer.doPrepare();
    preparer.afterPrepare(ps);
    return ps;
  }
 catch (  SQLException sqle) {
    log.error("sqlexception escaped proxy",sqle);
    throw logicalConnection.getJdbcServices().getSqlExceptionHelper().convert(sqle,"could not prepare statement",preparer.getSqlToPrepare());
  }
}
