{
  String entityName=event.getPersister().getEntityName();
  if (verCfg.getEntCfg().isVersioned(entityName)) {
    AuditSync verSync=verCfg.getSyncManager().get(event.getSession());
    verSync.addWorkUnit(new AddWorkUnit(event.getPersister().getEntityName(),verCfg,event.getId(),event.getPersister(),event.getState()));
    generateBidirectionalCollectionChangeWorkUnits(verSync,event.getPersister(),entityName,event.getState(),null,event.getSession());
  }
}
