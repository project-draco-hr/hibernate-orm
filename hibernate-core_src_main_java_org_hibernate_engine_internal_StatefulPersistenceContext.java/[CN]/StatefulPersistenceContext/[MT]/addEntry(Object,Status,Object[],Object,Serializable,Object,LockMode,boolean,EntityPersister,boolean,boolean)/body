{
  final EntityEntryFactory entityEntryFactory=persister.getEntityEntryFactory();
  final EntityEntry e=entityEntryFactory.createEntityEntry(status,loadedState,rowId,id,version,lockMode,existsInDatabase,persister,disableVersionIncrement,lazyPropertiesAreUnfetched,this);
  entityEntryContext.addEntityEntry(entity,e);
  setHasNonReadOnlyEnties(status);
  return e;
}
