{
  final EntityEntry e=new EntityEntry(status,loadedState,rowId,id,version,lockMode,existsInDatabase,persister,persister.getEntityMode(),session.getTenantIdentifier(),disableVersionIncrement,lazyPropertiesAreUnfetched,this);
  entityEntryContext.addEntityEntry(entity,e);
  setHasNonReadOnlyEnties(status);
  return e;
}
