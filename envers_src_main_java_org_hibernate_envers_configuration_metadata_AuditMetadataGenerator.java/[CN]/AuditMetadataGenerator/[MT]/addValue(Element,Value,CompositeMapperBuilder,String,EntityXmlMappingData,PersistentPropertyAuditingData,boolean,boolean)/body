{
  Type type=value.getType();
  if (firstPass) {
    if (basicMetadataGenerator.addBasic(parent,persistentPropertyAuditingData,value,currentMapper,insertable,false)) {
      return;
    }
  }
  if (type instanceof ComponentType) {
    componentMetadataGenerator.addComponent(parent,persistentPropertyAuditingData,value,currentMapper,entityName,xmlMappingData,firstPass);
  }
 else   if (type instanceof ManyToOneType) {
    if (!firstPass) {
      toOneRelationMetadataGenerator.addToOne(parent,persistentPropertyAuditingData,value,currentMapper,entityName,insertable);
    }
  }
 else   if (type instanceof OneToOneType) {
    if (!firstPass) {
      toOneRelationMetadataGenerator.addOneToOneNotOwning(persistentPropertyAuditingData,value,currentMapper,entityName);
    }
  }
 else   if (type instanceof CollectionType) {
    if (!firstPass) {
      CollectionMetadataGenerator collectionMetadataGenerator=new CollectionMetadataGenerator(this,(Collection)value,currentMapper,entityName,xmlMappingData,persistentPropertyAuditingData);
      collectionMetadataGenerator.addCollection();
    }
  }
 else {
    if (firstPass) {
      throwUnsupportedTypeException(type,entityName,persistentPropertyAuditingData.getName());
    }
  }
}
