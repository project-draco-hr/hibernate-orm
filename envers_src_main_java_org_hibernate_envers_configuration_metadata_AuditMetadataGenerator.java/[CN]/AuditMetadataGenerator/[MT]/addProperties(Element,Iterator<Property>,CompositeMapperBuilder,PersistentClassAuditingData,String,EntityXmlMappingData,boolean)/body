{
  while (properties.hasNext()) {
    Property property=properties.next();
    String propertyName=property.getName();
    if (auditingData.getPropertyAuditingData(propertyName) != null) {
      addValue(parent,property.getValue(),currentMapper,entityName,xmlMappingData,auditingData.getPropertyAuditingData(propertyName),property.isInsertable(),firstPass);
    }
  }
}
