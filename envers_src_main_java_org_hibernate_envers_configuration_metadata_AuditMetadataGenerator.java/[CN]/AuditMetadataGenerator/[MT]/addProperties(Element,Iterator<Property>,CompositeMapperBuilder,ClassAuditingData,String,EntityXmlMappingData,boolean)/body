{
  while (properties.hasNext()) {
    Property property=properties.next();
    String propertyName=property.getName();
    PropertyAuditingData propertyAuditingData=auditingData.getPropertyAuditingData(propertyName);
    if (propertyAuditingData != null) {
      addValue(parent,property.getValue(),currentMapper,entityName,xmlMappingData,propertyAuditingData,property.isInsertable(),firstPass);
    }
  }
}
