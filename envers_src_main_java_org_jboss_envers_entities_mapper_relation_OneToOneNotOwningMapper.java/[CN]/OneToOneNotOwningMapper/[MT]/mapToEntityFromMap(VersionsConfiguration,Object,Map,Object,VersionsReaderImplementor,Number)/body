{
  if (obj == null) {
    return;
  }
  Class<?> entityClass=ReflectionTools.loadClass(owningEntityName);
  Object value;
  try {
    value=versionsReader.createQuery().forEntitiesAtRevision(entityClass,revision).add(VersionsRestrictions.relatedIdEq(owningReferencePropertyName,primaryKey)).getSingleResult();
  }
 catch (  NoResultException e) {
    value=null;
  }
catch (  NonUniqueResultException e) {
    throw new VersionsException("Many versions results for one-to-one relationship: (" + owningEntityName + ", "+ owningReferencePropertyName+ ")");
  }
  Setter setter=ReflectionTools.getSetter(obj.getClass(),propertyName);
  setter.set(obj,value,null);
}
