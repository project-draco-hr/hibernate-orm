{
  final String name=attribute.getName();
  try {
    Field field=metamodelClass.getDeclaredField(name);
    try {
      if (!field.isAccessible()) {
        field.setAccessible(true);
      }
      field.set(null,attribute);
    }
 catch (    IllegalAccessException e) {
      throw new AssertionFailure("Unable to inject static metamodel attribute : " + metamodelClass.getName() + '#'+ name,e);
    }
catch (    IllegalArgumentException e) {
      log.error("Illegal argument on static metamodel field injection : " + metamodelClass.getName() + '#'+ name+ "; expected type :  "+ attribute.getClass().getName()+ "; encountered type : "+ field.getType().getName());
    }
  }
 catch (  NoSuchFieldException e) {
    log.error("Unable to locate static metamodel field : " + metamodelClass.getName() + '#'+ name);
  }
}
