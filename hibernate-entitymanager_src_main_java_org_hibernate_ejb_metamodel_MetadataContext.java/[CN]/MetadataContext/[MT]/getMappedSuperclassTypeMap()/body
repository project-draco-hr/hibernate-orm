{
  final Map<Class<?>,MappedSuperclassType<?>> mappedSuperClassTypeMap=CollectionHelper.mapOfSize(mappedSuperclassByMappedSuperclassMapping.size());
  for (  MappedSuperclassTypeImpl mappedSuperclassType : mappedSuperclassByMappedSuperclassMapping.values()) {
    mappedSuperClassTypeMap.put(mappedSuperclassType.getJavaType(),mappedSuperclassType);
  }
  return mappedSuperClassTypeMap;
}
