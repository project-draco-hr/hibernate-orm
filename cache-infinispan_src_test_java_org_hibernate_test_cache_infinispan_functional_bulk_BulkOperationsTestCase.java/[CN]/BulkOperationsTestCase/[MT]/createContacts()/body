{
  log.debug("Create 10 contacts");
  tm.begin();
  try {
    for (int i=0; i < 10; i++)     createCustomer(i);
  }
 catch (  Exception e) {
    log.error("Unable to create customer",e);
    tm.setRollbackOnly();
    throw e;
  }
 finally {
    if (tm.getStatus() == Status.STATUS_ACTIVE)     tm.commit();
 else     tm.rollback();
  }
}
