{
  super(sessionImplementor,entityName,enversService,id,RevisionType.MOD);
  this.entityPersister=entityPersister;
  this.oldState=oldState;
  this.newState=newState;
  this.data=new HashMap<String,Object>();
  this.changes=enversService.getEntitiesConfigurations().get(getEntityName()).getPropertyMapper().map(sessionImplementor,data,entityPersister.getPropertyNames(),newState,oldState);
}
