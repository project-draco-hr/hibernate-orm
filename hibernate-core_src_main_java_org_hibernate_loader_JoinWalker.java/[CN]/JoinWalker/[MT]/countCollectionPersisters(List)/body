{
  int result=0;
  Iterator iter=associations.iterator();
  while (iter.hasNext()) {
    OuterJoinableAssociation oj=(OuterJoinableAssociation)iter.next();
    if (oj.getJoinType() == JoinFragment.LEFT_OUTER_JOIN && oj.getJoinable().isCollection() && !oj.hasRestriction()) {
      result++;
    }
  }
  return result;
}
