{
  if (fetches == null) {
    return;
  }
  for (  Fetch fetch : fetches) {
    ((TableExpressionMapper)fetch).prepareAlias(renderingContext);
    jpaqlQuery.append(renderJoinType(fetch.getJoinType())).append("fetch ").append(((TableExpressionMapper)fetch).renderTableExpression(renderingContext));
    renderFetches(jpaqlQuery,renderingContext,fetch.getFetches());
  }
}
