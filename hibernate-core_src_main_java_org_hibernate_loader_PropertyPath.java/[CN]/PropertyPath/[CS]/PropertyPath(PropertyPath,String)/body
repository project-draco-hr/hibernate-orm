{
  this.parent=parent;
  this.property=property;
  final String prefix;
  if (parent != null) {
    final String resolvedParent=parent.getFullPath();
    if (StringHelper.isEmpty(resolvedParent)) {
      prefix="";
    }
 else {
      prefix=resolvedParent + '.';
    }
  }
 else {
    prefix="";
  }
  this.fullPath=prefix + property;
}
