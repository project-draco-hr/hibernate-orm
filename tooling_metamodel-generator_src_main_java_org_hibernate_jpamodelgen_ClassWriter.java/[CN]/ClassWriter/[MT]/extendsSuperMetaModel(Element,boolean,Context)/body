{
  String superClassName=((TypeElement)superClassElement).getQualifiedName().toString();
  if (context.containsMetaEntity(superClassName) || context.containsMetaEmbeddable(superClassName)) {
    return true;
  }
  if (!entityMetaComplete && (superClassElement.getAnnotation(Entity.class) != null || superClassElement.getAnnotation(MappedSuperclass.class) != null)) {
    return true;
  }
  return false;
}
