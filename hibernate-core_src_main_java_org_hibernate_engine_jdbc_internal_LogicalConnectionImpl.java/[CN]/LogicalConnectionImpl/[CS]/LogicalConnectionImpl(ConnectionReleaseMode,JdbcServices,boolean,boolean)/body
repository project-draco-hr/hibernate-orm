{
  this.connectionReleaseMode=determineConnectionReleaseMode(isUserSuppliedConnection,connectionReleaseMode,jdbcServices);
  this.jdbcServices=jdbcServices;
  this.jdbcResourceRegistry=new JdbcResourceRegistryImpl(jdbcServices.getSqlExceptionHelper());
  this.isUserSuppliedConnection=isUserSuppliedConnection;
  this.isClosed=isClosed;
}
