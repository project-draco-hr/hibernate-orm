{
  Comparator versionComparator=null;
  if (model.isVersioned()) {
    versionComparator=((VersionType)model.getVersioningValueBinding().getHibernateTypeDescriptor().getExplicitType()).getComparator();
  }
  return new CacheDataDescriptionImpl(model.isMutable(),model.isVersioned(),versionComparator);
}
