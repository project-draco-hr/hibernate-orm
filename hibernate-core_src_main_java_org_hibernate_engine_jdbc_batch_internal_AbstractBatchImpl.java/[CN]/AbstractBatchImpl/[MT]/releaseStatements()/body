{
  for (  PreparedStatement statement : getStatements().values()) {
    try {
      statement.clearBatch();
      jdbcCoordinator.release(statement);
    }
 catch (    SQLException e) {
      LOG.unableToReleaseBatchStatement();
      LOG.sqlExceptionEscapedProxy(e);
    }
  }
  getStatements().clear();
}
