{
  if (hasSingleIdAttribute()) {
    return null;
  }
  @SuppressWarnings("unchecked") Iterator<Property> properties=persistentClass.getIdentifierMapper().getPropertyIterator();
  Set<SingularAttribute<? super X,?>> attributes=new HashSet<SingularAttribute<? super X,?>>();
  while (properties.hasNext()) {
    attributes.add((SingularAttribute<? super X,?>)MetamodelFactory.getAttribute(this,properties.next(),context));
  }
  return attributes;
}
