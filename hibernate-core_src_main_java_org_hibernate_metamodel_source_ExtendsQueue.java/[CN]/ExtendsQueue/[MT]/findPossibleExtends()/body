{
  Iterator<ExtendsQueueEntry> itr=extendsQueue.iterator();
  while (itr.hasNext()) {
    final ExtendsQueueEntry entry=itr.next();
    boolean found=metadata.getEntityBinding(entry.getExplicitName()) == null && metadata.getEntityBinding(HbmHelper.getClassName(entry.getExplicitName(),entry.getMappingPackage())) != null;
    if (found) {
      itr.remove();
      return entry;
    }
  }
  return null;
}
