{
  walkEntityTree(persister,getAlias());
  List allAssociations=new ArrayList();
  allAssociations.addAll(associations);
  allAssociations.add(new OuterJoinableAssociation(persister.getEntityType(),null,null,alias,JoinFragment.LEFT_OUTER_JOIN,null,getFactory(),CollectionHelper.EMPTY_MAP));
  initPersisters(allAssociations,lockOptions);
  initStatementString(whereString,orderByString,lockOptions);
}
