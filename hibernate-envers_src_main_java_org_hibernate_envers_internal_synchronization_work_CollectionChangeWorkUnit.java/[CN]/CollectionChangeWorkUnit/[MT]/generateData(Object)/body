{
  fillDataWithId(data,revisionData);
  final Map<String,Object> preGenerateData=new HashMap<String,Object>(data);
  enversService.getEntitiesConfigurations().get(getEntityName()).getPropertyMapper().mapToMapFromEntity(sessionImplementor,data,entity,null);
  enversService.getEntitiesConfigurations().get(getEntityName()).getPropertyMapper().mapModifiedFlagsToMapFromEntity(sessionImplementor,data,entity,entity);
  enversService.getEntitiesConfigurations().get(getEntityName()).getPropertyMapper().mapModifiedFlagsToMapForCollectionChange(collectionPropertyName,data);
  data.putAll(preGenerateData);
  return data;
}
