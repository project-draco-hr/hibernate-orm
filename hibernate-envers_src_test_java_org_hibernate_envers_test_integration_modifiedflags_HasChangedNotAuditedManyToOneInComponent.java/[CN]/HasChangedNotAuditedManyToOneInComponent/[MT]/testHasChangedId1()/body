{
  List list=queryForPropertyHasChanged(NotAuditedManyToOneComponentTestEntity.class,mtocte_id1,"comp1");
  assertEquals(2,list.size());
  assertEquals(TestTools.makeList(1,2),extractRevisionNumbers(list));
  list=queryForPropertyHasNotChanged(NotAuditedManyToOneComponentTestEntity.class,mtocte_id1,"comp1");
  assertEquals(0,list.size());
}
