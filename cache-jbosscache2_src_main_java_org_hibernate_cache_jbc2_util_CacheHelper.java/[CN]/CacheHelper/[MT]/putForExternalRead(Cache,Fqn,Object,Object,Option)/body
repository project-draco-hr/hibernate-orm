{
  try {
    setInvocationOption(cache,option);
    cache.putForExternalRead(new Fqn(region,key),ITEM,value);
    return true;
  }
 catch (  TimeoutException te) {
    log.debug("ignoring write lock acquisition failure");
    return false;
  }
catch (  Throwable t) {
    throw new CacheException(t);
  }
}
