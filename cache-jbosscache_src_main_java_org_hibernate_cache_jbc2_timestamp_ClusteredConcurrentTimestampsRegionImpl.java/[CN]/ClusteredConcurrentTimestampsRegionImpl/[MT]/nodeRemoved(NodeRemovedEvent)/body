{
  if (event.isOriginLocal() || event.isPre())   return;
  Fqn fqn=event.getFqn();
  Fqn regFqn=getRegionFqn();
  if (fqn.isChildOrEquals(regFqn)) {
    if (fqn.size() == regFqn.size()) {
      localCache.clear();
    }
 else {
      Object key=fqn.get(regFqn.size());
      localCache.remove(key);
    }
  }
}
