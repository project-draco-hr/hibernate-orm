{
  final String collectionTableAlias=aliases.getCollectionTableAlias();
  final CollectionPersister persister=rightHandSide.getCollectionPersister();
  final QueryableCollection queryableCollection=(QueryableCollection)persister;
  final String filterFragment=queryableCollection.filterFragment(collectionTableAlias,buildingParameters.getQueryInfluencers().getEnabledFilters());
  joinFragment.addJoin(queryableCollection.getTableName(),collectionTableAlias,aliasedLhsColumnNames,queryableCollection.getKeyColumnNames(),JoinType.LEFT_OUTER_JOIN,filterFragment);
  joinFragment.addJoins(queryableCollection.fromJoinFragment(collectionTableAlias,false,true),queryableCollection.whereJoinFragment(collectionTableAlias,false,true));
}
