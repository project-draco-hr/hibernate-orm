{
  if (isDriver && transactionCoordinator().getTransactionContext().shouldAutoClose() && !transactionCoordinator().getTransactionContext().isClosed()) {
    try {
      transactionCoordinator().getTransactionContext().managedClose();
    }
 catch (    HibernateException e) {
      log.info("Could not close session; swallowing exception as transaction completed",e);
    }
  }
}
