{
  List list=getAuditReader().createQuery().forRevisionsOfEntity(IdMapKeyEntity.class,false,false).add(AuditEntity.id().eq(imke_id)).add(AuditEntity.property("idmap").hasChanged()).getResultList();
  assertEquals(2,list.size());
  assertEquals(TestTools.makeList(1,2),extractRevisionNumbers(list));
  list=getAuditReader().createQuery().forRevisionsOfEntity(IdMapKeyEntity.class,false,false).add(AuditEntity.id().eq(imke_id)).add(AuditEntity.property("idmap").hasNotChanged()).getResultList();
  assertEquals(0,list.size());
}
