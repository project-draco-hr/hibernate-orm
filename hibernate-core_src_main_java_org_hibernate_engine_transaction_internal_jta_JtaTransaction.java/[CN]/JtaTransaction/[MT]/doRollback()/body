{
  try {
    if (isInitiator) {
      if (getLocalStatus() != LocalStatus.FAILED_COMMIT) {
        userTransaction.rollback();
        LOG.debug("Rolled back JTA UserTransaction");
      }
    }
 else {
      markRollbackOnly();
    }
  }
 catch (  Exception e) {
    throw new TransactionException("JTA rollback failed",e);
  }
}
