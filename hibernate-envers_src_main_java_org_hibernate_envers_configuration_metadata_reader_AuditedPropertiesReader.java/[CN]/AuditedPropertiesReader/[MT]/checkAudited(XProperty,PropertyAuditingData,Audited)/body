{
  Audited aud=(property.isAnnotationPresent(Audited.class)) ? (property.getAnnotation(Audited.class)) : allClassAudited;
  if (aud != null) {
    propertyData.setStore(aud.modStore());
    propertyData.setRelationTargetAuditMode(aud.targetAuditMode());
    propertyData.setUsingModifiedFlag(globalCfg.isOverrideUsingModifiedFlag() ? globalCfg.isUsingModifiedFlag() : aud.usingModifiedFlag());
    return true;
  }
 else {
    return false;
  }
}
