{
  StringBuilder buf=new StringBuilder();
  for (int i=0; i < getLength(); i++) {
    Projection proj=getProjection(i);
    if (proj.isGrouped()) {
      buf.append(proj.toGroupSqlString(criteria,criteriaQuery)).append(", ");
    }
  }
  if (buf.length() > 2)   buf.setLength(buf.length() - 2);
  return buf.toString();
}
