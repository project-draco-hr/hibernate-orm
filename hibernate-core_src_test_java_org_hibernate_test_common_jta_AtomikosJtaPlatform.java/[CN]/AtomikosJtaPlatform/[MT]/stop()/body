{
  if (dataSourceBean != null) {
    try {
      dataSourceBean.close();
    }
 catch (    Exception e) {
      LOG.debug("Error closing DataSourceBean",e);
    }
  }
  if (transactionManager != null) {
    try {
      transactionManager.close();
    }
 catch (    Exception e) {
      LOG.debug("Error closing UserTransactionManager",e);
    }
  }
}
