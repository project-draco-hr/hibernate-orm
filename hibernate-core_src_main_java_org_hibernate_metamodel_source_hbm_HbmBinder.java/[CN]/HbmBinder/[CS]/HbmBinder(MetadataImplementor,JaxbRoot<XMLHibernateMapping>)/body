{
  this.jaxbRoot=jaxbRoot;
  this.hibernateMapping=jaxbRoot.getRoot();
  this.metadata=metadata;
  this.mappingDefaults=new OverriddenMappingDefaults(metadata.getMappingDefaults(),hibernateMapping.getPackage(),hibernateMapping.getSchema(),hibernateMapping.getCatalog(),null,null,hibernateMapping.getDefaultCascade(),hibernateMapping.getDefaultAccess(),hibernateMapping.isDefaultLazy());
  autoImport=hibernateMapping.isAutoImport();
  metaAttributeContext=extractMetaAttributes();
}
