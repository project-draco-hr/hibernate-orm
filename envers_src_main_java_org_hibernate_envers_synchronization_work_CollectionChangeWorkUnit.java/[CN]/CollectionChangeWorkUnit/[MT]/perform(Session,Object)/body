{
  Map<String,Object> data=new HashMap<String,Object>();
  fillDataWithId(data,revisionData,RevisionType.MOD);
  verCfg.getEntCfg().get(getEntityName()).getPropertyMapper().mapToMapFromEntity(sessionImplementor,data,entity,null);
  session.save(verCfg.getAuditEntCfg().getAuditEntityName(getEntityName()),data);
  setPerformed(data);
}
