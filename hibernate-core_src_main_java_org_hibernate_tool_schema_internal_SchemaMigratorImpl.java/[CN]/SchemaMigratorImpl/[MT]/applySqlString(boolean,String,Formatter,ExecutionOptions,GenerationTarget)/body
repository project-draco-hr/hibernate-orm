{
  if (StringHelper.isEmpty(sqlString)) {
    return;
  }
  for (  GenerationTarget target : targets) {
    try {
      target.accept(formatter.format(sqlString));
    }
 catch (    CommandAcceptanceException e) {
      if (!quiet) {
        options.getExceptionHandler().handleException(e);
      }
    }
  }
}
