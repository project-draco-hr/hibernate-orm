{
  Update update=new Update(getDialect()).setTableName(qualifiedTableName).addColumns(keyColumnNames);
  if (hasIndex && !indexContainsFormula)   update.addColumns(indexColumnNames);
  if (getFactory().getSettings().isCommentsEnabled()) {
    update.setComment("create one-to-many row " + getRole());
  }
  return update.setPrimaryKeyColumnNames(elementColumnNames).toStatementString();
}
