{
  Coordinate coordinate;
  if (hasMValues()) {
    coordinate=new MCoordinate();
    ((MCoordinate)coordinate).m=mValues[index];
  }
 else {
    coordinate=new Coordinate();
  }
  coordinate.x=points[2 * index];
  coordinate.y=points[2 * index + 1];
  if (hasZValues()) {
    coordinate.z=zValues[index];
  }
  return coordinate;
}
