{
  Session s=openSession();
  Baz baz=new Baz();
  List list=new ArrayList();
  baz.setBazez(list);
  list.add(new Baz());
  s.save(baz);
  s.flush();
  list.add(new Baz());
  s.flush();
  list.add(0,new Baz());
  s.flush();
  s.delete(list.remove(1));
  s.flush();
  s.delete(baz);
  s.flush();
  s.connection().commit();
  s.close();
}
