{
  Session s=openSession();
  s.beginTransaction();
  Foo foo=new Foo();
  s.save(foo);
  s.getTransaction().commit();
  s.close();
  s=openSession();
  s.beginTransaction();
  Foo foo2=new Foo();
  s.load(foo2,foo.getKey());
  assertTrue("create",foo.equalsFoo(foo2));
  s.delete(foo2);
  s.getTransaction().commit();
  s.close();
}
