{
  Session s=openSession();
  GlarchProxy g=new Glarch();
  Foo foo=new Foo();
  g.setAny(foo);
  Serializable gid=s.save(g);
  s.save(foo);
  s.flush();
  s.connection().commit();
  s.close();
  s=openSession();
  g=(GlarchProxy)s.load(Glarch.class,gid);
  assertTrue(g.getAny() != null && g.getAny() instanceof FooProxy);
  s.delete(g.getAny());
  s.delete(g);
  s.flush();
  s.connection().commit();
  s.close();
}
