{
  Session s=openSession();
  s.beginTransaction();
  ParamCheckingEntity e=new ParamCheckingEntity();
  e.setId(Long.valueOf(1));
  e.setName("dummy");
  s.update(e);
  try {
    s.flush();
    fail("expection flush failure!");
  }
 catch (  HibernateException ex) {
  }
  s.clear();
  s.getTransaction().commit();
  s.close();
}
