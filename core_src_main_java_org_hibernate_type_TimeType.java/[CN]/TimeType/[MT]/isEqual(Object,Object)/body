{
  if (x == y)   return true;
  if (x == null || y == null)   return false;
  Date xdate=(Date)x;
  Date ydate=(Date)y;
  if (xdate.getTime() == ydate.getTime())   return true;
  Calendar calendar1=java.util.Calendar.getInstance();
  Calendar calendar2=java.util.Calendar.getInstance();
  calendar1.setTime(xdate);
  calendar2.setTime(ydate);
  return calendar1.get(Calendar.HOUR_OF_DAY) == calendar2.get(Calendar.HOUR_OF_DAY) && calendar1.get(Calendar.MINUTE) == calendar2.get(Calendar.MINUTE) && calendar1.get(Calendar.SECOND) == calendar2.get(Calendar.SECOND) && calendar1.get(Calendar.MILLISECOND) == calendar2.get(Calendar.MILLISECOND);
}
