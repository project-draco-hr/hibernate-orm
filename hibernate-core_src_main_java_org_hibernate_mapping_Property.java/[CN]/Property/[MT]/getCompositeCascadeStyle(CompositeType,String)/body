{
  if (compositeType.isAnyType()) {
    return getCascadeStyle(cascade);
  }
  int length=compositeType.getSubtypes().length;
  for (int i=0; i < length; i++) {
    if (compositeType.getCascadeStyle(i) != CascadeStyle.NONE) {
      return CascadeStyle.ALL;
    }
  }
  return getCascadeStyle(cascade);
}
