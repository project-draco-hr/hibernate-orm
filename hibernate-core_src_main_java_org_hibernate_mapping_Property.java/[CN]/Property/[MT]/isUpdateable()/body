{
  final boolean[] columnUpdateability=value.getColumnUpdateability();
  final boolean isImmutableNaturalId=isNaturalIdentifier() && (NaturalIdMutability.IMMUTABLE.equals(getNaturalIdMutability()) || NaturalIdMutability.IMMUTABLE_CHECKED.equals(getNaturalIdMutability()));
  return updateable && !isImmutableNaturalId && !ArrayHelper.isAllFalse(columnUpdateability);
}
