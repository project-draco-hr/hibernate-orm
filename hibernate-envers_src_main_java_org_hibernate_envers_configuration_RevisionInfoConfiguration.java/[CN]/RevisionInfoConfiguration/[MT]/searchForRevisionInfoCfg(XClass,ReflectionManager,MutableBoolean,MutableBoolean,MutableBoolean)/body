{
  XClass superclazz=clazz.getSuperclass();
  if (!"java.lang.Object".equals(superclazz.getName())) {
    searchForRevisionInfoCfg(superclazz,reflectionManager,revisionNumberFound,revisionTimestampFound,modifiedEntityTypesFound);
  }
  searchForRevisionInfoCfgInProperties(clazz,reflectionManager,revisionNumberFound,revisionTimestampFound,modifiedEntityTypesFound,"field");
  searchForRevisionInfoCfgInProperties(clazz,reflectionManager,revisionNumberFound,revisionTimestampFound,modifiedEntityTypesFound,"property");
}
