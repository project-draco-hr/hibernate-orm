{
  Session session=openSession();
  session.getTransaction().begin();
  Person p=new Person();
  Name n=new Name();
  n.setName("name1");
  p.getNames().add(n);
  session.saveOrUpdate(p);
  session.getTransaction().commit();
  session.getTransaction().begin();
  n.setName("Changed name");
  session.saveOrUpdate(p);
  session.getTransaction().commit();
  session.getTransaction().begin();
  Name n2=new Name();
  n2.setName("name2");
  p.getNames().add(n2);
  session.getTransaction().commit();
  personId=p.getId();
}
