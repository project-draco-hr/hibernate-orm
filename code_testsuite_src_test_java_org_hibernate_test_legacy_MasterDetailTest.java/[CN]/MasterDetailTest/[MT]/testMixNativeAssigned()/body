{
  Session s=openSession();
  Category c=new Category();
  c.setName("NAME");
  Assignable assn=new Assignable();
  assn.setId("i.d.");
  List l=new ArrayList();
  l.add(c);
  assn.setCategories(l);
  c.setAssignable(assn);
  s.save(assn);
  s.flush();
  s.connection().commit();
  s.close();
  s=openSession();
  s.delete(assn);
  s.flush();
  s.connection().commit();
  s.close();
}
