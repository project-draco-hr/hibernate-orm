{
  Session s=openSession();
  W w1=new W();
  W w2=new W();
  Z z=new Z();
  z.setW(w1);
  s.save(z);
  s.flush();
  z.setW(w2);
  s.flush();
  s.connection().commit();
  s.close();
  s=openSession();
  s.update(z);
  s.flush();
  s.delete(z);
  s.delete("from W");
  s.flush();
  s.connection().commit();
  s.close();
}
