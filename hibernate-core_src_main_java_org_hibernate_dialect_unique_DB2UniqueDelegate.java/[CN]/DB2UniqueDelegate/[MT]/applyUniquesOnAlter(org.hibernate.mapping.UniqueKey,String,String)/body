{
  if (hasNullable(uniqueKey)) {
    return org.hibernate.mapping.Index.buildSqlCreateIndexString(dialect,uniqueKey.getName(),uniqueKey.getTable(),uniqueKey.columnIterator(),uniqueKey.getColumnOrderMap(),true,defaultCatalog,defaultSchema);
  }
 else {
    return super.applyUniquesOnAlter(uniqueKey,defaultCatalog,defaultSchema);
  }
}
