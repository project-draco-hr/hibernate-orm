{
  this.manyToOneElement=manyToOneElement;
  this.valueSources=buildValueSources(new ValueSourcesAdapter(){
    @Override public String getColumnAttribute(){
      return manyToOneElement.getColumn();
    }
    @Override public String getFormulaAttribute(){
      return manyToOneElement.getFormula();
    }
    @Override public List getColumnOrFormulaElements(){
      return manyToOneElement.getColumnOrFormula();
    }
  }
);
}
