{
  SecondaryNamingTestEntity ste=new SecondaryNamingTestEntity("a","1");
  EntityManager em=getEntityManager();
  em.getTransaction().begin();
  em.persist(ste);
  em.getTransaction().commit();
  em.getTransaction().begin();
  ste=em.find(SecondaryNamingTestEntity.class,ste.getId());
  ste.setS1("b");
  ste.setS2("2");
  em.getTransaction().commit();
  id=ste.getId();
}
