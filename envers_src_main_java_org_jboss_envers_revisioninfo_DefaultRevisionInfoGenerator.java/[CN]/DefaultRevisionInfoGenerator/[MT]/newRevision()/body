{
  Object revisionInfo;
  try {
    revisionInfo=revisionInfoClass.newInstance();
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
  revisionTimestampSetter.set(revisionInfo,System.currentTimeMillis(),null);
  if (listener != null) {
    listener.newRevision(revisionInfo);
  }
  return revisionInfo;
}
