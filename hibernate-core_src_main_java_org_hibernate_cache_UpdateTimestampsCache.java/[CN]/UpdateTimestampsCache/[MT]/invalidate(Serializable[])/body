{
  readWriteLock.writeLock().lock();
  try {
    Long ts=new Long(region.nextTimestamp());
    for (    Serializable space : spaces) {
      LOG.debugf("Invalidating space [%s], timestamp: %s",space,ts);
      region.put(space,ts);
    }
  }
  finally {
    readWriteLock.writeLock().unlock();
  }
}
