{
  if (!started) {
    throw new HibernateException("not yet started!");
  }
  started=false;
  rolledback=false;
  committed=false;
  try {
    DummyTransactionManager.INSTANCE.rollback();
    rolledback=true;
  }
 catch (  Throwable t) {
    throw new HibernateException("error on rollback()",t);
  }
}
