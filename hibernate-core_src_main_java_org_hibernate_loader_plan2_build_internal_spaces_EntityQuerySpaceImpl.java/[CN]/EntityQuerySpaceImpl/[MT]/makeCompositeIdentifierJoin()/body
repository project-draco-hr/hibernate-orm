{
  final String compositeQuerySpaceUid=getUid() + "-id";
  final CompositeQuerySpaceImpl rhs=new CompositeQuerySpaceImpl(this,new CompositePropertyMapping((CompositeType)getEntityPersister().getIdentifierType(),(PropertyMapping)getEntityPersister(),getEntityPersister().getIdentifierPropertyName()),compositeQuerySpaceUid);
  getQuerySpaces().registerQuerySpace(rhs);
  final JoinImpl join=new JoinImpl(this,"id",rhs,null,false);
  internalGetJoins().add(join);
  return join;
}
