{
  fillDataWithId(data,revisionData);
  verCfg.getEntCfg().get(getEntityName()).getPropertyMapper().mapToMapFromEntity(sessionImplementor,data,entity,null);
  verCfg.getEntCfg().get(getEntityName()).getPropertyMapper().mapModifiedFlagsToMapFromEntity(sessionImplementor,data,entity,entity);
  verCfg.getEntCfg().get(getEntityName()).getPropertyMapper().mapModifiedFlagsToMapForCollectionChange(collectionPropertyName,data);
  return data;
}
