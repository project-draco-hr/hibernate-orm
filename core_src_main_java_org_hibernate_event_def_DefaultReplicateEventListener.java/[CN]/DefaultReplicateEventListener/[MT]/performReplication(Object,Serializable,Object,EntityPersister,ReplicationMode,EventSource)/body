{
  if (log.isTraceEnabled()) {
    log.trace("replicating changes to " + MessageHelper.infoString(persister,id,source.getFactory()));
  }
  new OnReplicateVisitor(source,id,entity,true).process(entity,persister);
  source.getPersistenceContext().addEntity(entity,Status.MANAGED,null,new EntityKey(id,persister,source.getEntityMode()),version,LockMode.NONE,true,persister,true,false);
  cascadeAfterReplicate(entity,persister,replicationMode,source);
}
