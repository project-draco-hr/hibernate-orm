{
  if (getColumnIterator().hasNext()) {
    return getMappings().getTypeResolver().getTypeFactory().specialOneToOne(getReferencedEntityName(),foreignKeyType,referenceToPrimaryKey,referencedPropertyName,isLazy(),isUnwrapProxy(),entityName,propertyName);
  }
 else {
    return getMappings().getTypeResolver().getTypeFactory().oneToOne(getReferencedEntityName(),foreignKeyType,referenceToPrimaryKey,referencedPropertyName,isLazy(),isUnwrapProxy(),entityName,propertyName);
  }
}
