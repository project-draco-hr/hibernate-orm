{
  Type[] idTypes=new Type[ids.length];
  Arrays.fill(idTypes,type);
  try {
    doQueryAndInitializeNonLazyCollections(session,new QueryParameters(parameterTypes,parameterValues,namedParameters,ids),true);
  }
 catch (  SQLException sqle) {
    throw JDBCExceptionHelper.convert(factory.getSQLExceptionConverter(),sqle,"could not load collection by subselect: " + MessageHelper.collectionInfoString(getCollectionPersisters()[0],ids,getFactory()),getSQLString());
  }
}
