{
  Map result=locks;
  if (result == null) {
    LockOptions[] lockArray=getLockOptions(result);
    String[] aliases=getAliases();
    if (aliases != null && lockArray != null && lockArray.length > 0 && lockArray.length == aliases.length && lockArray[0].getLockMode() != LockMode.NONE) {
      result=new HashMap();
      for (int looper=0; looper < lockArray.length; looper++) {
        result.put(aliases[looper],lockArray[looper]);
      }
    }
  }
  return result;
}
