{
  String getterName="get" + capitalizeFirst(fieldName);
  StringBuffer sb=new StringBuffer();
  sb.append("public ").append(fieldClass.getName()).append(" ").append(getterName).append("(){").append("return (" + fieldClass.getName() + ")this.theMap.get(\"").append(fieldName).append("\")").append(";").append("}");
  return CtMethod.make(sb.toString(),declaringClass);
}
