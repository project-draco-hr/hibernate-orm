{
  Coordinate[] ar=this.getCoordinates();
  StringBuffer buf=new StringBuffer(ar.length * 17 * 3);
  for (int i=0; i < ar.length; i++) {
    buf.append(ar[i].x);
    buf.append(" ");
    buf.append(ar[i].y);
    buf.append(" ");
    buf.append(((MCoordinate)ar[i]).m);
    buf.append("\n");
  }
  return buf.toString();
}
