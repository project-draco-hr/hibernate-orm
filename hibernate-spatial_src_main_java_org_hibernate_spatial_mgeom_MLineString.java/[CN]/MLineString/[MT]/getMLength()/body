{
  if (getCoordinateSequence().size() == 0)   return Double.NaN;
  if (getCoordinateSequence().size() == 1)   return 0.0D;
 else {
    int lastIndex=getCoordinateSequence().size() - 1;
    double begin=getCoordinateSequence().getOrdinate(0,CoordinateSequence.M);
    double end=getCoordinateSequence().getOrdinate(lastIndex,CoordinateSequence.M);
    return (Double.isNaN(begin) || Double.isNaN(end)) ? Double.NaN : Math.abs(end - begin);
  }
}
