{
  if (lockOptions != null) {
    return dialect.getForUpdateString(aliases.toString(),lockOptions);
  }
 else   if (aliases.length() == 0) {
    if (lockMode != null) {
      return dialect.getForUpdateString(lockMode);
    }
    return "";
  }
  return isNowaitEnabled ? dialect.getForUpdateNowaitString(aliases.toString()) : dialect.getForUpdateString(aliases.toString());
}
