{
  if (x == y)   return true;
  if (x == null || y == null)   return false;
  if (x instanceof Character[]) {
    Object[] o1=(Object[])x;
    Object[] o2=(Object[])y;
    return ArrayHelper.isEquals(o1,o2);
  }
 else {
    char[] c1=(char[])x;
    char[] c2=(char[])y;
    return ArrayHelper.isEquals(c1,c2);
  }
}
