{
  Customer customer=null;
  beginTx();
  try {
    customer=new Customer();
    customer.setName("customer_" + nameSuffix);
    customer.setContacts(new HashSet<Contact>());
    sessionFactory().getCurrentSession().persist(customer);
  }
 catch (  Exception e) {
    setRollbackOnlyTx(e);
  }
 finally {
    commitOrRollbackTx();
  }
  return customer;
}
