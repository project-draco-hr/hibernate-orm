{
  if (original == null) {
    return null;
  }
  final Object result=target == null ? instantiate(owner,session) : target;
  Object[] values=TypeHelper.replace(getPropertyValues(original,entityMode),getPropertyValues(result,entityMode),propertyNames,propertyTypes,session,result,copyCache,foreignKeyDirection);
  setPropertyValues(result,values,entityMode);
  return result;
}
