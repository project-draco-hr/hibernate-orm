{
  if (x == y) {
    return true;
  }
  if (x == null || y == null) {
    return false;
  }
  Object[] xvalues=getPropertyValues(x,entityMode);
  Object[] yvalues=getPropertyValues(y,entityMode);
  for (int i=0; i < propertySpan; i++) {
    if (!propertyTypes[i].isSame(xvalues[i],yvalues[i])) {
      return false;
    }
  }
  return true;
}
