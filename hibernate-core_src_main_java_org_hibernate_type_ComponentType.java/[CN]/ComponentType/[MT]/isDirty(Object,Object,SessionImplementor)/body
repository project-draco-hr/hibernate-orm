{
  if (x == y) {
    return false;
  }
  if (x == null || y == null) {
    return true;
  }
  Object[] xvalues=getPropertyValues(x,entityMode);
  Object[] yvalues=getPropertyValues(y,entityMode);
  for (int i=0; i < xvalues.length; i++) {
    if (propertyTypes[i].isDirty(xvalues[i],yvalues[i],session)) {
      return true;
    }
  }
  return false;
}
