{
  this.rootReturnReader=rootReturnReader;
  this.entityReferenceReaders=entityReferenceReaders != null ? entityReferenceReaders : Collections.<EntityReferenceReader>emptyList();
  this.collectionReferenceReaders=collectionReferenceReaders != null ? collectionReferenceReaders : Collections.<CollectionReferenceReader>emptyList();
}
