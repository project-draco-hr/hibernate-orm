{
  Session s=openSession();
  s.beginTransaction();
  try {
    insertUser("q@s.com",21,false,"a1","b");
    insertUser("r@s.com",22,false,"a2","b");
    insertUser("s@s.com",23,true,"a3","b");
    insertUser("t@s.com",24,false,"a4","b");
    session.flush();
    s.getTransaction().commit();
  }
 catch (  Exception e) {
    s.getTransaction().rollback();
  }
}
