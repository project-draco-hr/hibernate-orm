{
  Set<String> lines=new HashSet<String>();
  String line;
  BufferedReader reader=new BufferedReader(new InputStreamReader(is));
  while ((line=reader.readLine()) != null) {
    line=line.trim();
    Matcher m=nonCommentPattern.matcher(line);
    if (m.find()) {
      lines.add(m.group().trim());
    }
  }
  return lines;
}
