{
  super(sessionFactory,alias,lockMode,sessionFactory.getCollectionPersister(fetchOwner.retrieveFetchSourcePersister().getEntityName() + '.' + ownerProperty),fetchOwner.getPropertyPath().append(ownerProperty),collectionAliases,elementEntityAliases);
  this.fetchOwner=fetchOwner;
  this.fetchStrategy=fetchStrategy;
  fetchOwner.addFetch(this);
}
