{
  if (log.isTraceEnabled())   log.trace("Inserting: " + key);
  try {
    cache.lock(key);
    Lockable lockable=(Lockable)cache.get(key);
    if (lockable == null) {
      cache.update(key,new Item(value,version,cache.nextTimestamp()));
      if (log.isTraceEnabled())       log.trace("Inserted: " + key);
      return true;
    }
 else {
      return false;
    }
  }
  finally {
    cache.unlock(key);
  }
}
