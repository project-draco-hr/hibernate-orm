{
  LOG.lookup(key);
  Lockable lockable=(Lockable)cache.get(key);
  boolean gettable=lockable != null && lockable.isGettable(txTimestamp);
  if (gettable) {
    LOG.hit(key);
    return ((Item)lockable).getValue();
  }
  if (lockable == null)   LOG.miss(key);
 else   LOG.locked(key);
  return null;
}
