{
  final CollectionReferenceAliasesImpl aliases=new CollectionReferenceAliasesImpl(createTableAlias(persister.getRole()),persister.isManyToMany() ? createTableAlias(persister.getRole()) : null,createCollectionAliases(persister),createCollectionElementAliases(persister));
  registerQuerySpaceAliases(uid,aliases);
  return aliases;
}
