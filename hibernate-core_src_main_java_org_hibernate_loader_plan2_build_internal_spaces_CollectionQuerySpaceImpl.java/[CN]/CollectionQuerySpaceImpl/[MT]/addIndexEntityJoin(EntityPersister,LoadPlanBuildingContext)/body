{
  final String entityQuerySpaceUid=getQuerySpaces().generateImplicitUid();
  final EntityQuerySpaceImpl entityQuerySpace=new EntityQuerySpaceImpl(indexPersister,entityQuerySpaceUid,getQuerySpaces(),sessionFactory());
  getQuerySpaces().registerQuerySpace(entityQuerySpace);
  final JoinImpl join=new JoinImpl(this,"index",entityQuerySpace,((Queryable)indexPersister).getKeyColumnNames(),false);
  internalGetJoins().add(join);
  return join;
}
