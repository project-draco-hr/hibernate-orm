{
  final String entityQuerySpaceUid=getExpandingQuerySpaces().generateImplicitUid();
  final ExpandingEntityQuerySpace entityQuerySpace=getExpandingQuerySpaces().makeEntityQuerySpace(entityQuerySpaceUid,elementPersister,canJoinsBeRequired());
  final JoinDefinedByMetadata join=JoinHelper.INSTANCE.createEntityJoin(this,CollectionPropertyNames.COLLECTION_ELEMENTS,entityQuerySpace,canJoinsBeRequired(),(EntityType)persister.getElementType(),sessionFactory());
  internalGetJoins().add(join);
  return entityQuerySpace;
}
