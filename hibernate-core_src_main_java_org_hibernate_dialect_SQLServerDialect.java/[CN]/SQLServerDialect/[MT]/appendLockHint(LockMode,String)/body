{
  if ((mode == LockMode.UPGRADE) || (mode == LockMode.UPGRADE_NOWAIT) || (mode == LockMode.PESSIMISTIC_WRITE)|| (mode == LockMode.WRITE)) {
    return tableName + " with (updlock, rowlock)";
  }
 else   if (mode == LockMode.PESSIMISTIC_READ) {
    return tableName + " with (holdlock, rowlock)";
  }
 else {
    return tableName;
  }
}
