{
  Session s=openSession();
  s.beginTransaction();
  ArrayList<String> params=new ArrayList<String>();
  params.add("Doe");
  params.add("Public");
  s.createQuery("from Human where name.last in (?1)").setParameterList("1",params).list();
  s.createQuery("from Human where name.last in ?1").setParameterList("1",params).list();
  s.getTransaction().commit();
  s.close();
}
