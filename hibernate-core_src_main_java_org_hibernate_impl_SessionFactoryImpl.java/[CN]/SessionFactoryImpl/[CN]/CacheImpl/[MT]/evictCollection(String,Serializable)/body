{
  CollectionPersister p=getCollectionPersister(role);
  if (p.hasCache()) {
    if (LOG.isDebugEnabled())     LOG.debug("Evicting second-level cache: " + MessageHelper.collectionInfoString(p,ownerIdentifier,SessionFactoryImpl.this));
    CacheKey cacheKey=buildCacheKey(ownerIdentifier,p);
    p.getCacheAccessStrategy().evict(cacheKey);
  }
}
