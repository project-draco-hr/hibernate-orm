{
  prepareForPossibleLoadingOutsideTransaction();
  if (currentSession == this.session) {
    this.session=null;
    return true;
  }
 else {
    if (this.session != null) {
      LOG.logCannotUnsetUnexpectedSessionInCollection(generateUnexpectedSessionStateMessage(currentSession));
    }
    return false;
  }
}
