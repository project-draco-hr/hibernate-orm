{
  if (hasQueuedOperations()) {
    return new Iterator(){
      int i=0;
      public Object next(){
        return operationQueue.get(i++).getAddedInstance();
      }
      public boolean hasNext(){
        return i < operationQueue.size();
      }
      public void remove(){
        throw new UnsupportedOperationException();
      }
    }
;
  }
 else {
    return EmptyIterator.INSTANCE;
  }
}
