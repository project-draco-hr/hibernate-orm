{
  if (isEarlyInsert()) {
    getSession().getPersistenceContext().getNaturalIdHelper().manageLocalNaturalIdCrossReference(getPersister(),generatedId,state,null,CachedNaturalIdValueSource.INSERT);
  }
  getSession().getPersistenceContext().getNaturalIdHelper().manageSharedNaturalIdCrossReference(getPersister(),generatedId,state,null,CachedNaturalIdValueSource.INSERT);
}
