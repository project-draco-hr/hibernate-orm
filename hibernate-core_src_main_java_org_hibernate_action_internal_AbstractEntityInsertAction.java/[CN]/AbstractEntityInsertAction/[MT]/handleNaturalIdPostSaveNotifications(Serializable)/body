{
  if (isEarlyInsert()) {
    getSession().getPersistenceContext().getNaturalIdHelper().manageLocalNaturalIdCrossReference(getPersister(),id,state,null,CachedNaturalIdValueSource.INSERT);
  }
  getSession().getPersistenceContext().getNaturalIdHelper().manageSharedNaturalIdCrossReference(getPersister(),getId(),state,null,CachedNaturalIdValueSource.INSERT);
}
