{
  entityManager().checkOpen(true);
  try {
    registerParameter(new ParameterRegistrationImpl(this,(ParameterRegistrationImplementor)procedureCall.registerParameter(parameterName,type,mode)));
  }
 catch (  HibernateException he) {
    throw entityManager().convert(he);
  }
catch (  RuntimeException e) {
    entityManager().markForRollbackOnly();
    throw e;
  }
  return this;
}
