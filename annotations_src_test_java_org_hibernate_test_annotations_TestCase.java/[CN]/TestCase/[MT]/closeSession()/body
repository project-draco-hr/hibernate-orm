{
  try {
    if (session != null && session.isOpen()) {
      if (session.isConnected()) {
        session.doWork(new RollbackWork());
      }
      session.close();
    }
  }
 catch (  Exception ignore) {
  }
  try {
    if (sessions != null) {
      sessions.close();
      sessions=null;
    }
  }
 catch (  Exception ignore) {
  }
}
