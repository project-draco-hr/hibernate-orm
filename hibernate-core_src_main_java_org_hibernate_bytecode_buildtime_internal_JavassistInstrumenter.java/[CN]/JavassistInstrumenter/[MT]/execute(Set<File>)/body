{
  ClassPool cp=ClassPool.getDefault();
  cp.insertClassPath(new ClassClassPath(this.getClass()));
  try {
    for (    File file : files) {
      cp.makeClass(new FileInputStream(file));
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e.getMessage(),e);
  }
  super.execute(files);
}
