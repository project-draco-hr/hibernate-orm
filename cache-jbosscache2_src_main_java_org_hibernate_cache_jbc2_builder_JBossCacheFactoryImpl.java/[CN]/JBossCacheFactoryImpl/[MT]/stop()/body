{
  if (started) {
synchronized (caches) {
      for (Iterator it=caches.entrySet().iterator(); it.hasNext(); ) {
        Map.Entry entry=(Entry)it.next();
        destroyCache((Cache)entry.getValue());
        it.remove();
      }
      caches.clear();
      checkouts.clear();
      configs.clear();
    }
    started=false;
  }
}
