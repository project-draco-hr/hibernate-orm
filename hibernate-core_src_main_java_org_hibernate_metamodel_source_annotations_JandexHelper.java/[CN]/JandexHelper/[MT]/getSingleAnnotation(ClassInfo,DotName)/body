{
  List<AnnotationInstance> annotationList=classInfo.annotations().get(annotationName);
  if (annotationList == null) {
    return null;
  }
 else   if (annotationList.size() == 1) {
    return annotationList.get(0);
  }
 else {
    throw new AssertionFailure("There should be only one annotation of type " + annotationName + " defined on"+ classInfo.name());
  }
}
