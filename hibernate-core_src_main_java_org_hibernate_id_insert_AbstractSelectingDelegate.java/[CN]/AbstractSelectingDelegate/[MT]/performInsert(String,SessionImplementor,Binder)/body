{
  try {
    PreparedStatement insert=session.getTransactionCoordinator().getJdbcCoordinator().getStatementPreparer().prepareStatement(insertSQL,PreparedStatement.NO_GENERATED_KEYS);
    try {
      binder.bindValues(insert);
      insert.executeUpdate();
    }
  finally {
      insert.close();
    }
  }
 catch (  SQLException sqle) {
    throw session.getFactory().getSQLExceptionHelper().convert(sqle,"could not insert: " + MessageHelper.infoString(persister),insertSQL);
  }
  final String selectSQL=getSelectSQL();
  try {
    PreparedStatement idSelect=session.getTransactionCoordinator().getJdbcCoordinator().getStatementPreparer().prepareStatement(selectSQL,false);
    try {
      bindParameters(session,idSelect,binder.getEntity());
      ResultSet rs=idSelect.executeQuery();
      try {
        return getResult(session,rs,binder.getEntity());
      }
  finally {
        rs.close();
      }
    }
  finally {
      idSelect.close();
    }
  }
 catch (  SQLException sqle) {
    throw session.getFactory().getSQLExceptionHelper().convert(sqle,"could not retrieve generated id after insert: " + MessageHelper.infoString(persister),insertSQL);
  }
}
