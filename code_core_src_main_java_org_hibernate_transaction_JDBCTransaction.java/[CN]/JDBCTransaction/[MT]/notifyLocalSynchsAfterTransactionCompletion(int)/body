{
  begun=false;
  if (synchronizations != null) {
    for (int i=0; i < synchronizations.size(); i++) {
      Synchronization sync=(Synchronization)synchronizations.get(i);
      try {
        sync.afterCompletion(status);
      }
 catch (      Throwable t) {
        log.error("exception calling user Synchronization",t);
      }
    }
  }
}
