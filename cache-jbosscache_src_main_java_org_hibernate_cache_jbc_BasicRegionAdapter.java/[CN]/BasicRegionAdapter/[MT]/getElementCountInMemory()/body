{
  if (checkValid()) {
    try {
      Set childrenNames=CacheHelper.getChildrenNames(jbcCache,regionFqn);
      int size=childrenNames.size();
      if (childrenNames.contains(CacheHelper.Internal.NODE)) {
        size--;
      }
      return size;
    }
 catch (    CacheException ce) {
      throw ce;
    }
catch (    Exception e) {
      throw new CacheException(e);
    }
  }
 else {
    return 0;
  }
}
