{
  if (!CollectionHelper.isEmpty(aliasTableMap)) {
    return aliasTableMap;
  }
 else   if (persistentClass != null) {
    String table=persistentClass.getTable().getQualifiedName(factory.getDialect(),factory.getSettings().getDefaultCatalogName(),factory.getSettings().getDefaultSchemaName());
    return Collections.singletonMap(null,table);
  }
 else {
    return Collections.emptyMap();
  }
}
