{
  cls=getTargetClassIfProxied(cls);
  checkNotNull(cls,"Entity class");
  checkNotNull(entityName,"Entity name");
  checkNotNull(primaryKey,"Primary key");
  checkSession();
  if (!enversService.getEntitiesConfigurations().isVersioned(entityName)) {
    throw new NotAuditedException(entityName,entityName + " is not versioned!");
  }
  return createQuery().forRevisionsOfEntity(cls,entityName,false,true).addProjection(AuditEntity.revisionNumber()).addOrder(AuditEntity.revisionNumber().asc()).add(AuditEntity.id().eq(primaryKey)).getResultList();
}
