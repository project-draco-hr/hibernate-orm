{
switch (accessType) {
case READ_ONLY:
    return new ReadOnlyAccess(this);
case TRANSACTIONAL:
  if (getCacheDataDescription().isMutable()) {
    return new TransactionalAccess(this);
  }
 else {
    return new ReadOnlyAccess(this);
  }
default :
throw new CacheException("Unsupported access type [" + accessType.getExternalName() + "]");
}
}
