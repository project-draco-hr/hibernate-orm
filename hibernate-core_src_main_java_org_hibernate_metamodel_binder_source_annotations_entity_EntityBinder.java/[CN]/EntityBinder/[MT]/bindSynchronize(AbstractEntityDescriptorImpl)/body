{
  AnnotationInstance synchronizeAnnotation=JandexHelper.getSingleAnnotation(entityClass.getClassInfo(),HibernateDotNames.SYNCHRONIZE);
  if (synchronizeAnnotation != null) {
    String[] tableNames=synchronizeAnnotation.value().asStringArray();
    for (    String tableName : tableNames) {
      entityBindingState.addSynchronizedTableName(tableName);
    }
  }
}
