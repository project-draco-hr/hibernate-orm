{
  if (!appliesTo(Dialect.getDialect()))   return;
  Throwable exception=null;
  setUp();
  try {
    runTest();
  }
 catch (  Throwable running) {
    exception=running;
  }
 finally {
    try {
      tearDown();
    }
 catch (    Throwable tearingDown) {
      if (exception == null)       exception=tearingDown;
    }
  }
  if (exception != null)   throw exception;
}
