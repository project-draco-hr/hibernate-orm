{
  this.resultingEntityKeys=resultingEntityKeys;
  this.queryParameters=queryParameters;
  this.namedParameterLocMap=namedParameterLocMap;
  this.loadable=loadable;
  this.alias=alias;
  final String queryString=queryParameters.getFilteredSQL();
  final int fromIndex=getFromIndex(queryString);
  final int orderByIndex=queryString.lastIndexOf("order by");
  this.queryString=orderByIndex > 0 ? queryString.substring(fromIndex,orderByIndex) : queryString.substring(fromIndex);
  if (LOG.isTraceEnabled()) {
    LOG.tracef("Generated SubselectFetch query: %s",queryString);
  }
}
