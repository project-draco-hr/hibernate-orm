{
  Type resolvedHibernateType=determineSingularTypeFromDescriptor(basicCollectionElement.getHibernateTypeDescriptor());
  if (resolvedHibernateType != null) {
    pushHibernateTypeInformationDownIfNeeded(basicCollectionElement.getHibernateTypeDescriptor(),basicCollectionElement.getElementValue(),resolvedHibernateType);
  }
}
