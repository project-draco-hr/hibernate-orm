{
  super.addSubEntityDescriptor(subEntityDescriptor);
  final InheritanceType inheritanceType=Helper.interpretInheritanceType(subEntityDescriptor.getEntitySourceInformation().getEntityElement());
  if (this.hierarchyInheritanceType != InheritanceType.NO_INHERITANCE && this.hierarchyInheritanceType != inheritanceType) {
  }
  this.hierarchyInheritanceType=inheritanceType;
}
