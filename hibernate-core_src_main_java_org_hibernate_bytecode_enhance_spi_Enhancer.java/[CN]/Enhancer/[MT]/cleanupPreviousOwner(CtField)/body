{
  StringBuilder builder=new StringBuilder();
  builder.append("if (").append(currentValue.getName()).append(" != null) ").append("((org.hibernate.engine.spi.CompositeTracker)").append(currentValue.getName()).append(").").append(EnhancerConstants.TRACKER_COMPOSITE_CLEAR_OWNER).append("(\"").append(currentValue.getName()).append("\");");
