{
  return (updates != null && !updates.isEmpty()) || (insertions != null && !insertions.isEmpty()) || hasUnresolvedEntityInsertActions()|| (deletions != null && !deletions.isEmpty())|| (collectionUpdates != null && !collectionUpdates.isEmpty())|| (collectionQueuedOps != null && !collectionQueuedOps.isEmpty())|| (collectionRemovals != null && !collectionRemovals.isEmpty())|| (collectionCreations != null && !collectionCreations.isEmpty());
}
