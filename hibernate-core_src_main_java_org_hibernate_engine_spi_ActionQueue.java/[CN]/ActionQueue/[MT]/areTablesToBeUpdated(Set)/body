{
  if (tables.isEmpty()) {
    return false;
  }
  for (int i=0; i < EXECUTABLE_LISTS.length; ++i) {
    ExecutableList<?> l=EXECUTABLE_LISTS[i].get(this);
    if (areTablesToBeUpdated(l,tables)) {
      return true;
    }
  }
  if (unresolvedInsertions == null) {
    return false;
  }
  return areTablesToBeUpdated(unresolvedInsertions,tables);
}
