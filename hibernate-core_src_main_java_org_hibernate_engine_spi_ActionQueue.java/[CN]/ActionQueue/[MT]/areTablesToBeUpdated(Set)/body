{
  return areTablesToUpdated(updates,tables) || areTablesToUpdated(insertions,tables) || areTablesToUpdated(unresolvedInsertions.getDependentEntityInsertActions(),tables)|| areTablesToUpdated(deletions,tables)|| areTablesToUpdated(collectionUpdates,tables)|| areTablesToUpdated(collectionCreations,tables)|| areTablesToUpdated(collectionRemovals,tables);
}
