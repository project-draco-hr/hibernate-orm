{
  if (loadCounter == 0) {
    LOG.debug("Initializing non-lazy collections");
    loadCounter++;
    try {
      int size;
      while ((size=nonlazyCollections.size()) > 0) {
        ((PersistentCollection)nonlazyCollections.remove(size - 1)).forceInitialization();
      }
    }
  finally {
      loadCounter--;
      clearNullProperties();
    }
  }
}
