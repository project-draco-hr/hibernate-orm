{
switch (code) {
case Types.INTEGER:
case Types.BIGINT:
case Types.SMALLINT:
    return smallIntegerCastTarget();
case Types.FLOAT:
case Types.REAL:
{
    return floatingPointNumberCastTarget();
  }
case Types.NUMERIC:
return fixedPointNumberCastTarget();
case Types.VARCHAR:
return "char";
case Types.VARBINARY:
return "binary";
default :
return super.getCastTypeName(code);
}
}
