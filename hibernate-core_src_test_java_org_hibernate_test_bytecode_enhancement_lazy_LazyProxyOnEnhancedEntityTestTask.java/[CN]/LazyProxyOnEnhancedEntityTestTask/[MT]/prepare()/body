{
  super.prepare(new Configuration());
  EntityManager em=getFactory().createEntityManager();
  em.getTransaction().begin();
  Child c=new Child();
  em.persist(c);
  Parent parent=new Parent();
  parent.setChild(c);
  em.persist(parent);
  parentID=parent.getId();
  em.getTransaction().commit();
  em.clear();
  em.close();
}
