{
  int count=0;
  if (getExpectedType() != null && (count=getExpectedType().getColumnSpan(sessionFactory)) > 1) {
    StringBuffer buffer=new StringBuffer();
    buffer.append("(?");
    for (int i=1; i < count; i++) {
