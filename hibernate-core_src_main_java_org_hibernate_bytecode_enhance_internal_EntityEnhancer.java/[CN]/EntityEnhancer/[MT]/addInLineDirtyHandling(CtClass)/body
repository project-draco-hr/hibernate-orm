{
  managedCtClass.addInterface(loadCtClassFromClass(SelfDirtinessTracker.class));
  FieldWriter.addField(managedCtClass,loadCtClassFromClass(DirtyTracker.class),EnhancerConstants.TRACKER_FIELD_NAME);
  FieldWriter.addField(managedCtClass,loadCtClassFromClass(CollectionTracker.class),EnhancerConstants.TRACKER_COLLECTION_NAME);
  createDirtyTrackerMethods(managedCtClass);
}
