{
  String tableName=null;
  if (hasOwnTable()) {
    tableName=getConfiguredClass().getSimpleName();
  }
 else   if (getParent() != null && !getParent().getConfiguredClassType().equals(ConfiguredClassType.MAPPED_SUPERCLASS)) {
    tableName=((EntityClass)getParent()).getPrimaryTableName();
  }
  return tableName;
}
