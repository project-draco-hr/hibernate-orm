{
  final AnnotationInstance synchronizeAnnotation=JandexHelper.getSingleAnnotation(getClassInfo(),HibernateDotNames.SYNCHRONIZE);
  if (synchronizeAnnotation != null) {
    final String[] tableNames=synchronizeAnnotation.value().asStringArray();
    return Arrays.asList(tableNames);
  }
 else {
    return Collections.emptyList();
  }
}
