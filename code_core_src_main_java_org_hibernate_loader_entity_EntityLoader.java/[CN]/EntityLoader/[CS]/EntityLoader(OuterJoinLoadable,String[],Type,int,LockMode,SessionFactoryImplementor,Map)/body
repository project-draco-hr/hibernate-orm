{
  super(persister,uniqueKeyType,factory,enabledFilters);
  JoinWalker walker=new EntityJoinWalker(persister,uniqueKey,batchSize,lockMode,factory,enabledFilters);
  initFromWalker(walker);
  postInstantiate();
  batchLoader=batchSize > 1;
  log.debug("Static select for entity " + entityName + ": "+ getSQLString());
}
