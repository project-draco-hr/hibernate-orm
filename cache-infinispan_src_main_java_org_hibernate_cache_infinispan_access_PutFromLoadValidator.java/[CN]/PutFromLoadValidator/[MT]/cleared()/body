{
  invalidationTimestamp=System.currentTimeMillis() + this.nakedPutInvalidationPeriod;
  pendingLock.lock();
  try {
    removalsLock.lock();
    try {
      pendingPuts.clear();
      pendingQueue.clear();
      overagePendingQueue.clear();
      recentRemovals.clear();
      removalsQueue.clear();
      earliestRemovalTimestamp=invalidationTimestamp;
    }
  finally {
      removalsLock.unlock();
    }
  }
  finally {
    pendingLock.unlock();
  }
}
