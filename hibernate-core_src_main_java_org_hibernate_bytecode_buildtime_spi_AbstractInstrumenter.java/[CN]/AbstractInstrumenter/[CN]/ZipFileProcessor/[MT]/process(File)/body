{
  final ZipInputStream zip=new ZipInputStream(new FileInputStream(file));
  try {
    ZipEntry entry;
    while ((entry=zip.getNextEntry()) != null) {
      final byte[] bytes=ByteCodeHelper.readByteCode(zip);
      entryHandler.handleEntry(entry,bytes);
      zip.closeEntry();
    }
  }
  finally {
    zip.close();
  }
}
