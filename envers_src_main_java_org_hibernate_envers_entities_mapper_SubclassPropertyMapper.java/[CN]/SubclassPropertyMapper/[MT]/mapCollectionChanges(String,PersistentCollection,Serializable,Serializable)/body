{
  List<PersistentCollectionChangeData> parentCollectionChanges=parentMapper.mapCollectionChanges(referencingPropertyName,newColl,oldColl,id);
  List<PersistentCollectionChangeData> mainCollectionChanges=main.mapCollectionChanges(referencingPropertyName,newColl,oldColl,id);
  if (parentCollectionChanges == null) {
    return mainCollectionChanges;
  }
 else {
    parentCollectionChanges.addAll(mainCollectionChanges);
    return parentCollectionChanges;
  }
}
