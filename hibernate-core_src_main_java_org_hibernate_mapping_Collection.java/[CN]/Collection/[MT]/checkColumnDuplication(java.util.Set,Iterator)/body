{
  while (columns.hasNext()) {
    Selectable s=(Selectable)columns.next();
    if (!s.isFormula()) {
      Column col=(Column)s;
      if (!distinctColumns.add(col.getName())) {
        throw new MappingException("Repeated column in mapping for collection: " + getRole() + " column: "+ col.getName());
      }
    }
  }
}
