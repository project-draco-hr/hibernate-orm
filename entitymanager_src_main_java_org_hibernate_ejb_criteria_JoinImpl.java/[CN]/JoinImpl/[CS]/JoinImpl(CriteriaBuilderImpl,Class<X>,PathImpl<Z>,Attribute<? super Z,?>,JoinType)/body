{
  super(criteriaBuilder,javaType,lhs,joinProperty,(Bindable<X>)(Attribute.PersistentAttributeType.EMBEDDED == joinProperty.getPersistentAttributeType() ? joinProperty : criteriaBuilder.getEntityManagerFactory().getMetamodel().managedType(javaType)));
  this.managedType=getManagedType();
  this.joinType=joinType;
}
