{
  PreparedStatement rtn=super.prepareBatchStatement(sql);
  if (batchSQL == null || !batchSQL.equals(sql)) {
    currentBatch++;
    batchSQL=sql;
    batchSizes.add(currentBatch,new Counter());
    System.out.println("--------------------------------------------------------");
    System.out.println("Preparing statement [" + sql + "]");
  }
  return rtn;
}
