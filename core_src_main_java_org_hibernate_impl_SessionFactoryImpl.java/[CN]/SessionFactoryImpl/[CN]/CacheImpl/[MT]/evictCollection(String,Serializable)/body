{
  CollectionPersister p=getCollectionPersister(role);
  if (p.hasCache()) {
    if (log.isDebugEnabled()) {
      log.debug("evicting second-level cache: " + MessageHelper.collectionInfoString(p,ownerIdentifier,SessionFactoryImpl.this));
    }
    CacheKey cacheKey=new CacheKey(ownerIdentifier,p.getKeyType(),p.getRole(),EntityMode.POJO,SessionFactoryImpl.this);
    p.getCacheAccessStrategy().evict(cacheKey);
  }
}
