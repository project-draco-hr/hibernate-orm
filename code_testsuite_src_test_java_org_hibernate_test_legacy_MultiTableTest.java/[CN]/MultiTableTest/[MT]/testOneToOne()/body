{
  Session s=openSession();
  Lower ls=new Lower();
  Serializable id=s.save(ls);
  s.flush();
  s.connection().commit();
  s.close();
  s=openSession();
  s.load(Lower.class,id);
  s.connection().commit();
  s.close();
  s=openSession();
  s.delete(s.load(Lower.class,id));
  s.flush();
  s.connection().commit();
  s.close();
}
