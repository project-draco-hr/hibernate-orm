{
  propertyIndexes.put(attribute.getName(),i);
  if (attribute.isSingular() && ((SingularAttribute)attribute).getSingularAttributeType().getNature() == TypeNature.COMPONENT) {
    org.hibernate.metamodel.domain.Component component=(org.hibernate.metamodel.domain.Component)((SingularAttribute)attribute).getSingularAttributeType();
    for (    Attribute subAttribute : component.getAttributes()) {
      propertyIndexes.put(attribute.getName() + '.' + subAttribute.getName(),i);
    }
  }
}
