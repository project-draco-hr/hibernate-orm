{
  return buildingContext.getBuildingOptions().getIdGenerationTypeInterpreter().determineGeneratorName(generatorEnum,new IdGenerationTypeInterpreter.Context(){
    Class javaType=null;
    @Override public Class getIdType(){
      if (javaType == null) {
        javaType=buildingContext.getBuildingOptions().getReflectionManager().toClass(javaTypeXClass);
      }
      return javaType;
    }
  }
);
}
