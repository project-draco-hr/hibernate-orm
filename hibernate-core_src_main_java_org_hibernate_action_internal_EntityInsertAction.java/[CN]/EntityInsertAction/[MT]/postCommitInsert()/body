{
  PostInsertEventListener[] postListeners=getSession().getListeners().getPostCommitInsertEventListeners();
  if (postListeners.length > 0) {
    PostInsertEvent postEvent=new PostInsertEvent(getInstance(),getId(),state,getPersister(),(EventSource)getSession());
    for (    PostInsertEventListener postListener : postListeners) {
      postListener.onPostInsert(postEvent);
    }
  }
}
