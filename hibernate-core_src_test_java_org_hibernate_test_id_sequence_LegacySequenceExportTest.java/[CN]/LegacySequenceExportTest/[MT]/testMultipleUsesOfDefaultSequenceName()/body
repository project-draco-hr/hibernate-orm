{
  final MetadataImplementor metadata=(MetadataImplementor)new MetadataSources(ssr).addAnnotatedClass(Entity1.class).addAnnotatedClass(Entity2.class).buildMetadata();
  metadata.validate();
  int auxCount=0;
  for (  AuxiliaryDatabaseObject auxiliaryDatabaseObject : metadata.getDatabase().getAuxiliaryDatabaseObjects()) {
    auxCount++;
  }
  assertEquals(1,auxCount);
}
