{
  return new String[]{new StringBuffer(dialect.getCreateTableString()).append(' ').append(tableName).append(" ( ").append(pkColumnName).append(' ').append(dialect.getTypeName(Types.VARCHAR,keySize,0,0)).append(",  ").append(valueColumnName).append(' ').append(dialect.getTypeName(Types.INTEGER)).append(" ) ").toString()};
}
