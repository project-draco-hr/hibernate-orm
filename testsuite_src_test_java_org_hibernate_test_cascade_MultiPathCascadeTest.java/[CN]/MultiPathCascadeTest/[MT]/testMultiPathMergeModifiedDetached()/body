{
  Session s=openSession();
  s.beginTransaction();
  A a=new A();
  a.setData("Anna");
  s.save(a);
  s.getTransaction().commit();
  s.close();
  modifyEntity(a);
  s=openSession();
  s.beginTransaction();
  a=(A)s.merge(a);
  s.getTransaction().commit();
  s.close();
  verifyModifications(a.getId());
}
