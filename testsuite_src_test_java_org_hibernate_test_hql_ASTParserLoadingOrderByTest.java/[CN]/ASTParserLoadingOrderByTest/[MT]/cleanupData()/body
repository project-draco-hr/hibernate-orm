{
  Session s=openSession();
  Transaction t=s.beginTransaction();
  if (zoo1 != null) {
    s.delete(zoo1);
    zoo1=null;
  }
  if (zoo2 != null) {
    s.delete(zoo2);
    zoo2=null;
  }
  if (zoo3 != null) {
    s.delete(zoo3);
    zoo3=null;
  }
  if (zoo4 != null) {
    s.delete(zoo4);
    zoo4=null;
  }
  t.commit();
  s.close();
}
