{
  final CompositeQuerySpaceImpl rhs=new CompositeQuerySpaceImpl(getUid(),compositionDefinition.getType(),querySpaceUid,Disposition.COMPOSITE,getQuerySpaces(),sessionFactory());
  final JoinDefinedByMetadata join=new JoinImpl(this,compositionDefinition,rhs,compositionDefinition.isNullable());
  internalGetJoins().add(join);
  getQuerySpaces().registerQuerySpace(rhs);
  return join;
}
