{
  RecordingTarget target=doCreation(new DefaultSchemaFilter());
  Assert.assertThat(target.getActions("schema.create"),containsExactly("the_schema_1","the_schema_2"));
  Assert.assertThat(target.getActions("table.create"),containsExactly("the_entity_0","the_schema_1.the_entity_1","the_schema_1.the_entity_2","the_schema_2.the_entity_3","the_schema_2.the_entity_4"));
}
