{
  javax.persistence.EnumType enumType=null;
  if (key) {
    MapKeyEnumerated enumAnn=property.getAnnotation(MapKeyEnumerated.class);
    if (enumAnn != null) {
      enumType=enumAnn.value();
    }
  }
 else {
    Enumerated enumAnn=property.getAnnotation(Enumerated.class);
    if (enumAnn != null) {
      enumType=enumAnn.value();
    }
  }
  return enumType;
}
