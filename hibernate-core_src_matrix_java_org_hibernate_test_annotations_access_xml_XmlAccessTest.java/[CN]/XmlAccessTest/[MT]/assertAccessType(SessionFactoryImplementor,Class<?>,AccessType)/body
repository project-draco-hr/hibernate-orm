{
  EntityTuplizer tuplizer=factory.getEntityPersister(classUnderTest.getName()).getEntityMetamodel().getTuplizer();
  if (AccessType.FIELD.equals(accessType)) {
    Assert.assertTrue("Field access was expected.",tuplizer.getGetter(0) instanceof DirectPropertyAccessor.DirectGetter);
  }
 else {
    Assert.assertTrue("Property access was expected.",tuplizer.getGetter(0) instanceof BasicPropertyAccessor.BasicGetter);
  }
}
