{
  if (persistentClass.hasIdentifierProperty()) {
    final Property declaredIdentifierProperty=persistentClass.getDeclaredIdentifierProperty();
    if (declaredIdentifierProperty != null) {
      jpaEntityType.getBuilder().applyIdAttribute(attributeFactory.buildIdAttribute(jpaEntityType,declaredIdentifierProperty,true));
    }
  }
 else {
    jpaEntityType.getBuilder().applyIdClassAttributes(buildIdClassAttributes(jpaEntityType,persistentClass));
  }
}
