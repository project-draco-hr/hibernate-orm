{
  if (LOG.isTraceEnabled())   LOG.trace("Replicating changes to " + MessageHelper.infoString(persister,id,source.getFactory()));
  new OnReplicateVisitor(source,id,entity,true).process(entity,persister);
  source.getPersistenceContext().addEntity(entity,(persister.isMutable() ? Status.MANAGED : Status.READ_ONLY),null,new EntityKey(id,persister,source.getEntityMode()),version,LockMode.NONE,true,persister,true,false);
  cascadeAfterReplicate(entity,persister,replicationMode,source);
}
