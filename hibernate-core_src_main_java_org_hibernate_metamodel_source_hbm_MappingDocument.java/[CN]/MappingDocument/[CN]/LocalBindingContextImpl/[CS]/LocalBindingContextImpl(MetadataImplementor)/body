{
  this.metadata=metadata;
  this.localMappingDefaults=new OverriddenMappingDefaults(metadata.getMappingDefaults(),hbmJaxbRoot.getRoot().getPackage(),hbmJaxbRoot.getRoot().getSchema(),hbmJaxbRoot.getRoot().getCatalog(),null,null,hbmJaxbRoot.getRoot().getDefaultCascade(),hbmJaxbRoot.getRoot().getDefaultAccess(),hbmJaxbRoot.getRoot().isDefaultLazy());
  if (hbmJaxbRoot.getRoot().getMeta() == null || hbmJaxbRoot.getRoot().getMeta().isEmpty()) {
    this.metaAttributeContext=new MetaAttributeContext(metadata.getGlobalMetaAttributeContext());
  }
 else {
    this.metaAttributeContext=Helper.extractMetaAttributeContext(hbmJaxbRoot.getRoot().getMeta(),true,metadata.getGlobalMetaAttributeContext());
  }
}
