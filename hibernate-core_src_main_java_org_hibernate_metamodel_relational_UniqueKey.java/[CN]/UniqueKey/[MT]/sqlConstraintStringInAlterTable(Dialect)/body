{
  StringBuffer buf=new StringBuffer(dialect.getAddPrimaryKeyConstraintString(getName())).append('(');
  boolean nullable=false;
  boolean first=true;
  for (  Column column : getColumns()) {
    if (first) {
      first=false;
    }
 else {
      buf.append(", ");
    }
    if (!nullable && column.isNullable()) {
      nullable=true;
    }
    buf.append(column.getColumnName().encloseInQuotesIfQuoted(dialect));
  }
  return !nullable || dialect.supportsNotNullUnique() ? StringHelper.replace(buf.append(')').toString(),"primary key","unique") : null;
}
