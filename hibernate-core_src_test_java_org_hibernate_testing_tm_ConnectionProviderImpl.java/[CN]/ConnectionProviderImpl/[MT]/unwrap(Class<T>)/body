{
  if (ConnectionProviderImpl.class.isAssignableFrom(unwrapType)) {
    return (T)this;
  }
 else   if (ConnectionProvider.class.isAssignableFrom(unwrapType) || getActualConnectionProvider().getClass().isAssignableFrom(unwrapType)) {
    return (T)getActualConnectionProvider();
  }
 else {
    throw new UnknownUnwrapTypeException(unwrapType);
  }
}
