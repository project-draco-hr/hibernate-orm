{
  statement.getFromClause().resolve();
  FromElement fromElement=(FromElement)statement.getFromClause().getFromElements().get(0);
  Queryable persister=fromElement.getQueryable();
  fromElement.setText(persister.getTableName());
  if (persister.getDiscriminatorType() != null) {
    new SyntheticAndFactory(getASTFactory()).addDiscriminatorWhereFragment(statement,persister,java.util.Collections.EMPTY_MAP,fromElement.getTableAlias());
  }
}
