{
  EntityManager em=getEntityManager();
  em.getTransaction().begin();
  StrTestEntity ste=new StrTestEntity("x");
  em.persist(ste);
  steId=ste.getId();
  em.getTransaction().commit();
  em.getTransaction().begin();
  ste=em.find(StrTestEntity.class,steId);
  ste.setStr("y");
  em.getTransaction().commit();
}
