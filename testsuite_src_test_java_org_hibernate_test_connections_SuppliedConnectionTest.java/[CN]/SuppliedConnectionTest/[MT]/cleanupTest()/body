{
  Connection conn=cp.getConnection();
  try {
    new SchemaExport(getCfg(),conn).drop(false,true);
  }
  finally {
    if (conn != null) {
      try {
        cp.closeConnection(conn);
      }
 catch (      Throwable ignore) {
      }
    }
  }
  try {
    cp.close();
  }
 catch (  Throwable ignore) {
  }
  super.cleanupTest();
}
