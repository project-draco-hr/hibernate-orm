{
  final PersistentClass superPersistentClass=persistentClass.getSuperclass();
  final EntityTypeImpl superEntityType=superPersistentClass == null ? null : locateOrBuildEntityType(superPersistentClass,context);
  final Class javaType=persistentClass.getMappedClass();
  EntityTypeImpl entityType=new EntityTypeImpl(javaType,superEntityType,persistentClass.getClassName(),persistentClass.hasIdentifierProperty(),persistentClass.isVersioned());
  context.registerEntityType(persistentClass,entityType);
  return entityType;
}
