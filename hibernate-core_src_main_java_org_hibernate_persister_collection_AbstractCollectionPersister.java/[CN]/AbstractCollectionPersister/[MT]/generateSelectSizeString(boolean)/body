{
  String selectValue=isIntegerIndexed ? "max(" + getIndexColumnNames()[0] + ") + 1" : "count(" + getElementColumnNames()[0] + ")";
  return new SimpleSelect(dialect).setTableName(getTableName()).addCondition(getKeyColumnNames(),"=?").addWhereToken(sqlWhereString).addColumn(selectValue).toStatementString();
}
