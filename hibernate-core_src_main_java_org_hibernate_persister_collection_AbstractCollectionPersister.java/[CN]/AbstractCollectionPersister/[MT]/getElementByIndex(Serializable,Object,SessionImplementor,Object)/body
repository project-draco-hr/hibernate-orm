{
  try {
    PreparedStatement st=session.getBatcher().prepareSelectStatement(sqlSelectRowByIndexString);
    try {
      getKeyType().nullSafeSet(st,key,1,session);
      getIndexType().nullSafeSet(st,incrementIndexByBase(index),keyColumnNames.length + 1,session);
      ResultSet rs=st.executeQuery();
      try {
        if (rs.next()) {
          return getElementType().nullSafeGet(rs,elementColumnAliases,session,owner);
        }
 else {
          return null;
        }
      }
  finally {
        rs.close();
      }
    }
  finally {
      session.getBatcher().closeStatement(st);
    }
  }
 catch (  SQLException sqle) {
    throw JDBCExceptionHelper.convert(getFactory().getSQLExceptionConverter(),sqle,"could not read row: " + MessageHelper.collectionInfoString(this,key,getFactory()),sqlSelectSizeString);
  }
}
