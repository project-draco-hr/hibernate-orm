{
  try {
    PreparedStatement st=session.getTransactionCoordinator().getJdbcCoordinator().getStatementPreparer().prepareStatement(sqlSelectRowByIndexString);
    try {
      getKeyType().nullSafeSet(st,key,1,session);
      getIndexType().nullSafeSet(st,incrementIndexByBase(index),keyColumnNames.length + 1,session);
      ResultSet rs=session.getTransactionCoordinator().getJdbcCoordinator().getResultSetReturn().extract(st);
      try {
        if (rs.next()) {
          return getElementType().nullSafeGet(rs,elementColumnAliases,session,owner);
        }
 else {
          return null;
        }
      }
  finally {
        session.getTransactionCoordinator().getJdbcCoordinator().release(rs,st);
      }
    }
  finally {
      session.getTransactionCoordinator().getJdbcCoordinator().release(st);
    }
  }
 catch (  SQLException sqle) {
    throw getSQLExceptionHelper().convert(sqle,"could not read row: " + MessageHelper.collectionInfoString(this,key,getFactory()),sqlSelectSizeString);
  }
}
