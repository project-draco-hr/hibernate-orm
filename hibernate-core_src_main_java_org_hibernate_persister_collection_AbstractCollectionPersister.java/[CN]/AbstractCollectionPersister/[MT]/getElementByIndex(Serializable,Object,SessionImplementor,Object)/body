{
  try {
    PreparedStatement st=session.getTransactionCoordinator().getJdbcCoordinator().getStatementPreparer().prepareStatement(sqlSelectRowByIndexString);
    try {
      getKeyType().nullSafeSet(st,key,1,session);
      getIndexType().nullSafeSet(st,incrementIndexByBase(index),keyColumnNames.length + 1,session);
      ResultSet rs=st.executeQuery();
      try {
        if (rs.next()) {
          return getElementType().nullSafeGet(rs,elementColumnAliases,session,owner);
        }
 else {
          return null;
        }
      }
  finally {
        rs.close();
      }
    }
  finally {
      st.close();
    }
  }
 catch (  SQLException sqle) {
    throw getFactory().getSQLExceptionHelper().convert(sqle,"could not read row: " + MessageHelper.collectionInfoString(this,key,getFactory()),sqlSelectSizeString);
  }
}
