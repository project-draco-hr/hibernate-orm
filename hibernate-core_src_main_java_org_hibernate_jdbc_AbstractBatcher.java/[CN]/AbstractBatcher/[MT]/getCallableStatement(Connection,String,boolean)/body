{
  if (scrollable && !factory.getSettings().isScrollableResultSetsEnabled())   throw new AssertionFailure("scrollable result sets are not enabled");
  sql=getSQL(sql);
  log(sql);
  LOG.preparingCallableStatement();
  if (scrollable)   return conn.prepareCall(sql,ResultSet.TYPE_SCROLL_INSENSITIVE,ResultSet.CONCUR_READ_ONLY);
  return conn.prepareCall(sql);
}
