{
  if (batchUpdate != null) {
    try {
      try {
        doExecuteBatch(batchUpdate);
      }
  finally {
        closeStatement(batchUpdate);
      }
    }
 catch (    SQLException sqle) {
      throw JDBCExceptionHelper.convert(factory.getSQLExceptionConverter(),sqle,"Could not execute JDBC batch update",batchUpdateSQL);
    }
 finally {
      batchUpdate=null;
      batchUpdateSQL=null;
    }
  }
}
