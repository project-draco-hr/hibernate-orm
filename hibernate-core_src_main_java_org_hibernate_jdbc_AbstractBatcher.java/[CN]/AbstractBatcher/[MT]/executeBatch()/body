{
  checkNotClosingBatchUpdate();
  if (batchUpdate != null) {
    try {
      try {
        doExecuteBatch(batchUpdate);
      }
  finally {
        closeBatchUpdate();
      }
    }
 catch (    SQLException sqle) {
      throw exceptionHelper.convert(sqle,"Could not execute JDBC batch update",batchUpdateSQL);
    }
 finally {
      batchUpdate=null;
      batchUpdateSQL=null;
    }
  }
}
