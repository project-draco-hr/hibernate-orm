{
  if (hasIndex) {
    for (int i=0; i < indexColumnIsSettable.length; i++) {
      if (indexColumnIsSettable[i]) {
        frag.addColumn(alias,indexColumnNames[i],indexColumnAliases[i]);
      }
 else {
        frag.addFormula(alias,indexFormulaTemplates[i],indexColumnAliases[i]);
      }
    }
  }
}
