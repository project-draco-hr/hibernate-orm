{
  if (!isInverse && collection.isRowUpdatePossible()) {
    if (log.isDebugEnabled()) {
      log.debug("Updating rows of collection: " + role + "#"+ id);
    }
    int count=doUpdateRows(id,collection,session);
    if (log.isDebugEnabled()) {
      log.debug("done updating rows: " + count + " updated");
    }
  }
}
