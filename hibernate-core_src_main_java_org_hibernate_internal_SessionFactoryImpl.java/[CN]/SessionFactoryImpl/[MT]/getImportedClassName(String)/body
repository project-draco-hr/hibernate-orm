{
  String result=imports.get(className);
  if (result == null) {
    try {
      serviceRegistry.getService(ClassLoaderService.class).classForName(className);
      imports.put(className,className);
      return className;
    }
 catch (    ClassLoadingException cnfe) {
      return null;
    }
  }
 else {
    return result;
  }
}
