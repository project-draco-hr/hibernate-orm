{
  CustomEntityDirtinessStrategy defaultValue=new CustomEntityDirtinessStrategy(){
    @Override public boolean canDirtyCheck(    Object entity,    EntityPersister persister,    Session session){
      return false;
    }
    @Override public boolean isDirty(    Object entity,    EntityPersister persister,    Session session){
      return false;
    }
    @Override public void resetDirty(    Object entity,    EntityPersister persister,    Session session){
    }
    @Override public void findDirty(    Object entity,    EntityPersister persister,    Session session,    DirtyCheckContext dirtyCheckContext){
    }
  }
;
  return serviceRegistry.getService(ConfigurationService.class).getSetting(AvailableSettings.CUSTOM_ENTITY_DIRTINESS_STRATEGY,CustomEntityDirtinessStrategy.class,defaultValue);
}
