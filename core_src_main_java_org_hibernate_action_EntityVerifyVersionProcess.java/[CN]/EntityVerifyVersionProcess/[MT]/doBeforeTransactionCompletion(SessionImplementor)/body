{
  final EntityPersister persister=entry.getPersister();
  Object latestVersion=persister.getCurrentVersion(entry.getId(),session);
  if (!entry.getVersion().equals(latestVersion))   throw new OptimisticLockException("Newer version (" + latestVersion + ") of entity ("+ entry.getEntityName()+ ") found in database.  id="+ entry.getId());
}
