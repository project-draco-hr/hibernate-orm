{
  if (values.size() == 0) {
    return false;
  }
  if (!isOperationQueueEnabled()) {
    write();
    return list.addAll(values);
  }
 else {
    Iterator iter=values.iterator();
    while (iter.hasNext()) {
      queueOperation(new SimpleAdd(iter.next()));
    }
    return values.size() > 0;
  }
}
