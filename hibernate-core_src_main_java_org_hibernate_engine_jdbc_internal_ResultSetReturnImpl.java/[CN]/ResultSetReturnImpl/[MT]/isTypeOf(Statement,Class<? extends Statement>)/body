{
  boolean matches=type.isInstance(statement);
  try {
    boolean jdbc4;
    try {
      jdbc4=(statement.getClass().getMethod("isWrapperFor",new Class<?>[]{Class.class}) != null);
    }
 catch (    Exception e) {
      jdbc4=false;
    }
    if (jdbc4) {
      matches=statement.isWrapperFor(type);
    }
  }
 catch (  Exception e) {
  }
  return matches;
}
