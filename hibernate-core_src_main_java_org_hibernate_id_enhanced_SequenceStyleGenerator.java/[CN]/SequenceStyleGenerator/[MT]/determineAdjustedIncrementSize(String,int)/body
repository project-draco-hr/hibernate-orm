{
  if (incrementSize > 1 && OptimizerFactory.StandardOptimizerDescriptor.NONE.getExternalName().equals(optimizationStrategy)) {
    LOG.honoringOptimizerSetting(OptimizerFactory.StandardOptimizerDescriptor.NONE.getExternalName(),INCREMENT_PARAM,incrementSize);
    incrementSize=1;
  }
  return incrementSize;
}
