{
  final String tableName=metadata.getDatabase().getJdbcEnvironment().getQualifiedObjectNameFormatter().format(uniqueKey.getTable().getQualifiedTableName(),metadata.getDatabase().getJdbcEnvironment().getDialect());
  final String constraintName=dialect.quote(uniqueKey.getName());
  return "alter table " + tableName + " add constraint "+ uniqueConstraintSql(uniqueKey)+ " constraint "+ constraintName;
}
