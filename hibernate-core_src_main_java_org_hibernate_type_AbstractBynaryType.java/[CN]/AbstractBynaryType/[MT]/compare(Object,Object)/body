{
  byte[] xbytes=toInternalFormat(x);
  byte[] ybytes=toInternalFormat(y);
  if (xbytes.length < ybytes.length)   return -1;
  if (xbytes.length > ybytes.length)   return 1;
  for (int i=0; i < xbytes.length; i++) {
    if (xbytes[i] < ybytes[i])     return -1;
    if (xbytes[i] > ybytes[i])     return 1;
  }
  return 0;
}
