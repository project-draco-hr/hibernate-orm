{
  if (idDatas.size() == 0) {
    return new OneEntityQueryGenerator(verEntCfg,auditMiddleEntityName,referencingIdData,componentDatas);
  }
 else   if (idDatas.size() == 1) {
    return new TwoEntityQueryGenerator(globalCfg,verEntCfg,auditMiddleEntityName,referencingIdData,idDatas.get(0),componentDatas);
  }
 else   if (idDatas.size() == 2) {
    return new ThreeEntityQueryGenerator(globalCfg,verEntCfg,auditMiddleEntityName,referencingIdData,idDatas.get(0),idDatas.get(1),componentDatas);
  }
 else {
    throw new IllegalStateException("Illegal number of related entities.");
  }
}
