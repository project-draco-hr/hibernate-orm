{
  if (attributeBinding.getAttribute() == null) {
    attributeBinding.initialize(new HbmSimpleAttributeDomainState(hibernateMappingBinder,propertyElement,entityBinding.getEntity().getOrCreateSingularAttribute(attributeName)));
  }
  if (attributeBinding.getValue() == null) {
    attributeBinding.initializeTupleValue(new HbmSimpleValueRelationalStateContainer(getHibernateMappingBinder(),propertyElement,true));
  }
}
