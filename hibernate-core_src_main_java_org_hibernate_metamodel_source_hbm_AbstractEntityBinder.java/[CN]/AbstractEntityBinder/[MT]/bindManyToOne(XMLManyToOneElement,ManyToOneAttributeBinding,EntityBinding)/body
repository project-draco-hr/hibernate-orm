{
  if (attributeBinding.getAttribute() == null) {
    attributeBinding.initialize(new HbmManyToOneAttributeDomainState(hibernateMappingBinder,entityBinding.getEntity().getOrCreateSingularAttribute(manyToOne.getName()),entityBinding.getMetaAttributes(),manyToOne));
  }
  if (attributeBinding.getValue() == null) {
    attributeBinding.initialize(new HbmManyToOneRelationalStateContainer(getHibernateMappingBinder(),true,manyToOne));
  }
}
