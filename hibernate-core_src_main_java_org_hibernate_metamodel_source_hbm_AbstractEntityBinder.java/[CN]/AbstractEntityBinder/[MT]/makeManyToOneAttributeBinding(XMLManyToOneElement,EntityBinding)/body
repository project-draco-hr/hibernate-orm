{
  ManyToOneAttributeBindingState bindingState=new HbmManyToOneAttributeBindingState(entityBinding.getEntity().getPojoEntitySpecifics().getClassName(),hibernateMappingBinder,entityBinding.getMetaAttributeContext(),manyToOne);
  ManyToOneRelationalState relationalState=new HbmManyToOneRelationalStateContainer(hibernateMappingBinder,true,manyToOne);
  entityBinding.getEntity().getOrCreateSingularAttribute(bindingState.getAttributeName());
  ManyToOneAttributeBinding manyToOneAttributeBinding=entityBinding.makeManyToOneAttributeBinding(bindingState.getAttributeName()).initialize(bindingState).initialize(relationalState);
  return manyToOneAttributeBinding;
}
