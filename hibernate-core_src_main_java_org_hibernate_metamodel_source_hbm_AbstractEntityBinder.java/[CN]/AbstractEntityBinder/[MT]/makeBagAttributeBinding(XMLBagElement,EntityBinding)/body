{
  PluralAttributeBindingState bindingState=new HbmPluralAttributeBindingState(entityBinding.getEntity().getPojoEntitySpecifics().getClassName(),hibernateMappingBinder,entityBinding.getMetaAttributeContext(),collection);
  BagBinding collectionBinding=entityBinding.makeBagAttributeBinding(bindingState.getAttributeName(),getCollectionElementType(collection)).initialize(bindingState);
  return collectionBinding;
}
