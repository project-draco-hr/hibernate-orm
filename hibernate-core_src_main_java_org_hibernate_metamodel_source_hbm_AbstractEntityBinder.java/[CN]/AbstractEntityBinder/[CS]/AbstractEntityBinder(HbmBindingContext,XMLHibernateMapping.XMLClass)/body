{
  this.bindingContext=bindingContext;
  this.schemaName=new Schema.Name(entityClazz.getSchema() == null ? bindingContext.getMappingDefaults().getDefaultSchemaName() : entityClazz.getSchema(),entityClazz.getCatalog() == null ? bindingContext.getMappingDefaults().getDefaultCatalogName() : entityClazz.getCatalog());
}
