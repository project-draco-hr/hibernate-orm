{
  this.hibernateMappingBinder=hibernateMappingBinder;
  this.schemaName=new Schema.Name((entityClazz.getSchema() == null ? hibernateMappingBinder.getMappingDefaults().getDefaultSchemaName() : entityClazz.getSchema()),(entityClazz.getCatalog() == null ? hibernateMappingBinder.getMappingDefaults().getDefaultCatalogName() : entityClazz.getCatalog()));
}
