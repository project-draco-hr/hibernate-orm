{
  EntityManager em=getOrCreateEntityManager();
  em.getTransaction().begin();
  Oven oven=new Oven();
  Kitchen kitchen=new Kitchen();
  em.persist(oven);
  em.persist(kitchen);
  kitchen.setOven(oven);
  oven.setKitchen(kitchen);
  em.flush();
  em.clear();
  oven=em.merge(oven);
  em.flush();
  em.getTransaction().rollback();
  em.close();
}
