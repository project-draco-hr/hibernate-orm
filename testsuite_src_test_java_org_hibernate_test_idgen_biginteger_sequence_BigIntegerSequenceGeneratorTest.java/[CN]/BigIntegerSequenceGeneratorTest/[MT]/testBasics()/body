{
  Session s=openSession();
  s.beginTransaction();
  Entity entity=new Entity("BigInteger + sequence #1");
  s.save(entity);
  Entity entity2=new Entity("BigInteger + sequence #2");
  s.save(entity2);
  s.getTransaction().commit();
  s.close();
  s=openSession();
  s.beginTransaction();
  s.delete(entity);
  s.delete(entity2);
  s.getTransaction().commit();
  s.close();
}
