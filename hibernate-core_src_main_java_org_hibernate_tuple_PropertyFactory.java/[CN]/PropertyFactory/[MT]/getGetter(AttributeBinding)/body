{
  if (mappingProperty == null || mappingProperty.getEntityBinding().getEntity().getJavaType() == null) {
    return null;
  }
  PropertyAccessor pa=PropertyAccessorFactory.getPropertyAccessor(mappingProperty,EntityMode.POJO);
  return pa.getGetter(mappingProperty.getEntityBinding().getEntity().getJavaType().getClassReference(),mappingProperty.getAttribute().getName());
}
