{
  String mappedUnsavedValue=((KeyValue)property.getValue()).getNullValue();
  VersionValue unsavedValue=UnsavedValueFactory.getUnsavedVersionValue(mappedUnsavedValue,getGetter(property),(VersionType)property.getHibernateTypeDescriptor().getResolvedTypeMapping(),getConstructor((EntityBinding)property.getContainer()));
  boolean lazy=lazyAvailable && property.isLazy();
  final CascadeStyle cascadeStyle=property.isAssociation() ? ((AssociationAttributeBinding)property).getCascadeStyle() : CascadeStyle.NONE;
  return new VersionProperty(property.getAttribute().getName(),null,property.getHibernateTypeDescriptor().getResolvedTypeMapping(),lazy,true,true,property.getGeneration() == PropertyGeneration.INSERT || property.getGeneration() == PropertyGeneration.ALWAYS,property.getGeneration() == PropertyGeneration.ALWAYS,property.isNullable(),!lazy,property.isIncludedInOptimisticLocking(),cascadeStyle,unsavedValue);
}
