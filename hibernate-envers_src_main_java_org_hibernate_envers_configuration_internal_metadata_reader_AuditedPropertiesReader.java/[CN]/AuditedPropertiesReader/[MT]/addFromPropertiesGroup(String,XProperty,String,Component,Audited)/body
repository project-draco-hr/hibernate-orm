{
  final ComponentAuditingData componentData=new ComponentAuditingData();
  final boolean isAudited=fillPropertyData(property,componentData,accessType,allClassAudited);
  if (isAudited) {
    componentData.setName(embeddedName);
    componentData.setBeanName(null);
    final PersistentPropertiesSource componentPropertiesSource=new ComponentPropertiesSource(reflectionManager,propertyValue);
    final AuditedPropertiesReader audPropReader=new AuditedPropertiesReader(defaultAudited,componentPropertiesSource,componentData,globalCfg,reflectionManager,propertyNamePrefix + MappingTools.createComponentPrefix(embeddedName));
    audPropReader.read();
    auditedPropertiesHolder.addPropertyAuditingData(embeddedName,componentData);
  }
}
