{
  if (isGenerated(dialect)) {
    String constraintString=sqlConstraintString(dialect,getName(),defaultCatalog,defaultSchema);
    StringBuilder buf=new StringBuilder("alter table ").append(getTable().getQualifiedName(dialect,defaultCatalog,defaultSchema)).append(constraintString);
    return buf.toString();
  }
 else {
    return null;
  }
}
