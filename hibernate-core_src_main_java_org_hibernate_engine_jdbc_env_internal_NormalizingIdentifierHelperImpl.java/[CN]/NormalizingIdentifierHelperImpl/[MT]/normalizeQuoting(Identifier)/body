{
  if (identifier == null) {
    return null;
  }
  if (identifier.isQuoted()) {
    return identifier;
  }
  if (globallyQuoteIdentifiers) {
    return Identifier.toIdentifier(identifier.getText(),true);
  }
  if (jdbcEnvironment.isReservedWord(identifier.getText())) {
    return Identifier.toIdentifier(identifier.getText(),true);
  }
  return identifier;
}
