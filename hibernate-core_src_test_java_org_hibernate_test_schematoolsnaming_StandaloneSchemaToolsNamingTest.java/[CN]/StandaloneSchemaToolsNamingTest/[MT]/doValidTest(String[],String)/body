{
  OUTFILE.delete();
  SchemaExport.main(args);
  assertTrue(OUTFILE.exists());
  assertFileContainsTrimmedLineStartingWith(OUTFILE,collectionTableNameExpected);
  SchemaValidator.main(args);
  OUTFILE.delete();
  SchemaUpdate.main(args);
  assertFileIsEmpty(OUTFILE);
  dropSchema(args);
  OUTFILE.delete();
  SchemaUpdate.main(args);
  assertTrue(OUTFILE.exists());
  assertFileContainsTrimmedLineStartingWith(OUTFILE,collectionTableNameExpected);
  SchemaValidator.main(args);
  dropSchema(args);
}
