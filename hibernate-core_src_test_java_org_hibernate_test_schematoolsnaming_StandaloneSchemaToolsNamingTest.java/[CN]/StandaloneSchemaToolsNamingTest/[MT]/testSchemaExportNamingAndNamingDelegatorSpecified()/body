{
  OUTFILE.delete();
  SchemaExport.main(new String[]{"--output=" + OUTFILE.getName(),"--config=org/hibernate/test/schematoolsnaming/hibernate.cfg.xml","--naming=" + EJB3NamingStrategy.INSTANCE.getClass().getName(),"--namingdelegator=" + ImprovedNamingStrategyDelegator.DEFAULT_INSTANCE.getClass().getName()});
  assertFalse(OUTFILE.exists());
}
