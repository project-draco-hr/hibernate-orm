{
  final ConfigurationService cfgService=registry.getService(ConfigurationService.class);
  final StrategySelector strategySelector=registry.getService(StrategySelector.class);
  final HibernateSpatialConfiguration spatialConfig=new HibernateSpatialConfiguration(cfgService.getSetting(OGC_STRICT,BOOLEAN,null),strategySelector.resolveStrategy(ConnectionFinder.class,cfgService.getSettings().get(CONNECTION_FINDER)));
  return new SpatialDialectFactory(spatialConfig);
}
