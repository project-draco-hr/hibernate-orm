{
  EntityManager em=getEntityManager();
  em.getTransaction().begin();
  ChildEntity ce=new ChildEntity("x",1l);
  em.persist(ce);
  id1=ce.getId();
  em.getTransaction().commit();
  em.getTransaction().begin();
  ce=em.find(ChildEntity.class,id1);
  ce.setData("y");
  ce.setNumber(2l);
  em.getTransaction().commit();
}
