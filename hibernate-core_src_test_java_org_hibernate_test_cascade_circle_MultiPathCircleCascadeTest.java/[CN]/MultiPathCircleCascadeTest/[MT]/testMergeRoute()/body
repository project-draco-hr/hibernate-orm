{
  Route route=getUpdatedDetachedEntity();
  clearCounts();
  Session s=openSession();
  s.beginTransaction();
  s.merge(route);
  s.getTransaction().commit();
  s.close();
  assertInsertCount(4);
  assertUpdateCount(1);
  s=openSession();
  s.beginTransaction();
  route=(Route)s.get(Route.class,route.getRouteID());
  checkResults(route,true);
  s.getTransaction().commit();
  s.close();
}
