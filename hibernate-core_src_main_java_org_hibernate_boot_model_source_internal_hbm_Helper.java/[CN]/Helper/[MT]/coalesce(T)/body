{
  if (values == null) {
    return null;
  }
  for (  T value : values) {
    if (value != null) {
      if (String.class.isInstance(value)) {
        if (StringHelper.isNotEmpty((String)value)) {
          return value;
        }
      }
 else {
        return value;
      }
    }
  }
  return null;
}
