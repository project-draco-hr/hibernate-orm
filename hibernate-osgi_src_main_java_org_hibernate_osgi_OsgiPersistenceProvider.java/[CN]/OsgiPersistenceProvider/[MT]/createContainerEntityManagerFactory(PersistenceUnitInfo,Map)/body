{
  if (properties == null) {
    properties=new HashMap();
  }
  properties.put(AvailableSettings.JTA_PLATFORM,osgiJtaPlatform);
  properties.put(org.hibernate.jpa.AvailableSettings.SCANNER,new OsgiScanner(((BundleReference)info.getClassLoader()).getBundle()));
  osgiClassLoader.addClassLoader(info.getClassLoader());
  return super.createContainerEntityManagerFactory(info,properties);
}
