{
  try {
    Element element=new Element(key,value);
    cache.put(element);
  }
 catch (  IllegalArgumentException e) {
    throw new CacheException(e);
  }
catch (  IllegalStateException e) {
    throw new CacheException(e);
  }
catch (  net.sf.ehcache.CacheException e) {
    throw new CacheException(e);
  }
}
