{
  List list=getAuditReader().createQuery().forRevisionsOfEntity(ParentEntity.class,false,false).add(AuditEntity.id().eq(id1)).add(AuditEntity.property("data").hasChanged()).getResultList();
  assertEquals(2,list.size());
  assertEquals(TestTools.makeList(1,2),extractRevisionNumbers(list));
  list=getAuditReader().createQuery().forRevisionsOfEntity(ParentEntity.class,false,false).add(AuditEntity.id().eq(id1)).add(AuditEntity.property("data").hasNotChanged()).getResultList();
  assertEquals(0,list.size());
}
