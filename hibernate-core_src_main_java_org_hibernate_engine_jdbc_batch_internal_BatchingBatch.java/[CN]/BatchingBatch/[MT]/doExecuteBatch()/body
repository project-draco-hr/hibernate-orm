{
  if (maxBatchPosition == 0)   LOG.debug("No batched statements to execute");
 else {
    LOG.debug("Executing " + getStatements().size() + " statements with maximum batch size "+ maxBatchPosition);
    try {
      executeStatements();
    }
 catch (    RuntimeException re) {
      LOG.unableToExecuteBatch(re.getMessage());
      throw re;
    }
 finally {
      for (      List<Expectation> expectations : expectationsBySql.values()) {
        expectations.clear();
      }
      maxBatchPosition=0;
    }
  }
}
