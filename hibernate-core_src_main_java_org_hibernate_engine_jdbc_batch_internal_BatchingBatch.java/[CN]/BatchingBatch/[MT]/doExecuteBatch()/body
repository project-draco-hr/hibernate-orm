{
  if (maxBatchPosition == 0) {
    log.debug("no batched statements to execute");
  }
 else {
    if (log.isDebugEnabled()) {
      log.debug("Executing {} statements with maximum batch size {} ",getStatements().size(),maxBatchPosition);
    }
    try {
      executeStatements();
    }
 catch (    RuntimeException re) {
      log.error("Exception executing batch [{}]",re.getMessage());
      throw re;
    }
 finally {
      for (      List<Expectation> expectations : expectationsBySql.values()) {
        expectations.clear();
      }
      maxBatchPosition=0;
    }
  }
}
