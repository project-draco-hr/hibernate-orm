{
  Session s=openSession();
  s.getTransaction().begin();
  order1=new Order();
  OrderLine line=new OrderLine();
  line.setArticleId("1000");
  order1.addLine(line);
  line=new OrderLine();
  line.setArticleId("3000");
  order1.addLine(line);
  s.persist(order1);
  order2=new Order();
  s.persist(order2);
  order3=new Order();
  line=new OrderLine();
  line.setArticleId("3000");
  order3.addLine(line);
  s.persist(order3);
  s.getTransaction().commit();
  s.close();
}
