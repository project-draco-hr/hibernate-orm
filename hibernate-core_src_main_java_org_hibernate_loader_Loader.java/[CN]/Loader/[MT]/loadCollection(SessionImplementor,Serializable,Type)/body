{
  if (LOG.isDebugEnabled())   LOG.debugf("Loading collection: %s",MessageHelper.collectionInfoString(getCollectionPersisters()[0],id,getFactory()));
  Serializable[] ids=new Serializable[]{id};
  try {
    doQueryAndInitializeNonLazyCollections(session,new QueryParameters(new Type[]{type},ids,ids),true);
  }
 catch (  SQLException sqle) {
    throw factory.getSQLExceptionHelper().convert(sqle,"could not initialize a collection: " + MessageHelper.collectionInfoString(getCollectionPersisters()[0],id,getFactory()),getSQLString());
  }
  LOG.debugf("Done loading collection");
}
