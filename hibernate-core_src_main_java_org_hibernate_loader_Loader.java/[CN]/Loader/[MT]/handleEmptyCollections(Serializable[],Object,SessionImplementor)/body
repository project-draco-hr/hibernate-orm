{
  if (keys != null) {
    final boolean debugEnabled=LOG.isDebugEnabled();
    CollectionPersister[] collectionPersisters=getCollectionPersisters();
    for (int j=0; j < collectionPersisters.length; j++) {
      for (int i=0; i < keys.length; i++) {
        if (debugEnabled) {
          LOG.debugf("Result set contains (possibly empty) collection: %s",MessageHelper.collectionInfoString(collectionPersisters[j],keys[i],getFactory()));
        }
        session.getPersistenceContext().getLoadContexts().getCollectionLoadContext((ResultSet)resultSetId).getLoadingCollection(collectionPersisters[j],keys[i]);
      }
    }
  }
}
