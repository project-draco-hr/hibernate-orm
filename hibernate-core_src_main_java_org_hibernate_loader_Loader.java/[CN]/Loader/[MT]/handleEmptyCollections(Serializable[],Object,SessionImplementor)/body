{
  if (keys != null) {
    final boolean debugEnabled=LOG.isDebugEnabled();
    CollectionPersister[] collectionPersisters=getCollectionPersisters();
    for (    CollectionPersister collectionPersister : collectionPersisters)     for (int i=0; i < keys.length; i++) {
      if (debugEnabled) {
        LOG.debugf("Result set contains (possibly empty) collection: %s",MessageHelper.collectionInfoString(collectionPersister,keys[i],getFactory()));
      }
      session.getPersistenceContext().getLoadContexts().getCollectionLoadContext((ResultSet)resultSetId).getLoadingCollection(collectionPersister,keys[i]);
    }
  }
}
