{
  LOG.loadingCollectionElementByIndex();
  List result;
  try {
    result=doQueryAndInitializeNonLazyCollections(session,new QueryParameters(new Type[]{keyType,indexType},new Object[]{key,index}),false);
  }
 catch (  SQLException sqle) {
    throw factory.getSQLExceptionHelper().convert(sqle,"could not collection element by index",getSQLString());
  }
  LOG.loadingEntityDone();
  return result;
}
