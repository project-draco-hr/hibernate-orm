{
  return new ClassLoaderServiceImpl((ClassLoader)configVales.get(AvailableSettings.APP_CLASSLOADER),(ClassLoader)configVales.get(AvailableSettings.RESOURCES_CLASSLOADER),(ClassLoader)configVales.get(AvailableSettings.HIBERNATE_CLASSLOADER),(ClassLoader)configVales.get(AvailableSettings.ENVIRONMENT_CLASSLOADER));
}
