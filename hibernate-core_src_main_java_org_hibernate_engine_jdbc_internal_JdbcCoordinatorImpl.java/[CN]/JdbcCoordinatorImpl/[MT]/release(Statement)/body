{
  LOG.tracev("Releasing statement [{0}]",statement);
  Set<ResultSet> resultSets=xref.get(statement);
  if (resultSets != null) {
    for (    ResultSet resultSet : resultSets) {
      close(resultSet);
    }
    resultSets.clear();
  }
  xref.remove(statement);
  close(statement);
  afterStatementExecution();
}
