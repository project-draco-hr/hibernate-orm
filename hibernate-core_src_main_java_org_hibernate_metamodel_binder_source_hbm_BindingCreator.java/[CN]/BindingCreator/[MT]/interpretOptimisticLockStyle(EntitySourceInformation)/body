{
  final String optimisticLockModeString=Helper.getStringValue(((XMLHibernateMapping.XMLClass)entitySourceInfo.getEntityElement()).getOptimisticLock(),"version");
  try {
    return OptimisticLockStyle.valueOf(optimisticLockModeString.toUpperCase());
  }
 catch (  Exception e) {
    throw new MappingException("Unknown optimistic-lock value : " + optimisticLockModeString,entitySourceInfo.getSourceMappingDocument().getOrigin());
  }
}
