{
  for (  QueryHint hint : hints) {
    if (hintName.equals(hint.name())) {
      if (hint.value().equalsIgnoreCase("true")) {
        return true;
      }
 else       if (hint.value().equalsIgnoreCase("false")) {
        return false;
      }
 else {
        throw new AnnotationException("Not a boolean in hint: " + query + ":"+ hint.name());
      }
    }
  }
  return false;
}
