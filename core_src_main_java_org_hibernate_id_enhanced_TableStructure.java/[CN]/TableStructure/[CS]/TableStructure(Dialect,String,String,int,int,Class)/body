{
  this.tableName=tableName;
  this.initialValue=initialValue;
  this.incrementSize=incrementSize;
  this.valueColumnName=valueColumnName;
  this.numberType=numberType;
  selectQuery="select " + valueColumnName + " as id_val"+ " from "+ dialect.appendLockHint(LockMode.PESSIMISTIC_WRITE,tableName)+ dialect.getForUpdateString();
  updateQuery="update " + tableName + " set "+ valueColumnName+ "= ?"+ " where "+ valueColumnName+ "=?";
}
