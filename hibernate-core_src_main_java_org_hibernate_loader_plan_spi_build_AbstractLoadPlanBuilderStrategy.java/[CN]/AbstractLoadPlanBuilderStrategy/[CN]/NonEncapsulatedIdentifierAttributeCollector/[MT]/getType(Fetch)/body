{
  if (!fetch.getOwnerPropertyName().equals(entityReference.getEntityPersister().getIdentifierPropertyName())) {
    throw new IllegalArgumentException(String.format("Fetch owner property name [%s] is not the same as the identifier property name [%s].",fetch.getOwnerPropertyName(),entityReference.getEntityPersister().getIdentifierPropertyName()));
  }
  return fetchOwnerDelegate.getType(fetch);
}
