{
  super(entityReference);
  this.propertyPath=((FetchOwner)entityReference).getPropertyPath().append("<id>");
  this.fetchOwnerDelegate=new CompositeFetchOwnerDelegate(entityReference.getEntityPersister().getFactory(),(CompositeType)entityReference.getEntityPersister().getIdentifierType(),((Loadable)entityReference.getEntityPersister()).getIdentifierColumnNames());
}
