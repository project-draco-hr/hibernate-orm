{
  StringBuilder likeExpr=new StringBuilder();
  likeExpr.append(((ExpressionImplementor)getMatchExpression()).render(renderingContext)).append(" like ").append(((ExpressionImplementor)getPattern()).render(renderingContext));
  if (escapeCharacter != null) {
    likeExpr.append(" escape ").append(((ExpressionImplementor)getEscapeCharacter()).render(renderingContext));
  }
  return likeExpr.toString();
}
