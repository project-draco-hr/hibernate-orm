{
  StringBuilder likeExpr=new StringBuilder();
  likeExpr.append(((Renderable)getMatchExpression()).render(renderingContext)).append(" like ").append(((Renderable)getPattern()).render(renderingContext));
  if (escapeCharacter != null) {
    likeExpr.append(" escape ").append(((Renderable)getEscapeCharacter()).render(renderingContext));
  }
  return likeExpr.toString();
}
