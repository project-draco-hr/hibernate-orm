{
  final Properties properties=new Properties();
  final String cfgFileName=(String)Environment.getProperties().get(InfinispanRegionFactory.INFINISPAN_CONFIG_RESOURCE_PROP);
  if (cfgFileName != null) {
    properties.put(InfinispanRegionFactory.INFINISPAN_CONFIG_RESOURCE_PROP,cfgFileName);
  }
  return properties;
}
