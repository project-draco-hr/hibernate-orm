{
  StringBuilder whereCondition=whereString(resolveEntityReturnAlias(aliasResolutionContext),uniqueKey,batchSize).append(getPersister().filterFragment(resolveEntityReturnAlias(aliasResolutionContext),Collections.EMPTY_MAP));
  return generateSql(whereCondition.toString(),"",new LockOptions().setLockMode(lockMode),factory,aliasResolutionContext);
}
