{
  if (ast == null) {
    return;
  }
  Stack stack=new Stack();
  if (ast != null) {
    stack.push(ast);
    while (!stack.empty()) {
      ast=(AST)stack.pop();
      strategy.visit(ast);
      if (ast.getNextSibling() != null)       stack.push(ast.getNextSibling());
      if (ast.getFirstChild() != null)       stack.push(ast.getFirstChild());
    }
  }
}
