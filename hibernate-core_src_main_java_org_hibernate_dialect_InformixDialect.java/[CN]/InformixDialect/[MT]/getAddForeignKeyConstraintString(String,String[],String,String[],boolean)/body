{
  final StringBuilder result=new StringBuilder(30).append(" add constraint ").append(" foreign key (").append(StringHelper.join(", ",foreignKey)).append(") references ").append(referencedTable);
  if (!referencesPrimaryKey) {
    result.append(" (").append(StringHelper.join(", ",primaryKey)).append(')');
  }
  result.append(" constraint ").append(constraintName);
  return result.toString();
}
