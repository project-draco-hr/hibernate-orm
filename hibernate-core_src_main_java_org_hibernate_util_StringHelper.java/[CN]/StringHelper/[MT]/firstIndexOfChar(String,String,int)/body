{
  int matchAt=-1;
  for (int i=0; i < string.length(); i++) {
    int curMatch=sqlString.indexOf(string.charAt(i),startindex);
    if (curMatch >= 0) {
      if (matchAt == -1) {
        matchAt=curMatch;
      }
 else {
        matchAt=Math.min(matchAt,curMatch);
      }
    }
  }
  return matchAt;
}
