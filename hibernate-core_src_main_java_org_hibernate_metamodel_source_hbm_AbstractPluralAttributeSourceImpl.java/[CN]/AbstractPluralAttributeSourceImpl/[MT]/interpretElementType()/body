{
  if (pluralAttributeElement.getElement() != null) {
    return new BasicPluralAttributeElementSourceImpl(pluralAttributeElement.getElement(),container.getLocalBindingContext());
  }
 else   if (pluralAttributeElement.getCompositeElement() != null) {
    return new CompositePluralAttributeElementSourceImpl(pluralAttributeElement.getCompositeElement(),container.getLocalBindingContext());
  }
 else   if (pluralAttributeElement.getOneToMany() != null) {
    return new OneToManyPluralAttributeElementSourceImpl(pluralAttributeElement.getOneToMany(),container.getLocalBindingContext());
  }
 else   if (pluralAttributeElement.getManyToMany() != null) {
    return new ManyToManyPluralAttributeElementSourceImpl(pluralAttributeElement.getManyToMany(),container.getLocalBindingContext());
  }
 else   if (pluralAttributeElement.getManyToAny() != null) {
    throw new NotYetImplementedException("Support for many-to-any not yet implemented");
  }
 else {
    throw new MappingException("Unexpected collection element type : " + pluralAttributeElement.getName(),bindingContext().getOrigin());
  }
}
