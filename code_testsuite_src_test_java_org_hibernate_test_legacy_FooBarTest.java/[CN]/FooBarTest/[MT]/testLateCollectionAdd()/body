{
  Session s=openSession();
  Baz baz=new Baz();
  List l=new ArrayList();
  baz.setStringList(l);
  l.add("foo");
  Serializable id=s.save(baz);
  l.add("bar");
  s.flush();
  l.add("baz");
  s.flush();
  s.connection().commit();
  s.close();
  s=openSession();
  baz=(Baz)s.load(Baz.class,id);
  assertTrue(baz.getStringList().size() == 3 && baz.getStringList().contains("bar"));
  s.delete(baz);
  s.flush();
  s.connection().commit();
  s.close();
}
