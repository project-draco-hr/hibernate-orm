{
  Session s=openSession();
  Glarch g1=new Glarch();
  Glarch g2=new Glarch();
  g1.setNext(g2);
  g2.setNext(g1);
  s.save(g1);
  s.save(g2);
  s.flush();
  s.connection().commit();
  s.close();
  s=openSession();
  List l=s.find("from Glarch g where g.next is not null");
  s.delete(l.get(0));
  s.delete(l.get(1));
  s.flush();
  s.connection().commit();
  s.close();
}
