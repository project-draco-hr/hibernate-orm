{
  Session s=openSession();
  Baz baz=new Baz();
  s.save(baz);
  baz.setDefaults();
  s.flush();
  s.connection().commit();
  s.close();
  s=openSession();
  baz.getTimeArray()[2]=new Date(123);
  baz.getTimeArray()[3]=new java.sql.Time(1234);
  s.flush();
  s.connection().commit();
  s.close();
  s=openSession();
  baz=(Baz)s.load(Baz.class,baz.getCode());
  s.delete(baz);
  s.flush();
  s.connection().commit();
  s.close();
}
