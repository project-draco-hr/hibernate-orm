{
  Session s=openSession();
  Baz baz=new Baz();
  baz.setDefaults();
  s.save(baz);
  s.flush();
  s.connection().commit();
  s.close();
  s=openSession();
  s.load(Baz.class,baz.getCode());
  s.flush();
  s.connection().commit();
  s.close();
  s=openSession();
  baz=(Baz)s.load(Baz.class,baz.getCode());
  s.delete(baz);
  s.flush();
  s.connection().commit();
  s.close();
}
