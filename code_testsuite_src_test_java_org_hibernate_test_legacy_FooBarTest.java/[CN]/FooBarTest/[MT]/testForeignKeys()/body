{
  Session s=openSession();
  Baz baz=new Baz();
  Foo foo=new Foo();
  List bag=new ArrayList();
  bag.add(foo);
  baz.setIdFooBag(bag);
  baz.setFoo(foo);
  s.save(baz);
  s.flush();
  s.connection().commit();
  s.close();
  s=openSession();
  baz=(Baz)s.load(Baz.class,baz.getCode());
  s.delete(baz);
  s.flush();
  s.connection().commit();
  s.close();
}
