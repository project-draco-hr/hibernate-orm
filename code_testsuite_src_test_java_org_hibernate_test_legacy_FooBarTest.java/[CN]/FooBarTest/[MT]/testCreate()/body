{
  Session s=openSession();
  Foo foo=new Foo();
  s.save(foo);
  s.flush();
  s.connection().commit();
  s.close();
  s=openSession();
  Foo foo2=new Foo();
  s.load(foo2,foo.getKey());
  assertTrue("create",foo.equalsFoo(foo2));
  s.delete(foo2);
  s.flush();
  s.connection().commit();
  s.close();
}
