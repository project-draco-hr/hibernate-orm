{
  final boolean hasArgs=!args.isEmpty();
  StringBuilder buf=new StringBuilder();
  buf.append(getName());
  if (hasArgs) {
    buf.append("(");
    for (int i=0; i < args.size(); i++) {
      buf.append(args.get(i));
      if (i < args.size() - 1) {
        buf.append(", ");
      }
    }
    buf.append(")");
  }
  return buf.toString();
}
