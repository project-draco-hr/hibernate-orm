{
  try {
    JaxbRoot jaxbRoot=jaxbHelper.unmarshal(inputStream,origin);
    jaxbRootList.add(jaxbRoot);
    return jaxbRoot;
  }
  finally {
    if (close) {
      try {
        inputStream.close();
      }
 catch (      IOException ignore) {
        LOG.trace("Was unable to close input stream");
      }
    }
  }
}
