{
  final ServiceTracker serviceTracker=getServiceTracker(contract.getName());
  try {
    T[] services=(T[])serviceTracker.getServices();
    if (services != null) {
      return services;
    }
  }
 catch (  Exception e) {
    LOG.unableToDiscoverOsgiService(contract.getName(),e);
  }
  return (T[])Array.newInstance(contract,0);
}
