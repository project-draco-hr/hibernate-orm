{
  if (value != null) {
    if (value instanceof String) {
      String string=(String)value;
      if (isIgnoreCaseEnabled)       string=string.toLowerCase();
      if (isLikeEnabled)       string=matchMode.toMatchString(string);
      value=string;
    }
    list.add(new TypedValue(type,value,null));
  }
}
