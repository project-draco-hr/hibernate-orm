{
  for (  MappedAttribute mappedAttribute : entityClass.getMappedAttributes()) {
    if (mappedAttribute instanceof AssociationAttribute) {
      bindAssociationAttribute(entityBinding,entityBinding.getEntity(),(AssociationAttribute)mappedAttribute);
    }
 else {
      bindSingleMappedAttribute(entityBinding,entityBinding.getEntity(),(SimpleAttribute)mappedAttribute);
    }
  }
}
