{
  if (xmlClazz.getId() != null) {
    bindSimpleId(xmlClazz.getId(),entityBinding);
    return;
  }
  if (xmlClazz.getCompositeId() != null) {
    bindCompositeId(xmlClazz.getCompositeId(),entityBinding);
  }
  throw new InvalidMappingException("Entity [" + entityBinding.getEntity().getName() + "] did not contain identifier mapping",getHibernateMappingBinder().getOrigin());
}
