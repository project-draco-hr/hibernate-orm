{
  if (componentPathStack.isEmpty()) {
    return propertyName;
  }
 else {
    StringBuffer buffer=new StringBuffer();
    Iterator itr=componentPathStack.iterator();
    while (itr.hasNext()) {
      buffer.append(itr.next()).append('.');
    }
    buffer.append(propertyName);
    return buffer.toString();
  }
}
