{
  this.region=region;
  this.cache=region.getCache();
  this.putValidator=validator;
  this.writeCache=Caches.isInvalidationCache(cache) ? Caches.ignoreReturnValuesCache(cache,Flag.CACHE_MODE_LOCAL) : Caches.ignoreReturnValuesCache(cache);
  this.putFromLoadCache=Caches.ignoreReturnValuesCache(cache);
}
