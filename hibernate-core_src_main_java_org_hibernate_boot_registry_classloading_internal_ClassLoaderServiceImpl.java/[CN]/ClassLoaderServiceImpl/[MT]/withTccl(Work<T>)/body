{
  final ClassLoader tccl=Thread.currentThread().getContextClassLoader();
  boolean set=false;
  try {
    Thread.currentThread().setContextClassLoader(new TcclSafeAggregatedClassLoader(aggregatedClassLoader,tccl));
    set=true;
  }
 catch (  Exception ignore) {
  }
  try {
    return work.perform();
  }
  finally {
    if (set) {
      Thread.currentThread().setContextClassLoader(tccl);
    }
  }
}
