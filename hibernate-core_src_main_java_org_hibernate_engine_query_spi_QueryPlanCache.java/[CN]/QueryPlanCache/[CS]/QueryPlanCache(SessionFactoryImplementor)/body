{
  int maxStrongReferenceCount=ConfigurationHelper.getInt(Environment.QUERY_PLAN_CACHE_MAX_STRONG_REFERENCES,factory.getProperties(),SoftLimitMRUCache.DEFAULT_STRONG_REF_COUNT);
  int maxSoftReferenceCount=ConfigurationHelper.getInt(Environment.QUERY_PLAN_CACHE_MAX_SOFT_REFERENCES,factory.getProperties(),SoftLimitMRUCache.DEFAULT_SOFT_REF_COUNT);
  this.factory=factory;
  this.sqlParamMetadataCache=new SimpleMRUCache(maxStrongReferenceCount);
  this.planCache=new SoftLimitMRUCache(maxStrongReferenceCount,maxSoftReferenceCount);
}
