{
  Coordinate[] coords=new Coordinate[]{new Coordinate(0,5),new Coordinate(3,4),new Coordinate(5,0),new Coordinate(3,-4),new Coordinate(0,-5),new Coordinate(-3,-4),new Coordinate(-5,0),new Coordinate(-3,4)};
  for (int i=0; i < coords.length; i++) {
    Coordinate p1=coords[i];
    Coordinate p2=coords[(i + 1) % coords.length];
    Coordinate p3=coords[(i + 2) % coords.length];
    Circle c=new Circle(p1,p2,p3);
    Circle.Arc a=c.createArc(p1,p2,p3);
    assertTrue("Failed Points:" + p1 + ", "+ p2+ ", "+ p3,a.isClockwise());
  }
  for (int i=0; i < coords.length; i++) {
    Coordinate p3=coords[i];
    Coordinate p2=coords[(i + 1) % coords.length];
    Coordinate p1=coords[(i + 2) % coords.length];
    Circle c=new Circle(p1,p2,p3);
    Circle.Arc a=c.createArc(p1,p2,p3);
    assertFalse("Failed Points:" + p1 + ", "+ p2+ ", "+ p3,a.isClockwise());
  }
}
