{
  EntityManager em=getOrCreateEntityManager();
  Music music=em.getReference(Music.class,new Integer(-1));
  try {
    music.getName();
    fail("Non existent entity should raise an exception when state is accessed");
  }
 catch (  EntityNotFoundException e) {
    log.debug("success");
  }
 finally {
    em.close();
  }
}
