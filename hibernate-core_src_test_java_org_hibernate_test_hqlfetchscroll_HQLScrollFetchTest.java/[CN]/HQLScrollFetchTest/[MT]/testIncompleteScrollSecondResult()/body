{
  try {
    insertTestData();
    Session s=openSession();
    ScrollableResults results=s.createQuery(QUERY + " order by p.name asc").scroll();
    results.next();
    Parent p=(Parent)results.get(0);
    assertResultFromOneUser(p);
    results.next();
    p=(Parent)results.get(0);
    assertResultFromOneUser(p);
    s.close();
  }
  finally {
    deleteAll();
  }
}
