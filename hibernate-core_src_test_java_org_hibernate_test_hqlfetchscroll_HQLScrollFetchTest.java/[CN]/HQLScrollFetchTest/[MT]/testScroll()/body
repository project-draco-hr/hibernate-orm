{
  try {
    insertTestData();
    Session s=openSession();
    ScrollableResults results=s.createQuery(QUERY).scroll();
    List list=new ArrayList();
    while (results.next()) {
      list.add(results.get(0));
    }
    assertResultFromAllUsers(list);
    s.close();
  }
  finally {
    deleteAll();
  }
}
