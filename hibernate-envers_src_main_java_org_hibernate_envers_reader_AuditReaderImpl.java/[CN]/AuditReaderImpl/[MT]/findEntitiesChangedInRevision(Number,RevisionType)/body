{
  Set<Class> clazz=findEntityTypesChangedInRevision(revision);
  List<Object> result=new ArrayList<Object>();
  for (  Class c : clazz) {
    result.addAll(createQuery().forEntitiesModifiedAtRevision(c,revision).add(new RevisionTypeAuditExpression(revisionType,"=")).getResultList());
  }
  return result;
}
