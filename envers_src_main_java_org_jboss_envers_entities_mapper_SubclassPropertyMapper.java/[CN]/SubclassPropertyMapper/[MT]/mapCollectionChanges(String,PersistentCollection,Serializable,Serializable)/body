{
  List<PersistentCollectionChangeData> collectionChanges=parentMapper.mapCollectionChanges(referencingPropertyName,newColl,oldColl,id);
  if (collectionChanges == null) {
    return main.mapCollectionChanges(referencingPropertyName,newColl,oldColl,id);
  }
 else {
    return collectionChanges;
  }
}
