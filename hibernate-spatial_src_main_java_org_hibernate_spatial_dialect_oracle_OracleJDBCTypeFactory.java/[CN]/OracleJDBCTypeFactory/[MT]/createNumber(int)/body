{
  try {
    return numberConstructor.newInstance(obj);
  }
 catch (  InvocationTargetException e) {
    throw new HibernateException("Error creating oracle NUMBER",e);
  }
catch (  InstantiationException e) {
    throw new HibernateException("Error creating oracle NUMBER",e);
  }
catch (  IllegalAccessException e) {
    throw new HibernateException("Error creating oracle NUMBER",e);
  }
}
