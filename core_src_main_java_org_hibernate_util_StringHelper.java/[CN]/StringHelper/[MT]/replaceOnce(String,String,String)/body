{
  if (template == null) {
    return template;
  }
  int loc=template == null ? -1 : template.indexOf(placeholder);
  if (loc < 0) {
    return template;
  }
 else {
    return new StringBuffer(template.substring(0,loc)).append(replacement).append(template.substring(loc + placeholder.length())).toString();
  }
}
