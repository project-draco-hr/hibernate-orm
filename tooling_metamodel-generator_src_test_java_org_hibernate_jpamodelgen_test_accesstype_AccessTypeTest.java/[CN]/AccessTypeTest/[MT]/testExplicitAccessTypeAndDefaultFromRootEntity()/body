{
  assertAbsenceOfField(LivingBeing.class.getName() + "_","nonPersistent","explicit access type on mapped superclass");
  assertAbsenceOfField(Hominidae.class.getName() + "_","nonPersistent","explicit access type on entity");
  assertAbsenceOfField(Human.class.getName() + "_","nonPersistent","proper inheritance from root entity access type");
}
