{
  EntityMode entityMode=getSession().getEntityMode();
  HashMap map=new HashMap(values.size());
  Iterator iter=values.iterator();
  int i=0;
  while (iter.hasNext()) {
    Object value=iter.next();
    map.put(identifiers.get(new Integer(i++)),persister.getElementType().deepCopy(value,entityMode,persister.getFactory()));
  }
  return map;
}
