{
  String existing=imports.put(rename,entityName);
  if (existing != null) {
    if (existing.equals(entityName))     LOG.duplicateImport(entityName,rename);
 else     throw new DuplicateMappingException("duplicate import: " + rename + " refers to both "+ entityName+ " and "+ existing+ " (try using auto-import=\"false\")","import",rename);
  }
}
