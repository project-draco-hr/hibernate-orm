{
  ClassCache cache=classCache.get(clazz);
  if (cache == null) {
    cache=new ClassCache(clazz);
    classCache.put(clazz,cache);
  }
  Member member=cache.members.get(property);
  if (member == null) {
    member=findMember(clazz,property);
    cache.members.put(property,member);
  }
  return member;
}
