{
  if (testCount == 0) {
    return;
  }
  try {
    log.info("Starting test-suite [" + getName() + "]");
    setUp();
    testPosition=0;
    super.run(testResult);
  }
  finally {
    try {
      tearDown();
    }
 catch (    Throwable ignore) {
    }
    log.info("Completed test-suite [" + getName() + "]");
  }
}
