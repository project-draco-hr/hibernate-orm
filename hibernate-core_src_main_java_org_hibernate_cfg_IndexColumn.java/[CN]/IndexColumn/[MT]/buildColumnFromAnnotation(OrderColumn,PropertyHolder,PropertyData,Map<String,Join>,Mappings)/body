{
  final IndexColumn column;
  if (ann != null) {
    final String sqlType=BinderHelper.isEmptyAnnotationValue(ann.columnDefinition()) ? null : ann.columnDefinition();
    final String name=BinderHelper.isEmptyAnnotationValue(ann.name()) ? inferredData.getPropertyName() + "_ORDER" : ann.name();
    column=new IndexColumn(false,sqlType,0,0,0,name,ann.nullable(),false,ann.insertable(),ann.updatable(),null,secondaryTables,propertyHolder,mappings);
  }
 else {
    column=new IndexColumn(true,null,0,0,0,null,true,false,true,true,null,null,propertyHolder,mappings);
  }
  return column;
}
