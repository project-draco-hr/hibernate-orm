{
  releaseBorrowedConnection();
  if (connection == null) {
    LOG.connectionAlreadyNullInCleanup();
    return null;
  }
  try {
    LOG.performingCleanup();
    batcher.closeStatements();
    Connection c=null;
    if (!wasConnectionSupplied) {
      closeConnection();
    }
 else {
      c=connection;
    }
    connection=null;
    return c;
  }
  finally {
    callback.connectionCleanedUp();
  }
}
