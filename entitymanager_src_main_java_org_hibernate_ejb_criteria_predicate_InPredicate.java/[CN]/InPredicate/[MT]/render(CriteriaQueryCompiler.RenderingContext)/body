{
  StringBuilder buffer=new StringBuilder("in");
  boolean isInSubqueryPredicate=getValues().size() == 1 && Subquery.class.isInstance(getValues().get(0));
  if (isInSubqueryPredicate) {
    buffer.append(((ExpressionImplementor)getValues().get(0)).render(renderingContext));
  }
 else {
    buffer.append('(');
    String sep="";
    for (    Expression value : getValues()) {
      buffer.append(sep).append(((ExpressionImplementor)value).render(renderingContext));
      sep=", ";
    }
    buffer.append(')');
  }
  return buffer.toString();
}
