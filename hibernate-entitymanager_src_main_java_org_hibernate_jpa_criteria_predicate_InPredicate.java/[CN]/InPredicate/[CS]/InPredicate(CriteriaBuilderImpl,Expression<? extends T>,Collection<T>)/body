{
  super(criteriaBuilder);
  this.expression=expression;
  this.values=new ArrayList<Expression<? extends T>>(values.size());
  ValueHandlerFactory.ValueHandler<? extends T> valueHandler=ValueHandlerFactory.isNumeric(expression.getJavaType()) ? ValueHandlerFactory.determineAppropriateHandler((Class<? extends T>)expression.getJavaType()) : new ValueHandlerFactory.NoOpValueHandler<T>();
  for (  T value : values) {
    this.values.add(new LiteralExpression<T>(criteriaBuilder,valueHandler.convert(value)));
  }
}
