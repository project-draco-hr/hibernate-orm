{
  Iterator<ExtendsQueueEntry> itr=extendsQueue.keySet().iterator();
  while (itr.hasNext()) {
    final ExtendsQueueEntry entry=itr.next();
    boolean found=getClassMapping(entry.getExplicitName()) != null || getClassMapping(HbmBinder.getClassName(entry.getExplicitName(),entry.getMappingPackage())) != null;
    if (found) {
      itr.remove();
      return entry;
    }
  }
  return null;
}
