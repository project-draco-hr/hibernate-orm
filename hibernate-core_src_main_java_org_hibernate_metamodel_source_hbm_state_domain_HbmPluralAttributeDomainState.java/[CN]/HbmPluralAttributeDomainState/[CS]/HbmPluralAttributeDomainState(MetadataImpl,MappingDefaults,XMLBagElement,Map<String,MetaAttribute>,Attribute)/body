{
  super(metadata,mappingDefaults,attribute,collection.getNode(),HbmHelper.extractMetas(collection.getMeta(),entityMetaAttributes),HbmHelper.getPropertyAccessorName(collection.getAccess(),collection.isEmbedXml(),mappingDefaults.getDefaultAccess()),collection.isOptimisticLock());
  this.collection=collection;
  this.hibernateTypeDescriptor.setTypeName(collection.getCollectionType());
  this.cascade=MappingHelper.getStringValue(collection.getCascade(),mappingDefaults.getDefaultCascade());
  this.hibernateTypeDescriptor.setTypeName(collection.getCollectionType());
}
