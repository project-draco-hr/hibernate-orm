{
  if (batchSize == 0)   LOG.noBatchedStatementsToExecute();
 else {
    LOG.executingBatchSize(batchSize);
    try {
      checkRowCounts(ps.executeBatch(),ps);
    }
 catch (    RuntimeException re) {
      LOG.error(LOG.unableToExecuteBatch(),re);
      throw re;
    }
 finally {
      batchSize=0;
    }
  }
}
