{
  final Map<String,Object> data=new HashMap<String,Object>();
  fillDataWithId(data,revisionData);
  if (verCfg.getGlobalCfg().isStoreDataAtDelete()) {
    verCfg.getEntCfg().get(getEntityName()).getPropertyMapper().map(sessionImplementor,data,propertyNames,state,state);
  }
 else {
    verCfg.getEntCfg().get(getEntityName()).getPropertyMapper().map(sessionImplementor,data,propertyNames,null,state);
  }
  return data;
}
