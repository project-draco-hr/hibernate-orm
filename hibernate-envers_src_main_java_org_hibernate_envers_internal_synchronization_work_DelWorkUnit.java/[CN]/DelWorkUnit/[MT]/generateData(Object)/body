{
  final Map<String,Object> data=new HashMap<String,Object>();
  fillDataWithId(data,revisionData);
  if (enversService.getGlobalConfiguration().isStoreDataAtDelete()) {
    enversService.getEntitiesConfigurations().get(getEntityName()).getPropertyMapper().map(sessionImplementor,data,propertyNames,state,state);
  }
 else {
    enversService.getEntitiesConfigurations().get(getEntityName()).getPropertyMapper().map(sessionImplementor,data,propertyNames,null,state);
  }
  return data;
}
