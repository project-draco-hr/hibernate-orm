{
  Session s=openSession();
  Transaction t=s.beginTransaction();
  Parent p=new Parent("Marc");
  Parent p2=new Parent("Nathalie");
  s.persist(p);
  s.persist(p2);
  Child c=new Child("Elvira");
  Child c2=new Child("Blase");
  p.getChildren().add(c);
  c.setParent(p);
  p.getChildren().add(c2);
  c2.setParent(p);
  t.commit();
  s.close();
}
