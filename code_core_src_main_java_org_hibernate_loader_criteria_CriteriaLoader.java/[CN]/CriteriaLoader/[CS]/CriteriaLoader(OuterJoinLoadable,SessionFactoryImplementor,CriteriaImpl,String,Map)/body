{
  super(factory,enabledFilters);
  translator=new CriteriaQueryTranslator(factory,criteria,rootEntityName,CriteriaQueryTranslator.ROOT_SQL_ALIAS);
  querySpaces=translator.getQuerySpaces();
  CriteriaJoinWalker walker=new CriteriaJoinWalker(persister,translator,factory,criteria,rootEntityName,enabledFilters);
  initFromWalker(walker);
  userAliases=walker.getUserAliases();
  resultTypes=walker.getResultTypes();
  postInstantiate();
}
