{
  if (!hasSubclasses()) {
    return this;
  }
 else {
    final String concreteEntityName=getTuplizer(entityMode).determineConcreteSubclassEntityName(instance,factory);
    if (concreteEntityName == null || getEntityName().equals(concreteEntityName)) {
      return this;
    }
 else {
      return factory.getEntityPersister(concreteEntityName);
    }
  }
}
