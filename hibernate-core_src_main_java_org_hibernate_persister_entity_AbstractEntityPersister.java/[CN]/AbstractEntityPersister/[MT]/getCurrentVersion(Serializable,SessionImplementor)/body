{
  if (LOG.isTraceEnabled()) {
    LOG.tracev("Getting version: {0}",MessageHelper.infoString(this,id,getFactory()));
  }
  try {
    PreparedStatement st=session.getTransactionCoordinator().getJdbcCoordinator().getStatementPreparer().prepareStatement(getVersionSelectString());
    try {
      getIdentifierType().nullSafeSet(st,id,1,session);
      ResultSet rs=session.getTransactionCoordinator().getJdbcCoordinator().getResultSetReturn().extract(st);
      try {
        if (!rs.next()) {
          return null;
        }
        if (!isVersioned()) {
          return this;
        }
        return getVersionType().nullSafeGet(rs,getVersionColumnName(),session,null);
      }
  finally {
        session.getTransactionCoordinator().getJdbcCoordinator().release(rs);
      }
    }
  finally {
      session.getTransactionCoordinator().getJdbcCoordinator().release(st);
    }
  }
 catch (  SQLException e) {
    throw getFactory().getSQLExceptionHelper().convert(e,"could not retrieve version: " + MessageHelper.infoString(this,id,getFactory()),getVersionSelectString());
  }
}
