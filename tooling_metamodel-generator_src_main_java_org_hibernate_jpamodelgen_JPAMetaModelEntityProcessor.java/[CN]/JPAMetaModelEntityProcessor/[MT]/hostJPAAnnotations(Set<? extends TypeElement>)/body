{
  for (  TypeElement type : annotations) {
    final String typeName=type.getQualifiedName().toString();
    if (typeName.equals(ENTITY_ANN)) {
      return true;
    }
 else     if (typeName.equals(EMBEDDABLE_ANN)) {
      return true;
    }
 else     if (typeName.equals(MAPPED_SUPERCLASS_ANN)) {
      return true;
    }
  }
  return false;
}
