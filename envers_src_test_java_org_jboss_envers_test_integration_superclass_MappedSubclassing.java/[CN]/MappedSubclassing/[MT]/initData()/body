{
  EntityManager em=getEntityManager();
  em.getTransaction().begin();
  SubclassEntity se1=new SubclassEntity("x");
  em.persist(se1);
  id1=se1.getId();
  em.getTransaction().commit();
  em.getTransaction().begin();
  se1=em.find(SubclassEntity.class,id1);
  se1.setStr("y");
  em.getTransaction().commit();
}
