{
  URL jarUrl;
  try {
    jarUrl=new URL(jarPath);
  }
 catch (  MalformedURLException e) {
    try {
      jarUrl=new URL("file:" + jarPath);
    }
 catch (    MalformedURLException ee) {
      throw new IllegalArgumentException("Unable to find jar:" + jarPath,ee);
    }
  }
  this.jarUrl=jarUrl;
  unqualify();
}
