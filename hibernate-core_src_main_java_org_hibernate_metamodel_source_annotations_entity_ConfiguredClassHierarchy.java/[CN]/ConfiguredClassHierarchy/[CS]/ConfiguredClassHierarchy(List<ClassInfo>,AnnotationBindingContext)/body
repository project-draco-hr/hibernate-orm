{
  defaultAccessType=determineDefaultAccessType(classes);
  inheritanceType=determineInheritanceType(classes);
  ResolvedTypeWithMembers resolvedType=context.resolveType(classes.get(classes.size() - 1).name().toString());
  entityClasses=new ArrayList<EntityClass>();
  EntityClass parent=null;
  for (  ClassInfo info : classes) {
    EntityClass entityClass=new EntityClass(info,parent,defaultAccessType,inheritanceType,resolvedType,context);
    entityClasses.add(entityClass);
    parent=entityClass;
  }
}
