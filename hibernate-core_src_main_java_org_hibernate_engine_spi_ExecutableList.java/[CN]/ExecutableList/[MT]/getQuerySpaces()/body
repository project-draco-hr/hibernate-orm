{
  if (querySpaces == null) {
    querySpaces=new HashSet<Serializable>();
    for (    E e : executables) {
      Serializable[] propertySpaces=e.getPropertySpaces();
      if (querySpaces != null && propertySpaces != null) {
        Collections.addAll(querySpaces,propertySpaces);
      }
    }
  }
  return querySpaces;
}
