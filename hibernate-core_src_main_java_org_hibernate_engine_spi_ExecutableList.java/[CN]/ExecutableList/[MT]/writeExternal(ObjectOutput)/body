{
  oos.writeBoolean(sorted);
  oos.writeInt(executables.size());
  for (  E e : executables) {
    oos.writeObject(e);
  }
  if (querySpaces == null) {
    oos.writeInt(-1);
  }
 else {
    oos.writeInt(querySpaces.size());
    for (    Serializable querySpace : querySpaces) {
      oos.writeUTF(querySpace.toString());
    }
  }
}
