{
  if (spaces == null) {
    spaces=new HashSet<Serializable>();
    for (    E e : executables) {
      Serializable[] propertySpaces=e.getPropertySpaces();
      if (spaces != null && propertySpaces != null) {
        for (        Serializable s : propertySpaces) {
          spaces.add(s);
        }
      }
    }
  }
  return spaces;
}
