{
  Iterator<ConfiguredClass> iter;
  StringBuilder builder=new StringBuilder();
  builder.append("[");
  iter=iterator();
  while (iter.hasNext()) {
    builder.append(iter.next().getClassInfo().name().toString());
    if (iter.hasNext()) {
      builder.append(", ");
    }
  }
  builder.append("]");
  return builder.toString();
}
