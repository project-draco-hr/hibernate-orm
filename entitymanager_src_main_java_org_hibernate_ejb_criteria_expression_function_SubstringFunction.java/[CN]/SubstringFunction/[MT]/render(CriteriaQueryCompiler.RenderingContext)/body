{
  StringBuilder buffer=new StringBuilder();
  buffer.append("substring(").append(((ExpressionImplementor)getValue()).render(renderingContext)).append(',').append(((ExpressionImplementor)getStart()).render(renderingContext));
  if (getLength() != null) {
    buffer.append(',').append(((ExpressionImplementor)getLength()).render(renderingContext));
  }
  buffer.append(')');
  return buffer.toString();
}
