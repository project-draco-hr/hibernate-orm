{
  originalClassLoader=Thread.currentThread().getContextClassLoader();
  ClassLoader parent=originalClassLoader == null ? IsolatedClassLoaderTest.class.getClassLoader() : originalClassLoader;
  ClassLoader selectedTCCL=new SelectedClassnameClassLoader(new String[]{org.hibernate.test.cache.infinispan.functional.classloader.Account.class.getName(),org.hibernate.test.cache.infinispan.functional.classloader.AccountHolder.class.getName()},null,null,parent);
  Thread.currentThread().setContextClassLoader(selectedTCCL);
}
