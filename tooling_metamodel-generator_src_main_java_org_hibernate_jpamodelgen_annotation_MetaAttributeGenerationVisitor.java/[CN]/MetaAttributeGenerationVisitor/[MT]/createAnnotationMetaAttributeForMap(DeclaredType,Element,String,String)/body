{
  String keyType;
  if (TypeUtils.containsAnnotation(element,MapKeyClass.class)) {
    TypeMirror typeMirror=(TypeMirror)TypeUtils.getAnnotationValue(TypeUtils.getAnnotationMirror(element,MapKeyClass.class),TypeUtils.DEFAULT_ANNOTATION_PARAMETER_NAME);
    keyType=typeMirror.toString();
  }
 else {
    keyType=TypeUtils.getKeyType(declaredType,context);
  }
  return new AnnotationMetaMap(entity,element,collection,keyType,getElementType(declaredType,targetEntity));
}
