{
  this.hbmHandler=hbmHandler;
  this.jaxbRoot=jaxbRoot;
  this.hibernateMapping=jaxbRoot.getRoot();
  this.mappingDefaults=new OverriddenMappingDefaults(hbmHandler.getMappingDefaults(),hibernateMapping.getPackage(),hibernateMapping.getSchema(),hibernateMapping.getCatalog(),null,null,hibernateMapping.getDefaultCascade(),hibernateMapping.getDefaultAccess(),hibernateMapping.isDefaultLazy());
  this.autoImport=hibernateMapping.isAutoImport();
  this.entityBinder=new EntityBinder(this);
  this.metaAttributeContext=extractMetaAttributes();
}
