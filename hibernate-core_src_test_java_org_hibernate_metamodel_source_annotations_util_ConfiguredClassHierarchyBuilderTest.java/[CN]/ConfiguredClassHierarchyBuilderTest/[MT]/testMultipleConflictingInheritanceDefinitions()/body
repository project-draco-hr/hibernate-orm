{
@Entity @Inheritance(strategy=InheritanceType.JOINED) class A {
    String id;
  }
@Entity @Inheritance(strategy=InheritanceType.TABLE_PER_CLASS) class B extends A {
  }
  Index index=JandexHelper.indexForClass(service,B.class,A.class);
  ConfiguredClassHierarchyBuilder.createEntityHierarchies(index,serviceRegistry);
}
