{
  if (counter == 0)   assertEquals("foo.street1",attributeName);
  if (counter == 1)   assertEquals("foo.city",attributeName);
  if (counter == 2)   assertEquals("bar.city",attributeName);
  if (counter == 3)   assertEquals("bar.country",attributeName);
  if (counter == 4)   assertEquals("moo.country",attributeName);
  counter++;
}
