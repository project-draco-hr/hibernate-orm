{
  if (key == null) {
    throw new NullPointerException("Key to get cannot be null");
  }
  clearObsoleteReferences();
  SoftReference ref=(SoftReference)softRefCache.get(key);
  if (ref != null) {
    Object refValue=ref.get();
    if (refValue != null) {
      strongRefCache.put(key,refValue);
      return refValue;
    }
  }
  return null;
}
