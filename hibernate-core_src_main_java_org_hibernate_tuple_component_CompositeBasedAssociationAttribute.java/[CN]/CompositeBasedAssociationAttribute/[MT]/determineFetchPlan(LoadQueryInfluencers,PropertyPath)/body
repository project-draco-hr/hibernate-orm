{
  final EntityPersister owningPersister=locateOwningPersister();
  FetchStyle style=determineFetchStyleByProfile(loadQueryInfluencers,owningPersister,propertyPath,ownerAttributeNumber());
  if (style == null) {
    style=determineFetchStyleByMetadata(getSource().getType().getFetchMode(attributeNumber()),getType());
  }
  return new FetchStrategy(determineFetchTiming(style),style);
}
