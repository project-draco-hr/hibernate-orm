{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  final Circle circle=(Circle)o;
  if (Double.compare(circle.radius,this.radius) != 0) {
    return false;
  }
  if (this.center != null ? !this.center.equals2D(circle.center) : circle.center != null) {
    return false;
  }
  return true;
}
