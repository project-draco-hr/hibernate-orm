{
  Object result=null;
  try {
    final QueryParameters qp=new QueryParameters();
    qp.setPositionalParameterTypes(new Type[]{entityPersister.getIdentifierType()});
    qp.setPositionalParameterValues(new Object[]{id});
    qp.setOptionalObject(optionalObject);
    qp.setOptionalEntityName(entityPersister.getEntityName());
    qp.setOptionalId(id);
    qp.setLockOptions(lockOptions);
    final List results=executeLoad(session,qp,staticSql,staticResultSetProcessor,staticAliasResolutionContext,false,null);
    result=extractEntityResult(results);
  }
 catch (  SQLException sqle) {
    throw factory.getSQLExceptionHelper().convert(sqle,"could not load an entity: " + MessageHelper.infoString(entityPersister,id,entityPersister.getIdentifierType(),factory),getSqlStatement());
  }
  log.debugf("Done entity load : %s#%s",getEntityName(),id);
  return result;
}
