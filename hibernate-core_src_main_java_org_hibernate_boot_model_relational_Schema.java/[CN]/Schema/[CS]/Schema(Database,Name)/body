{
  this.database=database;
  this.name=name;
  final Identifier physicalCatalogIdentifier=database.getPhysicalNamingStrategy().toPhysicalCatalogName(name.getCatalog(),database.getJdbcEnvironment());
  final Identifier physicalSchemaIdentifier=database.getPhysicalNamingStrategy().toPhysicalCatalogName(name.getSchema(),database.getJdbcEnvironment());
  this.physicalName=new Name(physicalCatalogIdentifier,physicalSchemaIdentifier);
}
