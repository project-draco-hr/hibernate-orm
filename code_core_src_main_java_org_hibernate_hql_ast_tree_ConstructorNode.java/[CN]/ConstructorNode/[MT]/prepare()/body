{
  constructorArgumentTypes=resolveConstructorArgumentTypes();
  String path=((PathNode)getFirstChild()).getPath();
  if ("map".equals(path.toLowerCase())) {
    isMap=true;
  }
 else   if ("list".equals(path.toLowerCase())) {
    isList=true;
  }
 else {
    constructor=resolveConstructor(path);
  }
}
