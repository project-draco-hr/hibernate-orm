{
  Map map=new HashMap();
  String propValue=properties.getProperty(property);
  if (propValue != null) {
    StringTokenizer tokens=new StringTokenizer(propValue,delim);
    while (tokens.hasMoreTokens()) {
      map.put(tokens.nextToken(),tokens.hasMoreElements() ? tokens.nextToken() : "");
    }
  }
  return map;
}
