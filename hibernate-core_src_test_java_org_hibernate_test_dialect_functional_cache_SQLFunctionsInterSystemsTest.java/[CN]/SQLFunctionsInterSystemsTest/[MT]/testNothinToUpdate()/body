{
  Session s=openSession();
  Transaction t=s.beginTransaction();
  Simple simple=new Simple();
  simple.setName("Simple 1");
  s.save(simple,Long.valueOf(10));
  t.commit();
  s.close();
  s=openSession();
  t=s.beginTransaction();
  s.update(simple,Long.valueOf(10));
  t.commit();
  s.close();
  s=openSession();
  t=s.beginTransaction();
  s.update(simple,Long.valueOf(10));
  s.delete(simple);
  t.commit();
  s.close();
}
