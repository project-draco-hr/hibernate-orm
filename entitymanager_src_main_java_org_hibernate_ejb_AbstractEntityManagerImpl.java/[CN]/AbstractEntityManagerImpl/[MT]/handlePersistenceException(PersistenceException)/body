{
  if (e instanceof NoResultException) {
    return;
  }
  if (e instanceof NonUniqueResultException) {
    return;
  }
  if (e instanceof LockTimeoutException) {
    return;
  }
  try {
    markAsRollback();
  }
 catch (  Exception ne) {
    log.error("Unable to mark for rollback on PersistenceException: ",ne);
  }
}
