{
  if (Session.class.isAssignableFrom(clazz)) {
    return (T)getSession();
  }
  if (SessionImplementor.class.isAssignableFrom(clazz)) {
    return (T)getSession();
  }
  if (EntityManager.class.isAssignableFrom(clazz)) {
    return (T)this;
  }
  throw new PersistenceException("Hibernate cannot unwrap " + clazz);
}
