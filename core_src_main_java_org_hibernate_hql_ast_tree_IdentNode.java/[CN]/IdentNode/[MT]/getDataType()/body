{
  Type type=super.getDataType();
  if (type != null) {
    return type;
  }
  FromElement fe=getFromElement();
  if (fe != null) {
    return fe.getDataType();
  }
  SQLFunction sf=getWalker().getSessionFactoryHelper().findSQLFunction(getText());
  return sf == null ? null : sf.getReturnType(null,null);
}
